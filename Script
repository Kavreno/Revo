    -- Explode

        -- Explode Button

            Revo.Buttons.Explode = Instance.new("TextButton")
            Revo.Buttons.Explode.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.Explode.Position = UDim2.new(0, 5, 0, 145)
            Revo.Buttons.Explode.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.Explode.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.Explode.Font = Enum.Font.Gotham
            Revo.Buttons.Explode.TextSize = 16
            Revo.Buttons.Explode.Text = "Explode"
            Revo.Buttons.Explode.Parent = Revo.GUI.MiscSection
            Revo.GUI:AddCorner(Revo.Buttons.Explode)

        -- Explode Function

            Revo.Buttons.Explode.MouseButton1Click:Connect(function()
                local char = Revo.Player.Character
                if not char then return end

                local hum = char:FindFirstChildOfClass("Humanoid")
                local root = Revo:GetRoot(Revo.Player)
                if not hum or not root then return end

                Revo:ToggleRagdoll(false)
                task.wait()
                hum:ChangeState(Enum.HumanoidStateType.Physics)

                local oldSpin = root:FindFirstChild("Spin")
                if oldSpin then oldSpin:Destroy() end

                local spin = Instance.new("BodyAngularVelocity")
                spin.Name = "Spin"
                spin.MaxTorque = Vector3.new(0, math.huge, 0)
                spin.AngularVelocity = Vector3.new(0, 150, 0)
                spin.Parent = root

                task.wait(3)
                spin:Destroy()
                hum:ChangeState(Enum.HumanoidStateType.GettingUp)
            end)


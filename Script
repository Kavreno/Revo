        Revo.GUI.TargetButtons.Push         = Revo.GUI:CreateTargetButton("Push",         "Push",                  UDim2.new(0, 5, 0, 295))
        Revo.GUI.TargetButtons.FaceBang     = Revo.GUI:CreateTargetButton("FaceBang",     "Face Bang",             UDim2.new(0, 215, 0, 295))

        Revo.GUI.TargetButtons.Fling.Parent    = Revo.GUI.TargetLeftColumn
        Revo.GUI.TargetButtons.Focus.Parent    = Revo.GUI.TargetLeftColumn
        Revo.GUI.TargetButtons.Stand.Parent    = Revo.GUI.TargetLeftColumn
        Revo.GUI.TargetButtons.Doggy.Parent    = Revo.GUI.TargetLeftColumn
        Revo.GUI.TargetButtons.Drag.Parent     = Revo.GUI.TargetLeftColumn
        Revo.GUI.TargetButtons.Push.Parent     = Revo.GUI.TargetLeftColumn

        Revo.GUI.TargetButtons.View.Parent     = Revo.GUI.TargetRightColumn
        Revo.GUI.TargetButtons.Headsit.Parent  = Revo.GUI.TargetRightColumn
        Revo.GUI.TargetButtons.Backpack.Parent = Revo.GUI.TargetRightColumn
        Revo.GUI.TargetButtons.Bang.Parent     = Revo.GUI.TargetRightColumn
        Revo.GUI.TargetButtons.Teleport.Parent = Revo.GUI.TargetRightColumn
        Revo.GUI.TargetButtons.FaceBang.Parent = Revo.GUI.TargetRightColumn

        Revo.GUI.TargetNameInput = Instance.new("TextBox")
        Revo.GUI.TargetNameInput.Name = "TargetName_Input"
        Revo.GUI.TargetNameInput.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
        Revo.GUI.TargetNameInput.BackgroundTransparency = 0.5
        Revo.GUI.TargetNameInput.BorderColor3 = Color3.fromRGB(100, 100, 100)
        Revo.GUI.TargetNameInput.Position = UDim2.new(0, 110, 0, 10)
        Revo.GUI.TargetNameInput.Size = UDim2.new(0, 200, 0, 30)
        Revo.GUI.TargetNameInput.Font = Enum.Font.Gotham
        Revo.GUI.TargetNameInput.PlaceholderText = "Target"
        Revo.GUI.TargetNameInput.Text = ""
        Revo.GUI.TargetNameInput.TextColor3 = Color3.fromRGB(255, 255, 255)
        Revo.GUI.TargetNameInput.TextSize = 14
        Revo.GUI.TargetNameInput.ClearTextOnFocus = true
        Revo.GUI.TargetNameInput.Parent = Revo.GUI.TargetSection
        Revo.GUI:AddCorner(Revo.GUI.TargetNameInput)

    -- Update Target

        function Revo:UpdateTarget(player)
            if player and player ~= "" and player ~= self.Player then
                self.Target.Player = player.Name

                Revo.GUI.TargetNameInput.Text = player.Name
                Revo.GUI.UserIDTargetLabel.Text =
                    "UserID: "..player.UserId..
                    "\nDisplay: "..player.DisplayName..
                    "\nJoined: "..os.date("%d-%m-%Y", os.time() - player.AccountAge * 24 * 3600).." [Day/Month/Year]"

                Revo.GUI.TargetImage.Image = self.Services.Players:GetUserThumbnailAsync(
                    player.UserId,
                    Enum.ThumbnailType.HeadShot,
                    Enum.ThumbnailSize.Size420x420
                )

            else
                Revo.GUI.TargetNameInput.Text = "Target"
                Revo.GUI.UserIDTargetLabel.Text = "UserID: \nDisplay: \nJoined: "
                Revo.GUI.TargetImage.Image = "rbxassetid://10818605405"
                self.Target.Player = nil

            end
        end

    -- Get Player

        function Revo:GetPlayer(UserDisplay)
            if UserDisplay ~= "" then
                UserDisplay = UserDisplay:lower()
                for _, v in pairs(self.Services.Players:GetPlayers()) do
                    if v.Name:lower():match(UserDisplay) or v.DisplayName:lower():match(UserDisplay) then
                        return v
                    end
                end
            end
            return nil
        end

        Revo.GUI.TargetNameInput.FocusLost:Connect(function()
            local text = Revo.GUI.TargetNameInput.Text:gsub("@", "")

            if text == "" then
                Revo:UpdateTarget(nil)
                return
            end

            local player = Revo:GetPlayer(text)

            if player and player ~= Revo.Player then
                Revo:UpdateTarget(player)
                return
            end

            Revo:UpdateTarget(nil)
        end)

    -- Target Tool

        -- Target Tool Button

            Revo.GUI.ClickTargetToolButton = Instance.new("ImageButton")
            Revo.GUI.ClickTargetToolButton.Name = "ClickTargetToolButton"
            Revo.GUI.ClickTargetToolButton.BackgroundTransparency = 1
            Revo.GUI.ClickTargetToolButton.Position = UDim2.new(1, 5, 0, 0)
            Revo.GUI.ClickTargetToolButton.Size = UDim2.new(0, 30, 0, 30)
            Revo.GUI.ClickTargetToolButton.Image = "rbxassetid://2716591855"
            Revo.GUI.ClickTargetToolButton.Parent = Revo.GUI.TargetNameInput

        -- Target Tool Function

            Revo.GUI.ClickTargetToolButton.MouseButton1Click:Connect(function()

                local existingTool = Revo.Player.Backpack:FindFirstChild("ClickTarget") 
                    or (Revo.Player.Character and Revo.Player.Character:FindFirstChild("ClickTarget"))

                if existingTool then
                    return
                end

                local GetTargetTool = Instance.new("Tool")
                GetTargetTool.Name = "ClickTarget"
                GetTargetTool.RequiresHandle = false
                GetTargetTool.TextureId = "rbxassetid://2716591855"
                GetTargetTool.ToolTip = "Select Target"

                local function ActivateTool()
                    local root = Revo.Player.Character and Revo.Player.Character:FindFirstChild("HumanoidRootPart")
                    if not root then return end

                    local hit = Revo.Mouse.Target
                    local person = nil

                    if hit and hit.Parent then
                        if hit.Parent:IsA("Model") then
                            person = Revo.Services.Players:GetPlayerFromCharacter(hit.Parent)
                        elseif hit.Parent:IsA("Accessory") then
                            person = Revo.Services.Players:GetPlayerFromCharacter(hit.Parent.Parent)
                        end

                        if person then
                            Revo:UpdateTarget(person)
                        end
                    end
                end

                GetTargetTool.Activated:Connect(ActivateTool)
                GetTargetTool.Parent = Revo.Player.Backpack
            end)

    -- View Target

        Revo.GUI.TargetButtons.View.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end
            Revo.Target.Viewing = not Revo.Target.Viewing

            if Revo.Target.Viewing then
                task.spawn(function()
                    while Revo.Target.Viewing do
                        pcall(function()
                            local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                            if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
                                game.Workspace.CurrentCamera.CameraSubject = targetPlayer.Character.Humanoid
                            end
                        end)
                        task.wait()
                    end
                end)
            else
                if Revo.Player.Character and Revo.Player.Character:FindFirstChild("Humanoid") then
                    game.Workspace.CurrentCamera.CameraSubject = Revo.Player.Character.Humanoid
                end
            end
        end)

    -- Focus Target

        Revo.GUI.TargetButtons.Focus.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Focusing = not Revo.Target.Focusing

            if Revo.Target.Focusing then
                task.spawn(function()
                    while Revo.Target.Focusing do
                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            Revo:TeleportMeToPlayer(targetPlayer, "safe")
                        end
                        task.wait(0.2)
                    end
                end)
            end
        end)

    -- Teleport To Target

        Revo.GUI.TargetButtons.Teleport.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
            if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                Revo:TeleportMeToPlayer(targetPlayer, "safe")
            end
        end)

    -- Headsit Target

        Revo.GUI.TargetButtons.Headsit.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Headsitting = not Revo.Target.Headsitting

            local char = Revo.Player.Character
            if not char then return end

            local humanoid = char:FindFirstChildOfClass("Humanoid")
            local root = Revo:GetRoot(Revo.Player)
            if not humanoid or not root then return end

            if not Revo.Target.Headsitting then

                if root:FindFirstChild("BreakVelocity") then
                    root.BreakVelocity:Destroy()
                end

                humanoid.Sit = false
                humanoid.AutoRotate = true
                humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)

                task.wait()
                humanoid:ChangeState(Enum.HumanoidStateType.Running)

                if Revo._NoSitBeforeActions then
                    Revo.NoSit.Enabled = true
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
                end

                return
            end

            Revo._NoSitBeforeActions = Revo.NoSit.Enabled

            if Revo.NoSit.Enabled then
                Revo.NoSit.Enabled = false
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
            end

            task.spawn(function()
                while Revo.Target.Headsitting do
                    pcall(function()
                        if not root:FindFirstChild("BreakVelocity") then
                            local tempV = Revo.VelocityAsset:Clone()
                            tempV.Parent = root
                        end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
                            local targetHead = targetPlayer.Character.Head
                            humanoid.Sit = true
                            root.CFrame = targetHead.CFrame * CFrame.new(0, 2, 0)
                            root.Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end
            end)
        end)

    -- Backpack Target

        Revo.GUI.TargetButtons.Backpack.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Backpacking = not Revo.Target.Backpacking

            local char = Revo.Player.Character
            if not char then return end

            local humanoid = char:FindFirstChildOfClass("Humanoid")
            local root = Revo:GetRoot(Revo.Player)
            if not humanoid or not root then return end

            if not Revo.Target.Backpacking then

                if root:FindFirstChild("BreakVelocity") then
                    root.BreakVelocity:Destroy()
                end

                humanoid.Sit = false
                humanoid.AutoRotate = true
                humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)

                task.wait()
                humanoid:ChangeState(Enum.HumanoidStateType.Running)

                if Revo._NoSitBeforeSitActions then
                    Revo.NoSit.Enabled = true
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
                end

                return
            end

            Revo._NoSitBeforeSitActions = Revo.NoSit.Enabled

            if Revo.NoSit.Enabled then
                Revo.NoSit.Enabled = false
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
            end

            humanoid.Sit = true
            humanoid.AutoRotate = false

            task.spawn(function()
                while Revo.Target.Backpacking do
                    pcall(function()
                        local root = Revo:GetRoot(Revo.Player)
                        if not root then return end

                        if not root:FindFirstChild("BreakVelocity") then
                            local tempV = Revo.VelocityAsset:Clone()
                            tempV.Parent = root
                        end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            local targetRoot = targetPlayer.Character.HumanoidRootPart

                            root.CFrame =
                                targetRoot.CFrame
                                * CFrame.new(0, 0, 1.2)
                                * CFrame.Angles(0, -3, 0)

                            root.Velocity = Vector3.zero
                        end
                    end)

                    task.wait()
                end
            end)
        end)

    -- Stand Target

        Revo.GUI.TargetButtons.Stand.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Standing = not Revo.Target.Standing

            if not Revo.Target.Standing then
                Revo:StopAnim()
                local root = Revo:GetRoot(Revo.Player)
                if root and root:FindFirstChild("BreakVelocity") then
                    root.BreakVelocity:Destroy()
                end
                return
            end

            Revo:PlayAnim(13823324057, 4, 0)

            task.spawn(function()
                while Revo.Target.Standing do
                    pcall(function()
                        local root = Revo:GetRoot(Revo.Player)
                        if not root then return end

                        if not root:FindFirstChild("BreakVelocity") then
                            local tempV = Revo.VelocityAsset:Clone()
                            tempV.Parent = root
                        end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            local targetRoot = targetPlayer.Character.HumanoidRootPart
                            root.CFrame = targetRoot.CFrame * CFrame.new(-3, 1, 0)
                            root.Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end
            end)
        end)
    
    -- Doggy Target

        Revo.GUI.TargetButtons.Doggy.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Dogging = not Revo.Target.Dogging

            if not Revo.Target.Dogging then
                Revo:StopAnim()
                local root = Revo:GetRoot(Revo.Player)
                if root and root:FindFirstChild("BreakVelocity") then
                    root.BreakVelocity:Destroy()
                end
                return
            end

            Revo:PlayAnim(13694096724, 3.4, 0)

            task.spawn(function()
                while Revo.Target.Dogging do
                    pcall(function()
                        local root = Revo:GetRoot(Revo.Player)
                        if not root then return end

                        if not root:FindFirstChild("BreakVelocity") then
                            local tempV = Revo.VelocityAsset:Clone()
                            tempV.Parent = root
                        end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("LowerTorso") then
                            local targetRoot = targetPlayer.Character.LowerTorso
                            root.CFrame = targetRoot.CFrame * CFrame.new(0, 0.23, 0)
                            root.Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end
            end)
        end)

    -- Drag Target

        Revo.GUI.TargetButtons.Drag.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Dragging = not Revo.Target.Dragging

            if not Revo.Target.Dragging then
                Revo:StopAnim()
                local root = Revo:GetRoot(Revo.Player)
                if root and root:FindFirstChild("BreakVelocity") then
                    root.BreakVelocity:Destroy()
                end
                return
            end

            Revo:PlayAnim(10714360343, 0.5, 0)

            task.spawn(function()
                while Revo.Target.Dragging do
                    pcall(function()
                        local root = Revo:GetRoot(Revo.Player)
                        if not root then return end

                        if not root:FindFirstChild("BreakVelocity") then
                            local tempV = Revo.VelocityAsset:Clone()
                            tempV.Parent = root
                        end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("RightHand") then
                            local targetRoot = targetPlayer.Character.RightHand
                            root.CFrame = targetRoot.CFrame * CFrame.new(0, -2.5, 1) * CFrame.Angles(-2, -3, 0)
                            root.Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end
            end)
        end)

    -- Bang Target

        Revo.GUI.TargetButtons.Bang.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Banging = not Revo.Target.Banging

            if not Revo.Target.Banging then
                Revo:StopAnim()
                local root = Revo:GetRoot(Revo.Player)
                if root and root:FindFirstChild("BreakVelocity") then
                    root.BreakVelocity:Destroy()
                end
                return
            end

            Revo:PlayAnim(5918726674, 0, 1)

            task.spawn(function()
                while Revo.Target.Banging do
                    pcall(function()
                        local root = Revo:GetRoot(Revo.Player)
                        if not root then return end

                        if not root:FindFirstChild("BreakVelocity") then
                            local tempV = Revo.VelocityAsset:Clone()
                            tempV.Parent = root
                        end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character then
                            local otherRoot = Revo:GetRoot(targetPlayer)
                            root.CFrame = otherRoot.CFrame * CFrame.new(0, 0, 1.1)
                            root.Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end
            end)
        end)

    -- Fling Target

        function Revo:StartFling(targetPlayer)
            if not targetPlayer or not targetPlayer.Character then return end
            
            local myChar = self.Player.Character
            local myRoot = self:GetRoot(self.Player)
            local targetRoot = self:GetRoot(targetPlayer)
            local humanoid = myChar and myChar:FindFirstChildOfClass("Humanoid")

            if not myRoot or not targetRoot or not humanoid then return end

            self.FlingData.OldCFrame = myRoot.CFrame
            self:StopTargetActions()
            self.Target.Flinging = true

            if Revo.NoRagdoll and Revo.NoRagdoll.Enabled then
                Revo.NoRagdoll.Bypass = true
                self.FlingData.BypassingNoRagdoll = true
            end

            humanoid.PlatformStand = true
            humanoid.AutoRotate = false

            local bv = Instance.new("BodyVelocity")
            bv.Name = "BreakVelocity"
            bv.MaxForce = Vector3.new(1e9, 1e9, 1e9)
            bv.Velocity = Vector3.zero
            bv.Parent = myRoot

            self.FlingData.Connection = game:GetService("RunService").Heartbeat:Connect(function()

                if not self.Target.Flinging then return end

                local target = self.Services.Players:FindFirstChild(self.Target.Player)
                if not target or not target.Character then return end

                local targetRoot = self:GetRoot(target)
                if not targetRoot then return end

                myRoot.CFrame = targetRoot.CFrame

                myRoot.AssemblyAngularVelocity = Vector3.new(9999, 9999, 9999)
                myRoot.AssemblyLinearVelocity = Vector3.new(9999, 9999, 9999)

            end)
        end

        function Revo:StopFling()
            self.Target.Flinging = false

            if self.FlingData.Connection then
                self.FlingData.Connection:Disconnect()
                self.FlingData.Connection = nil
            end

            local char = self.Player.Character
            if not char then return end

            local root = self:GetRoot(self.Player)
            local humanoid = char:FindFirstChildOfClass("Humanoid")

            task.spawn(function()
                task.wait(0.15)

                if root then
                    local breakVel = root:FindFirstChild("BreakVelocity")
                    if breakVel then
                        breakVel:Destroy()
                    end

                    root.AssemblyAngularVelocity = Vector3.zero
                    root.AssemblyLinearVelocity = Vector3.zero
                    root.Velocity = Vector3.zero
                    root.RotVelocity = Vector3.zero

                    if self.FlingData.OldCFrame then
                        root.CFrame = self.FlingData.OldCFrame
                    end
                end

                if humanoid then
                    humanoid.PlatformStand = false
                    humanoid.AutoRotate = true
                    humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)
                    task.wait()
                    humanoid:ChangeState(Enum.HumanoidStateType.Running)
                end

                if self.FlingData.BypassingNoRagdoll then
                    Revo.NoRagdoll.Bypass = false
                    self.FlingData.BypassingNoRagdoll = nil
                end

                self.FlingData.OldCFrame = nil
            end)
        end

        Revo.GUI.TargetButtons.Fling.MouseButton1Click:Connect(function()

            if not Revo.Target.Player then
                return
            end

            if not Revo.Target.Flinging then
                Revo:StartFling(game.Players:FindFirstChild(Revo.Target.Player))
            else
                Revo:StopFling()
            end

        end)

    -- Push Target

        function Revo:PushTarget(targetPlayer)
            if not targetPlayer or not targetPlayer.Character then return end

            local myChar = self.Player.Character
            local myRoot = self:GetRoot(self.Player)
            local targetRoot = self:GetRoot(targetPlayer)
            local humanoid = myChar and myChar:FindFirstChildOfClass("Humanoid")

            if not myRoot or not targetRoot or not humanoid then return end

            local oldCFrame = myRoot.CFrame

            -- ðŸ”¥ Bypass NoRagdoll if enabled
            local bypassing = false
            if Revo.NoRagdoll and Revo.NoRagdoll.Enabled then
                Revo.NoRagdoll.Bypass = true
                bypassing = true
            end

            humanoid.PlatformStand = true
            humanoid.AutoRotate = false

            local bv = Instance.new("BodyVelocity")
            bv.Name = "BreakVelocity"
            bv.MaxForce = Vector3.new(1e9,1e9,1e9)
            bv.Velocity = Vector3.zero
            bv.Parent = myRoot

            local startTime = tick()
            local connection

            connection = game:GetService("RunService").Heartbeat:Connect(function()
                if tick() - startTime > 0.5 then
                    connection:Disconnect()
                    return
                end

                myRoot.CFrame = targetRoot.CFrame
                myRoot.AssemblyAngularVelocity = Vector3.new(6000,6000,6000)
                myRoot.AssemblyLinearVelocity = Vector3.new(6000,6000,6000)
            end)

            task.delay(0.55, function()

                if connection then
                    connection:Disconnect()
                end

                if myRoot then
                    local breakVel = myRoot:FindFirstChild("BreakVelocity")
                    if breakVel then
                        breakVel:Destroy()
                    end

                    myRoot.AssemblyAngularVelocity = Vector3.zero
                    myRoot.AssemblyLinearVelocity = Vector3.zero
                    myRoot.Velocity = Vector3.zero

                    myRoot.CFrame = oldCFrame
                end

                if humanoid then
                    humanoid.PlatformStand = false
                    humanoid.AutoRotate = true
                    humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)
                    task.wait()
                    humanoid:ChangeState(Enum.HumanoidStateType.Running)
                end

                -- ðŸ”¥ Restore NoRagdoll protection
                if bypassing then
                    Revo.NoRagdoll.Bypass = false
                end
            end)
        end

        Revo.GUI.TargetButtons.Push.MouseButton1Click:Connect(function()

            if not Revo.Target.Player then return end

            local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
            if not targetPlayer then return end

            Revo:PushTarget(targetPlayer)

        end)

    -- Face Bang Target

        -- Speed And Distance Buttons

            Revo.Buttons.FaceBangSpeed = Instance.new("TextButton")
            Revo.Buttons.FaceBangSpeed.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.FaceBangSpeed.Position = UDim2.new(0, 215, 0, 330)
            Revo.Buttons.FaceBangSpeed.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.FaceBangSpeed.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.FaceBangSpeed.Font = Enum.Font.Gotham
            Revo.Buttons.FaceBangSpeed.TextSize = 16
            Revo.Buttons.FaceBangSpeed.Text = "Face Bang Speed: " .. (Revo.FaceBangSettings.Speed or 3)
            Revo.Buttons.FaceBangSpeed.Parent = Revo.GUI.TargetRightColumn
            Revo.GUI:AddCorner(Revo.Buttons.FaceBangSpeed)

            Revo.Inputs.FaceBangSpeed = Instance.new("TextBox")
            Revo.Inputs.FaceBangSpeed.Size = Revo.Buttons.FaceBangSpeed.Size
            Revo.Inputs.FaceBangSpeed.Position = Revo.Buttons.FaceBangSpeed.Position
            Revo.Inputs.FaceBangSpeed.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Inputs.FaceBangSpeed.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Inputs.FaceBangSpeed.Font = Enum.Font.Gotham
            Revo.Inputs.FaceBangSpeed.TextSize = 16
            Revo.Inputs.FaceBangSpeed.PlaceholderText = "Enter Face Bang Speed"
            Revo.Inputs.FaceBangSpeed.Visible = false
            Revo.Inputs.FaceBangSpeed.ClearTextOnFocus = true
            Revo.Inputs.FaceBangSpeed.Text = ""
            Revo.Inputs.FaceBangSpeed.Parent = Revo.GUI.TargetRightColumn
            Revo.GUI:AddCorner(Revo.Inputs.FaceBangSpeed)

            Revo.Buttons.FaceBangDistance = Instance.new("TextButton")
            Revo.Buttons.FaceBangDistance.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.FaceBangDistance.Position = UDim2.new(0, 5, 0, 330)
            Revo.Buttons.FaceBangDistance.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.FaceBangDistance.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.FaceBangDistance.Font = Enum.Font.Gotham
            Revo.Buttons.FaceBangDistance.TextSize = 16
            Revo.Buttons.FaceBangDistance.Text = "Face Bang Distance: " .. (Revo.FaceBangSettings.Distance or 0.5)
            Revo.Buttons.FaceBangDistance.Parent = Revo.GUI.TargetLeftColumn
            Revo.GUI:AddCorner(Revo.Buttons.FaceBangDistance)

            Revo.Inputs.FaceBangDistance = Instance.new("TextBox")
            Revo.Inputs.FaceBangDistance.Size = Revo.Buttons.FaceBangDistance.Size
            Revo.Inputs.FaceBangDistance.Position = Revo.Buttons.FaceBangDistance.Position
            Revo.Inputs.FaceBangDistance.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Inputs.FaceBangDistance.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Inputs.FaceBangDistance.Font = Enum.Font.Gotham
            Revo.Inputs.FaceBangDistance.TextSize = 16
            Revo.Inputs.FaceBangDistance.PlaceholderText = "Enter Face Bang Distance"
            Revo.Inputs.FaceBangDistance.Visible = false
            Revo.Inputs.FaceBangDistance.ClearTextOnFocus = true
            Revo.Inputs.FaceBangDistance.Text = ""
            Revo.Inputs.FaceBangDistance.Parent = Revo.GUI.TargetLeftColumn
            Revo.GUI:AddCorner(Revo.Inputs.FaceBangDistance)

            -- Speed And Distance Functions

                Revo.Buttons.FaceBangSpeed.MouseButton1Click:Connect(function()
                    Revo.Buttons.FaceBangSpeed.Visible = false
                    Revo.Inputs.FaceBangSpeed.Visible = true
                    Revo.Inputs.FaceBangSpeed:CaptureFocus()
                end)

                Revo.Inputs.FaceBangSpeed.FocusLost:Connect(function()
                    pcall(function()
                        local Speed = Revo.Inputs.FaceBangSpeed.Text:gsub("%D", "")
                        if Speed == "" then Speed = 3 end

                        Speed = tonumber(Speed)

                        Revo.FaceBangSettings.Speed = Speed
                        Revo.Buttons.FaceBangSpeed.Text = "Face Bang Speed: " .. Speed

                        Revo.Save.Data.FaceBangSpeed = Speed
                        Revo.Save:Write()
                    end)

                    Revo.Inputs.FaceBangSpeed.Text = ""
                    Revo.Inputs.FaceBangSpeed.Visible = false
                    Revo.Buttons.FaceBangSpeed.Visible = true
                end)

                Revo.Buttons.FaceBangDistance.MouseButton1Click:Connect(function()
                    Revo.Buttons.FaceBangDistance.Visible = false
                    Revo.Inputs.FaceBangDistance.Visible = true
                    Revo.Inputs.FaceBangDistance:CaptureFocus()
                end)

                Revo.Inputs.FaceBangDistance.FocusLost:Connect(function()
                    pcall(function()
                        local Distance = Revo.Inputs.FaceBangDistance.Text:gsub("[^%d%.]", "")
                        if Distance == "" then Distance = 0.5 end

                        Distance = tonumber(Distance)

                        Revo.FaceBangSettings.Distance = Distance
                        Revo.Buttons.FaceBangDistance.Text = "Face Bang Distance: " .. Distance

                        Revo.Save.Data.FaceBangDistance = Distance
                        Revo.Save:Write()
                    end)

                    Revo.Inputs.FaceBangDistance.Text = ""
                    Revo.Inputs.FaceBangDistance.Visible = false
                    Revo.Buttons.FaceBangDistance.Visible = true
                end)

        -- Face Bang Function

            Revo.GUI.TargetButtons.FaceBang.MouseButton1Click:Connect(function()
                if not Revo.Target.Player then return end

                Revo.Target.FaceBanging = not Revo.Target.FaceBanging

                local char = Revo.Player.Character
                if not char then return end

                local humanoid = char:FindFirstChildOfClass("Humanoid")
                local root = Revo:GetRoot(Revo.Player)
                if not humanoid or not root then return end

                if not Revo.Target.FaceBanging then

                    if root:FindFirstChild("BreakVelocity") then
                        root.BreakVelocity:Destroy()
                    end

                    humanoid.PlatformStand = false
                    humanoid.AutoRotate = true
                    humanoid:ChangeState(Enum.HumanoidStateType.Running)

                    char.Animate.Disabled = false

                    if Revo._NoSitBeforeActions then
                        Revo.NoSit.Enabled = true
                        humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
                    end

                    return
                end

                Revo._NoSitBeforeActions = Revo.NoSit.Enabled

                if Revo.NoSit.Enabled then
                    Revo.NoSit.Enabled = false
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
                end

                humanoid.AutoRotate = false
                humanoid.PlatformStand = true

                task.spawn(function()
                    local t = 0
                    while Revo.Target.FaceBanging do
                        pcall(function()

                            if not root:FindFirstChild("BreakVelocity") then
                                local tempV = Revo.VelocityAsset:Clone()
                                tempV.Parent = root
                            end

                            local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                            if targetPlayer 
                            and targetPlayer.Character 
                            and targetPlayer.Character:FindFirstChild("Head") then

                                local targetHead = targetPlayer.Character.Head

                                local startOffset = 0.7
                                local offsetZ = math.sin(t * Revo.FaceBangSettings.Speed) 
                                    * Revo.FaceBangSettings.Distance

                                root.CFrame =
                                    targetHead.CFrame
                                    * CFrame.new(0, 1, -Revo.FaceBangSettings.Distance - startOffset + offsetZ)
                                    * CFrame.Angles(0, math.rad(180), 0)

                                root.Velocity = Vector3.new(0, 0, 0)
                            end

                            t = t + task.wait()
                        end)
                    end
                end)
            end)

    -- Inspect Target

        Revo.GUI.TargetButtons.Inspect      = Revo.GUI:CreateTargetButton("Inspect",     "Inspect",                UDim2.new(0, 5, 0, 365))
        Revo.GUI.TargetButtons.Inspect.Parent  = Revo.GUI.TargetLeftColumn

        Revo.GUI.TargetButtons.Inspect.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            local target = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
            if not target or not target.Character then return end

            local humanoid = target.Character:FindFirstChildOfClass("Humanoid")
            if not humanoid then return end

            local description = humanoid:GetAppliedDescription()

            Revo.Services.GuiService:InspectPlayerFromHumanoidDescription(
                description,
                target.Name
            )
        end)

    -- Player Removing

        function Revo:StopTargetActions()

            Revo.Target.Viewing      = false
            Revo.Target.Focusing     = false
            Revo.Target.Headsitting  = false
            Revo.Target.Backpacking  = false
            Revo.Target.Standing     = false
            Revo.Target.Dogging      = false
            Revo.Target.Dragging     = false
            Revo.Target.Banging      = false
            Revo.Target.FaceBanging  = false

            if Revo.Target.Flinging then
                self:StopFling()
            end

            local char = Revo.Player.Character
            if not char then return end

            local humanoid = char:FindFirstChildOfClass("Humanoid")
            local animate = char:FindFirstChild("Animate")
            local root = Revo:GetRoot(Revo.Player)

            if root and root:FindFirstChild("BreakVelocity") then
                root.BreakVelocity:Destroy()
            end

            if humanoid then
                humanoid.PlatformStand = false
                humanoid.Sit = false
                humanoid.AutoRotate = true
                humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)
                task.wait()
                humanoid:ChangeState(Enum.HumanoidStateType.Running)

                if self._NoSitBeforeActions then
                    self.NoSit.Enabled = true
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
                end
            end

            if animate then
                animate.Disabled = false
            end

            local camera = workspace.CurrentCamera
            if camera and humanoid then
                camera.CameraSubject = humanoid
            end

            if Revo.NoRagdoll and Revo.NoRagdoll.Bypass then
                Revo.NoRagdoll.Bypass = false
            end
        end

        Revo.Services.Players.PlayerRemoving:Connect(function(player)
            pcall(function()
                if Revo.Target.Player and player.Name == Revo.Target.Player then
                    Revo:UpdateTarget(nil)
                    Revo:StopTargetActions()
                    Revo:Notify("Revo", "Targeted Player Left Or Rejoined.", 5)
                end
            end)
        end)

-- Animations

    Revo.AnimScrollFrame = Instance.new("ScrollingFrame")
    Revo.AnimScrollFrame.Name = "Anim_Section"
    Revo.AnimScrollFrame.Size = UDim2.new(1, 6, 1, -40)
    Revo.AnimScrollFrame.Position = UDim2.new(0, -3, 0, 35)
    Revo.AnimScrollFrame.BackgroundTransparency = 1
    Revo.AnimScrollFrame.ScrollBarThickness = 0
    Revo.AnimScrollFrame.ScrollBarImageTransparency = 1
    Revo.AnimScrollFrame.ClipsDescendants = true
    Revo.AnimScrollFrame.ScrollingDirection = Enum.ScrollingDirection.Y
    Revo.AnimScrollFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
    Revo.AnimScrollFrame.Parent = Revo.Pages.Animations

    Revo.AnimLeftColumn = Instance.new("Frame")
    Revo.AnimLeftColumn.Size = UDim2.new(0.5, 0, 1, 0)
    Revo.AnimLeftColumn.Position = UDim2.new(0, 0, 0, 0)
    Revo.AnimLeftColumn.BackgroundTransparency = 1
    Revo.AnimLeftColumn.Parent = Revo.AnimScrollFrame

    Revo.AnimRightColumn = Instance.new("Frame")
    Revo.AnimRightColumn.Size = UDim2.new(0.5, 0, 1, 0)
    Revo.AnimRightColumn.Position = UDim2.new(0.5, 0, 0, 0)
    Revo.AnimRightColumn.BackgroundTransparency = 1
    Revo.AnimRightColumn.Parent = Revo.AnimScrollFrame

    Revo.AnimLeftLayout = Instance.new("UIListLayout")
    Revo.AnimLeftLayout.FillDirection = Enum.FillDirection.Vertical
    Revo.AnimLeftLayout.Padding = UDim.new(0, 5)
    Revo.AnimLeftLayout.SortOrder = Enum.SortOrder.LayoutOrder
    Revo.AnimLeftLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    Revo.AnimLeftLayout.Parent = Revo.AnimLeftColumn

    Revo.AnimRightLayout = Instance.new("UIListLayout")
    Revo.AnimRightLayout.FillDirection = Enum.FillDirection.Vertical
    Revo.AnimRightLayout.Padding = UDim.new(0, 5)
    Revo.AnimRightLayout.SortOrder = Enum.SortOrder.LayoutOrder
    Revo.AnimRightLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    Revo.AnimRightLayout.Parent = Revo.AnimRightColumn

    Revo.AnimPadding = Instance.new("UIPadding")
    Revo.AnimPadding.PaddingTop = UDim.new(0, 3)
    Revo.AnimPadding.PaddingBottom = UDim.new(0, 5)
    Revo.AnimPadding.PaddingLeft = UDim.new(0, 0)
    Revo.AnimPadding.PaddingRight = UDim.new(0, 0)
    Revo.AnimPadding.Parent = Revo.AnimScrollFrame

    function Revo:UpdateAnimCanvas()
        pcall(function()
            local scroll = Revo.AnimScrollFrame
            local leftLayout = Revo.AnimLeftLayout
            local rightLayout = Revo.AnimRightLayout
            if not scroll or not leftLayout or not rightLayout then return end

            scroll.CanvasSize = UDim2.new(
                0, 0,
                0, math.max(leftLayout.AbsoluteContentSize.Y, rightLayout.AbsoluteContentSize.Y) + 10
            )
        end)
    end

    Revo.AnimLeftLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        Revo:UpdateAnimCanvas()
    end)
    Revo.AnimRightLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        Revo:UpdateAnimCanvas()
    end)

    task.defer(function()
        Revo:UpdateAnimCanvas()
    end)


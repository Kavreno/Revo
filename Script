            Revo.Inputs.RewindKey.Size = Revo.Buttons.RewindKey.Size
            Revo.Inputs.RewindKey.Position = Revo.Buttons.RewindKey.Position
            Revo.Inputs.RewindKey.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Inputs.RewindKey.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Inputs.RewindKey.Font = Enum.Font.Gotham
            Revo.Inputs.RewindKey.TextSize = 16
            Revo.Inputs.RewindKey.PlaceholderText = "Press Any Key"
            Revo.Inputs.RewindKey.Visible = false
            Revo.Inputs.RewindKey.ClearTextOnFocus = true
            Revo.Inputs.RewindKey.Text = ""
            Revo.Inputs.RewindKey.Parent = Revo.GUI.CharacterSection
            Revo.GUI:AddCorner(Revo.Inputs.RewindKey)

        -- Rewind Function

            Revo.Services.RunService.Heartbeat:Connect(function()
                if not Revo.Rewind.Recording then return end
                local now = os.clock()
                if now - Revo.Rewind.LastRecordTime < Revo.Rewind.RecordInterval then return end
                Revo.Rewind.LastRecordTime = now

                local char = Revo.Player.Character
                if not char then return end
                local hrp = char:FindFirstChild("HumanoidRootPart")
                if not hrp then return end

                Revo.Rewind.History[Revo.Rewind.WriteIndex] = hrp.CFrame
                Revo.Rewind.WriteIndex = Revo.Rewind.WriteIndex + 1
                if Revo.Rewind.WriteIndex > Revo.Rewind.MaxFrames then
                    Revo.Rewind.WriteIndex = 1
                end

                Revo.Rewind.StoredFrames = math.min(Revo.Rewind.StoredFrames + 1, Revo.Rewind.MaxFrames)
            end)

            function Revo.Rewind:LerpCFrame(cfA, cfB, alpha)
                local pos = cfA.Position:Lerp(cfB.Position, alpha)
                local rot = cfA - cfA.Position
                local rotB = cfB - cfB.Position
                return CFrame.new(pos) * CFrame.fromMatrix(Vector3.new(), rot.LookVector:Lerp(rotB.LookVector, alpha), rot.UpVector:Lerp(rotB.UpVector, alpha))
            end

            function Revo.Rewind:Toggle()
                self.Enabled = not self.Enabled
                self.Recording = not self.Enabled

                if not self.Enabled then return end

                task.spawn(function()
                    local char = Revo.Player.Character
                    if not char then return end
                    local hrp = char:FindFirstChild("HumanoidRootPart")
                    if not hrp then return end

                    local lastRewindTime = 0

                    while self.Enabled and self.StoredFrames > 0 do
                        local now = os.clock()
                        if now - lastRewindTime >= self.Speed then
                            lastRewindTime = now

                            local prevIndex = self.WriteIndex - 1
                            if prevIndex < 1 then prevIndex = self.MaxFrames end

                            local cf1 = self.History[prevIndex]
                            local cf2 = self.History[self.WriteIndex]
                            if cf1 and cf2 then
                                hrp.CFrame = self:LerpCFrame(cf2, cf1, 0.5)
                            end

                            self.WriteIndex = self.WriteIndex - 1
                            if self.WriteIndex < 1 then self.WriteIndex = self.MaxFrames end
                            if self.StoredFrames > 0 then
                                self.StoredFrames = self.StoredFrames - 1
                            end
                        end
                        Revo.Services.RunService.Heartbeat:Wait()
                    end

                    self.Enabled = false
                    self.Recording = true
                end)
            end

        -- Rewind Key Function

            function Revo.Rewind:EnableGlobalListener()
                if self.GlobalConnection then return end

                self.GlobalConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if self.CapturingKey then return end

                    if self.Key
                    and input.UserInputType == Enum.UserInputType.Keyboard
                    and input.KeyCode == self.Key then
                        self:Toggle()
                    end
                end)
            end

            Revo.Rewind:EnableGlobalListener()

            function Revo.Rewind:CancelKeyCapture()
                self.Key = nil
                Revo.Buttons.RewindKey.Text = "Rewind Key: None"

                Revo.Inputs.RewindKey.Visible = false
                Revo.Buttons.RewindKey.Visible = true

                self.CapturingKey = false

                if self.CaptureConnection then
                    self.CaptureConnection:Disconnect()
                    self.CaptureConnection = nil
                end

                self:EnableGlobalListener()
            end

            Revo.Buttons.RewindKey.MouseButton1Click:Connect(function()
                Revo.Buttons.RewindKey.Visible = false
                Revo.Inputs.RewindKey.Visible = true
                Revo.Inputs.RewindKey.Text = "Press Any Key"

                Revo.Rewind.CapturingKey = true

                if Revo.Rewind.GlobalConnection then
                    Revo.Rewind.GlobalConnection:Disconnect()
                    Revo.Rewind.GlobalConnection = nil
                end

                if Revo.Rewind.CaptureConnection then
                    Revo.Rewind.CaptureConnection:Disconnect()
                end

                Revo.Rewind.CaptureConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if not Revo.Rewind.CapturingKey then return end

                    if input.UserInputType == Enum.UserInputType.Keyboard then
                        if input.KeyCode == Enum.KeyCode.Backspace then
                            Revo.Rewind:CancelKeyCapture()
                        else
                            Revo.Rewind.Key = input.KeyCode
                            Revo.Buttons.RewindKey.Text = "Rewind Key: " .. Revo.Rewind.Key.Name

                            Revo.Inputs.RewindKey.Visible = false
                            Revo.Buttons.RewindKey.Visible = true
                            Revo.Rewind.CapturingKey = false

                            Revo.Rewind.CaptureConnection:Disconnect()
                            Revo.Rewind.CaptureConnection = nil

                            Revo.Rewind:EnableGlobalListener()
                        end

                    elseif input.UserInputType == Enum.UserInputType.MouseButton1
                        or input.UserInputType == Enum.UserInputType.MouseButton2
                        or input.UserInputType == Enum.UserInputType.MouseButton3 then
                        Revo.Rewind:CancelKeyCapture()
                    end
                end)
            end) 

    -- Trip

        -- Trip Key

            Revo.Buttons.TripKey = Instance.new("TextButton")
            Revo.Buttons.TripKey.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.TripKey.Position = UDim2.new(0, 215, 0, 110)
            Revo.Buttons.TripKey.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.TripKey.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.TripKey.Font = Enum.Font.Gotham
            Revo.Buttons.TripKey.TextSize = 16
            Revo.Buttons.TripKey.Text = "Trip Key: "..(Revo.Trip.Key and Revo.Trip.Key.Name or "None")
            Revo.Buttons.TripKey.Parent = Revo.GUI.CharacterSection
            Revo.GUI:AddCorner(Revo.Buttons.TripKey)

            Revo.Inputs.TripKey = Instance.new("TextBox")
            Revo.Inputs.TripKey.Size = Revo.Buttons.TripKey.Size
            Revo.Inputs.TripKey.Position = Revo.Buttons.TripKey.Position
            Revo.Inputs.TripKey.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Inputs.TripKey.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Inputs.TripKey.Font = Enum.Font.Gotham
            Revo.Inputs.TripKey.TextSize = 16
            Revo.Inputs.TripKey.PlaceholderText = "Press Any Key"
            Revo.Inputs.TripKey.Visible = false
            Revo.Inputs.TripKey.ClearTextOnFocus = true
            Revo.Inputs.TripKey.Text = ""
            Revo.Inputs.TripKey.Parent = Revo.GUI.CharacterSection
            Revo.GUI:AddCorner(Revo.Inputs.TripKey)

        -- Trip Function

            function Revo.Trip:TripPlayer()
                local character = Revo.Player.Character
                if not character then return end

                local humanoid = character:FindFirstChildWhichIsA("Humanoid")
                local root = character:FindFirstChild("HumanoidRootPart")
                if humanoid and root then
                    humanoid:ChangeState(Enum.HumanoidStateType.FallingDown)
                    root.Velocity = root.CFrame.LookVector * 30
                end
            end     
            
        -- Trip Key Function

            function Revo.Trip:EnableGlobalListener()
                if self.GlobalConnection then return end

                self.GlobalConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if self.CapturingKey then return end

                    if self.Key
                    and input.UserInputType == Enum.UserInputType.Keyboard
                    and input.KeyCode == self.Key then
                        self:TripPlayer()
                    end
                end)
            end

            Revo.Trip:EnableGlobalListener()

            function Revo.Trip:CancelKeyCapture()
                self.Key = nil
                Revo.Buttons.TripKey.Text = "Trip Key: None"

                Revo.Inputs.TripKey.Visible = false
                Revo.Buttons.TripKey.Visible = true

                self.CapturingKey = false

                if self.CaptureConnection then
                    self.CaptureConnection:Disconnect()
                    self.CaptureConnection = nil
                end

                self:EnableGlobalListener()
            end

            Revo.Buttons.TripKey.MouseButton1Click:Connect(function()
                Revo.Buttons.TripKey.Visible = false
                Revo.Inputs.TripKey.Visible = true
                Revo.Inputs.TripKey.Text = "Press Any Key"

                Revo.Trip.CapturingKey = true

                if Revo.Trip.GlobalConnection then
                    Revo.Trip.GlobalConnection:Disconnect()
                    Revo.Trip.GlobalConnection = nil
                end

                if Revo.Trip.CaptureConnection then
                    Revo.Trip.CaptureConnection:Disconnect()
                end

                Revo.Trip.CaptureConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if not Revo.Trip.CapturingKey then return end

                    if input.UserInputType == Enum.UserInputType.Keyboard then
                        if input.KeyCode == Enum.KeyCode.Backspace then
                            Revo.Trip:CancelKeyCapture()
                        else
                            Revo.Trip.Key = input.KeyCode
                            Revo.Buttons.TripKey.Text = "Trip Key: " .. Revo.Trip.Key.Name

                            Revo.Inputs.TripKey.Visible = false
                            Revo.Buttons.TripKey.Visible = true
                            Revo.Trip.CapturingKey = false

                            Revo.Trip.CaptureConnection:Disconnect()
                            Revo.Trip.CaptureConnection = nil

                            Revo.Trip:EnableGlobalListener()
                        end

                    elseif input.UserInputType == Enum.UserInputType.MouseButton1
                        or input.UserInputType == Enum.UserInputType.MouseButton2
                        or input.UserInputType == Enum.UserInputType.MouseButton3 then
                        Revo.Trip:CancelKeyCapture()
                    end
                end)
            end)

    -- Jerk

        -- Jerk Button

            Revo.Buttons.Jerk = Instance.new("TextButton")
            Revo.Buttons.Jerk.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.Jerk.Position = UDim2.new(0, 5, 0, 75)
            Revo.Buttons.Jerk.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.Jerk.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.Jerk.Font = Enum.Font.Gotham
            Revo.Buttons.Jerk.TextSize = 16
            Revo.Buttons.Jerk.Text = "Jerk"
            Revo.Buttons.Jerk.Parent = Revo.GUI.CharacterSection
            Revo.GUI:AddCorner(Revo.Buttons.Jerk)

        -- Jerk Function

            function Revo:runJerk()
                local character = self.Player.Character or self.Player.CharacterAdded:Wait()
                local humanoid = character:WaitForChild("Humanoid")
                local backpack = self.Player:WaitForChild("Backpack")

                if backpack:FindFirstChild("Jerk Off") then return end

                local tool = Instance.new("Tool")
                tool.Name = "Jerk Off"
                tool.ToolTip = ";)"
                tool.RequiresHandle = false
                tool.Parent = backpack

                local jorkin = false
                local track

                local function stopTomfoolery()
                    jorkin = false
                    if track then
                        track:Stop()
                        track = nil
                    end
                end

                tool.Equipped:Connect(function()
                    jorkin = true
                end)

                tool.Unequipped:Connect(stopTomfoolery)
                humanoid.Died:Connect(stopTomfoolery)

                task.spawn(function()
                    while task.wait() do
                        if jorkin then
                            local isR15 = humanoid.RigType == Enum.HumanoidRigType.R15

                            if not track then
                                local anim = Instance.new("Animation")
                                anim.AnimationId = isR15
                                    and "rbxassetid://698251653"
                                    or "rbxassetid://72042024"

                                track = humanoid:LoadAnimation(anim)
                            end

                            track:Play()
                            track:AdjustSpeed(isR15 and 0.7 or 0.65)
                            track.TimePosition = 0.6

                            task.wait(0.1)
                            while track and track.TimePosition < (isR15 and 0.7 or 0.65) do
                                task.wait(0.1)
                            end

                            if track then
                                track:Stop()
                                track = nil
                            end
                        end
                    end
                end)
            end

            Revo.Buttons.Jerk.MouseButton1Click:Connect(function()
                Revo:runJerk()
            end)

    -- Walk On Air

        -- Walk On Air Button

            Revo.Buttons.WalkOnAir = Instance.new("TextButton")
            Revo.Buttons.WalkOnAir.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.WalkOnAir.Position = UDim2.new(0, 5, 0, 110)

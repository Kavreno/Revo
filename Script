        -- Fly Key Function

            function Revo.Fly:EnableGlobalListener()
                if self.GlobalConnection then return end

                self.GlobalConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if self.CapturingKey then return end

                    if self.Key and input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == self.Key then
                        self.Enabled = not self.Enabled
                        if self.Enabled then
                            self:ConnectKeys(Revo.Player:GetMouse())
                            task.spawn(function() self:Start() end)
                        else
                            self:DisconnectKeys()
                            self.Ctrl = {f = 0, b = 0, l = 0, r = 0}
                            self.LastCtrl = {f = 0, b = 0, l = 0, r = 0}
                        end
                    end
                end)
            end

            Revo.Fly:EnableGlobalListener()

            function Revo.Fly:CancelKeyCapture()
                self.Key = nil
                Revo.Buttons.FlyKey.Text = "Fly Key: None"

                Revo.Inputs.FlyKey.Visible = false
                Revo.Buttons.FlyKey.Visible = true

                self.CapturingKey = false

                if self.CaptureConnection then
                    self.CaptureConnection:Disconnect()
                    self.CaptureConnection = nil
                end

                self:EnableGlobalListener()
            end

            Revo.Buttons.FlyKey.MouseButton1Click:Connect(function()
                Revo.Buttons.FlyKey.Visible = false
                Revo.Inputs.FlyKey.Visible = true
                Revo.Inputs.FlyKey.Text = "Press Any Key"

                Revo.Fly.CapturingKey = true

                if Revo.Fly.GlobalConnection then
                    Revo.Fly.GlobalConnection:Disconnect()
                    Revo.Fly.GlobalConnection = nil
                end

                if Revo.Fly.CaptureConnection then
                    Revo.Fly.CaptureConnection:Disconnect()
                end

                Revo.Fly.CaptureConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if not Revo.Fly.CapturingKey then return end

                    if input.UserInputType == Enum.UserInputType.Keyboard then
                        if input.KeyCode == Enum.KeyCode.Backspace then
                            Revo.Fly:CancelKeyCapture()
                        else
                            Revo.Fly.Key = input.KeyCode
                            Revo.Buttons.FlyKey.Text = "Fly Key: " .. Revo.Fly.Key.Name

                            Revo.Inputs.FlyKey.Visible = false
                            Revo.Buttons.FlyKey.Visible = true
                            Revo.Fly.CapturingKey = false

                            Revo.Fly.CaptureConnection:Disconnect()
                            Revo.Fly.CaptureConnection = nil

                            Revo.Fly:EnableGlobalListener()
                        end
                    elseif input.UserInputType == Enum.UserInputType.MouseButton1
                        or input.UserInputType == Enum.UserInputType.MouseButton2
                        or input.UserInputType == Enum.UserInputType.MouseButton3 then
                        Revo.Fly:CancelKeyCapture()
                    end
                end)
            end)


            Revo.Inputs.WalkSpeed.Position = Revo.Buttons.WalkSpeed.Position
            Revo.Inputs.WalkSpeed.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Inputs.WalkSpeed.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Inputs.WalkSpeed.Font = Enum.Font.Gotham
            Revo.Inputs.WalkSpeed.TextSize = 16
            Revo.Inputs.WalkSpeed.PlaceholderText = "Enter Walk Speed"
            Revo.Inputs.WalkSpeed.Visible = false
            Revo.Inputs.WalkSpeed.ClearTextOnFocus = true
            Revo.Inputs.WalkSpeed.Parent = Revo.GUI.CharacterLeftColumn
            Revo.GUI:AddCorner(Revo.Inputs.WalkSpeed)

        -- Walk Speed Function

            Revo.Buttons.WalkSpeed.MouseButton1Click:Connect(function()
                Revo.Buttons.WalkSpeed.Visible = false
                Revo.Inputs.WalkSpeed.Visible = true
                Revo.Inputs.WalkSpeed:CaptureFocus()
            end)

            Revo.Inputs.WalkSpeed.FocusLost:Connect(function()
                pcall(function()
                    local Speed = Revo.Inputs.WalkSpeed.Text:gsub("%D", "")
                    if Speed == "" then Speed = 16 end

                    Speed = tonumber(Speed)

                    Revo.WalkSpeed = Speed

                    local hum = Revo.Player.Character and Revo.Player.Character:FindFirstChildOfClass("Humanoid")
                    if hum then
                        hum.WalkSpeed = Speed
                    end

                    Revo.Buttons.WalkSpeed.Text = "Walk Speed: " .. Speed

                    Revo.Save.Data.WalkSpeed = Speed
                    Revo.Save:Write()
                end)

                Revo.Inputs.WalkSpeed.Text = ""
                Revo.Inputs.WalkSpeed.Visible = false
                Revo.Buttons.WalkSpeed.Visible = true
            end)

    -- Fly

        -- Fly Function

            function Revo.Fly:Start()
                if self.ThreadRunning then return end
                self.ThreadRunning = true
                self.Enabled = true

                local player = Revo.Player
                local char = player.Character
                if not char then self.ThreadRunning = false return end

                local humanoid = char:FindFirstChildOfClass("Humanoid")
                local root = char:FindFirstChild("HumanoidRootPart")
                if not humanoid or not root then
                    self.ThreadRunning = false
                    return
                end

                local bypassing = false
                if Revo.NoRagdoll and Revo.NoRagdoll.Enabled then
                    Revo.NoRagdoll.Bypass = true
                    bypassing = true
                end

                humanoid.PlatformStand = true

                local BG = Instance.new("BodyGyro")
                BG.P = 9e4
                BG.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
                BG.CFrame = workspace.CurrentCamera.CFrame
                BG.Parent = root

                local BV = Instance.new("BodyVelocity")
                BV.Velocity = Vector3.new(0, 0, 0)
                BV.MaxForce = Vector3.new(9e9, 9e9, 9e9)
                BV.Parent = root

                local SPEED = 0

                while self.Enabled and root.Parent do
                    task.wait()

                    local camera = workspace.CurrentCamera
                    local ctrl = self.Ctrl
                    local lastctrl = self.LastCtrl

                    if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then
                        SPEED = self.Speed
                    elseif SPEED ~= 0 then
                        SPEED = 0
                    end

                    if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then
                        BV.Velocity =
                            ((camera.CFrame.LookVector * (ctrl.f + ctrl.b)) +
                            ((camera.CFrame * CFrame.new(ctrl.l + ctrl.r, (ctrl.f + ctrl.b) * 0.2, 0).p)
                            - camera.CFrame.p)) * SPEED

                        self.LastCtrl = {
                            f = ctrl.f,
                            b = ctrl.b,
                            l = ctrl.l,
                            r = ctrl.r
                        }

                    elseif SPEED ~= 0 then
                        BV.Velocity =
                            ((camera.CFrame.LookVector * (lastctrl.f + lastctrl.b)) +
                            ((camera.CFrame * CFrame.new(lastctrl.l + lastctrl.r, (lastctrl.f + ctrl.b) * 0.2, 0).p)
                            - camera.CFrame.p)) * SPEED
                    else
                        BV.Velocity = Vector3.new(0, 0, 0)
                    end

                    BG.CFrame = camera.CFrame
                end

                BG:Destroy()
                BV:Destroy()
                humanoid.PlatformStand = false

                if bypassing then
                    Revo.NoRagdoll.Bypass = false
                end

                self.ThreadRunning = false
            end

            function Revo.Fly:Stop()
                self.Enabled = false
            end

            function Revo.Fly:ConnectKeys(mouse)
                self.KeyDownConnection = mouse.KeyDown:Connect(function(key)
                    key = key:lower()
                    if key == "w" then self.Ctrl.f = 1
                    elseif key == "s" then self.Ctrl.b = -1
                    elseif key == "a" then self.Ctrl.l = -1
                    elseif key == "d" then self.Ctrl.r = 1
                    end
                end)

                self.KeyUpConnection = mouse.KeyUp:Connect(function(key)
                    key = key:lower()
                    if key == "w" then self.Ctrl.f = 0
                    elseif key == "s" then self.Ctrl.b = 0
                    elseif key == "a" then self.Ctrl.l = 0
                    elseif key == "d" then self.Ctrl.r = 0
                    end
                end)
            end

            function Revo.Fly:DisconnectKeys()
                if self.KeyDownConnection then self.KeyDownConnection:Disconnect() end
                if self.KeyUpConnection then self.KeyUpConnection:Disconnect() end
            end

        -- Fly Speed

            -- Fly Speed Button

                Revo.Fly.Speed = Revo.Save.Data.FlySpeed or 50

                Revo.Buttons.FlySpeed = Instance.new("TextButton")
                Revo.Buttons.FlySpeed.Size = UDim2.new(0, 200, 0, 30)
                Revo.Buttons.FlySpeed.Position = UDim2.new(0, 215, 0, 5)

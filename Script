            Revo.Inputs.GUIKey.PlaceholderText = "Press Any Key"
            Revo.Inputs.GUIKey.Visible = false
            Revo.Inputs.GUIKey.ClearTextOnFocus = true
            Revo.Inputs.GUIKey.Text = ""
            Revo.Inputs.GUIKey.Parent = Revo.GUI.MiscSection
            Revo.GUI:AddCorner(Revo.Inputs.GUIKey)

        -- GUI Key Function

            function Revo.GUI:EnableGlobalListener()
                if self.GlobalConnection then return end

                self.GlobalConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if self.CapturingKey then return end

                    if self.Key and input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == self.Key then
                        self.Enabled = not self.Enabled
                        Revo.GUI.ScreenGui.Enabled = self.Enabled

                        if not self.Enabled then
                            Revo.GUI:ResetWalkOnAirFlags()
                        end
                    end
                end)
            end

            Revo.GUI:EnableGlobalListener()

            function Revo.GUI:CancelKeyCapture()
                self.Key = nil
                Revo.Buttons.GUIKey.Text = "GUI Key: None"

                Revo.Inputs.GUIKey.Visible = false
                Revo.Buttons.GUIKey.Visible = true

                self.CapturingKey = false

                if self.CaptureConnection then
                    self.CaptureConnection:Disconnect()
                    self.CaptureConnection = nil
                end

                self:EnableGlobalListener()
                Revo:UpdateHomeText()
            end

            Revo.Buttons.GUIKey.MouseButton1Click:Connect(function()
                Revo.Buttons.GUIKey.Visible = false
                Revo.Inputs.GUIKey.Visible = true
                Revo.Inputs.GUIKey.Text = "Press Any Key"

                Revo.GUI.CapturingKey = true

                if Revo.GUI.GlobalConnection then
                    Revo.GUI.GlobalConnection:Disconnect()
                    Revo.GUI.GlobalConnection = nil
                end

                if Revo.GUI.CaptureConnection then
                    Revo.GUI.CaptureConnection:Disconnect()
                end

                Revo.GUI.CaptureConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if not Revo.GUI.CapturingKey then return end

                    if input.UserInputType == Enum.UserInputType.Keyboard then
                        if input.KeyCode == Enum.KeyCode.Backspace then
                            Revo.GUI:CancelKeyCapture()
                        else
                            Revo.GUI.Key = input.KeyCode
                            Revo.Buttons.GUIKey.Text = "GUI Key: " .. Revo.GUI.Key.Name
                            Revo:UpdateHomeText()

                            Revo.Inputs.GUIKey.Visible = false
                            Revo.Buttons.GUIKey.Visible = true
                            Revo.GUI.CapturingKey = false

                            Revo.GUI.CaptureConnection:Disconnect()
                            Revo.GUI.CaptureConnection = nil

                            Revo.GUI:EnableGlobalListener()
                        end
                    elseif input.UserInputType == Enum.UserInputType.MouseButton1
                        or input.UserInputType == Enum.UserInputType.MouseButton2
                        or input.UserInputType == Enum.UserInputType.MouseButton3 then
                        Revo.GUI:CancelKeyCapture()
                    end
                end)
            end)

    -- Auto Clicker

        -- Auto Clicker Key

            Revo.Buttons.AutoClickerKey = Instance.new("TextButton")
            Revo.Buttons.AutoClickerKey.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.AutoClickerKey.Position = UDim2.new(0, 215, 0, 180)
            Revo.Buttons.AutoClickerKey.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.AutoClickerKey.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.AutoClickerKey.Font = Enum.Font.Gotham
            Revo.Buttons.AutoClickerKey.TextSize = 16
            Revo.Buttons.AutoClickerKey.Text = "Auto Clicker Key: "..(Revo.AutoClicker.Key and Revo.AutoClicker.Key.Name or "None")
            Revo.Buttons.AutoClickerKey.Parent = Revo.GUI.MiscSection
            Revo.GUI:AddCorner(Revo.Buttons.AutoClickerKey)

            Revo.Inputs = Revo.Inputs or {}
            Revo.Inputs.AutoClickerKey = Instance.new("TextBox")
            Revo.Inputs.AutoClickerKey.Size = Revo.Buttons.AutoClickerKey.Size
            Revo.Inputs.AutoClickerKey.Position = Revo.Buttons.AutoClickerKey.Position
            Revo.Inputs.AutoClickerKey.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Inputs.AutoClickerKey.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Inputs.AutoClickerKey.Font = Enum.Font.Gotham
            Revo.Inputs.AutoClickerKey.TextSize = 16
            Revo.Inputs.AutoClickerKey.PlaceholderText = "Press Any Key"
            Revo.Inputs.AutoClickerKey.Visible = false
            Revo.Inputs.AutoClickerKey.ClearTextOnFocus = true
            Revo.Inputs.AutoClickerKey.Text = ""
            Revo.Inputs.AutoClickerKey.Parent = Revo.GUI.MiscSection
            Revo.GUI:AddCorner(Revo.Inputs.AutoClickerKey)

        -- Auto Clicker Function

            task.spawn(function()
                while true do
                    if Revo.AutoClicker.Enabled then
                        mouse1click()
                    end
                    task.wait()
                end
            end)

        -- Auto Clicker Key Function

            function Revo.AutoClicker:EnableGlobalListener()
                if self.GlobalConnection then return end

                self.GlobalConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if self.CapturingKey then return end

                    if self.Key and input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == self.Key then
                        self.Enabled = not self.Enabled
                        Revo:Notify("Revo", self.Enabled and "Auto Clicker Enabled" or "Auto Clicker Disabled", 5)
                    end
                end)
            end

            Revo.AutoClicker:EnableGlobalListener()

            function Revo.AutoClicker:CancelKeyCapture()
                self.Key = nil
                Revo.Buttons.AutoClickerKey.Text = "Auto Clicker Key: None"

                Revo.Inputs.AutoClickerKey.Visible = false
                Revo.Buttons.AutoClickerKey.Visible = true

                self.CapturingKey = false

                if self.CaptureConnection then
                    self.CaptureConnection:Disconnect()
                    self.CaptureConnection = nil
                end

                self:EnableGlobalListener()
            end

            Revo.Buttons.AutoClickerKey.MouseButton1Click:Connect(function()
                Revo.Buttons.AutoClickerKey.Visible = false
                Revo.Inputs.AutoClickerKey.Visible = true
                Revo.Inputs.AutoClickerKey.Text = "Press Any Key"

                Revo.AutoClicker.CapturingKey = true

                if Revo.AutoClicker.GlobalConnection then
                    Revo.AutoClicker.GlobalConnection:Disconnect()
                    Revo.AutoClicker.GlobalConnection = nil
                end

                if Revo.AutoClicker.CaptureConnection then
                    Revo.AutoClicker.CaptureConnection:Disconnect()
                end

                Revo.AutoClicker.CaptureConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if not Revo.AutoClicker.CapturingKey then return end

                    if input.UserInputType == Enum.UserInputType.Keyboard then
                        if input.KeyCode == Enum.KeyCode.Backspace then
                            Revo.AutoClicker:CancelKeyCapture()
                        else
                            Revo.AutoClicker.Key = input.KeyCode
                            Revo.Buttons.AutoClickerKey.Text = "Auto Clicker Key: " .. input.KeyCode.Name

                            Revo.Inputs.AutoClickerKey.Visible = false
                            Revo.Buttons.AutoClickerKey.Visible = true
                            Revo.AutoClicker.CapturingKey = false

                            Revo.AutoClicker.CaptureConnection:Disconnect()
                            Revo.AutoClicker.CaptureConnection = nil

                            Revo.AutoClicker:EnableGlobalListener()
                        end
                    elseif input.UserInputType.Name:find("MouseButton") then
                        Revo.AutoClicker:CancelKeyCapture()
                    end
                end)
            end)

    -- Anti AFK

        -- Anti AFK Button

            Revo.Buttons.AntiAFK = Instance.new("TextButton")
            Revo.Buttons.AntiAFK.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.AntiAFK.Position = UDim2.new(0, 5, 0, 180)
            Revo.Buttons.AntiAFK.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.AntiAFK.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.AntiAFK.Font = Enum.Font.Gotham
            Revo.Buttons.AntiAFK.TextSize = 16
            Revo.Buttons.AntiAFK.Text = "Anti AFK"
            Revo.Buttons.AntiAFK.Parent = Revo.GUI.MiscSection
            Revo.GUI:AddCorner(Revo.Buttons.AntiAFK)

        -- Anit AFK Function

            function Revo:ToggleAntiAFK()
                Revo.AntiAfk.Enabled = not Revo.AntiAfk.Enabled
                Revo:Notify(
                    "Revo",
                    Revo.AntiAfk.Enabled and "Anti AFK Enabled" or "Anti AFK Disabled",
                    5
                )
            end

            Revo.Buttons.AntiAFK.MouseButton1Click:Connect(function()
                Revo:ToggleAntiAFK()
            end)

            Revo.Player.Idled:Connect(function()
                if Revo.AntiAfk.Enabled then
                    Revo.Services.VirtualUser:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
                    task.wait(1)
                    Revo.Services.VirtualUser:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
                end
            end)

    -- Anti Void

        -- Anti Void Button

            Revo.Buttons.AntiVoid = Instance.new("TextButton")
            Revo.Buttons.AntiVoid.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.AntiVoid.Position = UDim2.new(0, 5, 0, 215)
            Revo.Buttons.AntiVoid.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.AntiVoid.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.AntiVoid.Font = Enum.Font.Gotham
            Revo.Buttons.AntiVoid.TextSize = 16
            Revo.Buttons.AntiVoid.Text = "Anti Void"
            Revo.Buttons.AntiVoid.Parent = Revo.GUI.MiscSection
            Revo.GUI:AddCorner(Revo.Buttons.AntiVoid)

        -- Anti Void Function

            function Revo:AntiVoidCheck()
                if not Revo.AntiVoid.Enabled then return end

                if Revo.Player.Character and Revo.Player.Character:FindFirstChild("HumanoidRootPart") then
                    local root = Revo.Player.Character.HumanoidRootPart
                    if root.Position.Y < Revo.VOID_Y then
                        root.Velocity = Vector3.new(0, 500, 0)
                    end
                end
            end

            game:GetService("RunService").Heartbeat:Connect(function()
                Revo:AntiVoidCheck()
            end)

            Revo.Buttons.AntiVoid.MouseButton1Click:Connect(function()
                Revo.AntiVoid.Enabled = not Revo.AntiVoid.Enabled
                
                if Revo.AntiVoid.Enabled then
                    Revo:Notify("Revo", "Anti Void Enabled", 5)
                else
                    Revo:Notify("Revo", "Anti Void Disabled", 5)
                end
            end)

    -- No Sit

        Revo.Buttons.Nosit = Instance.new("TextButton")
        Revo.Buttons.Nosit.Size = UDim2.new(0, 200, 0, 30)
        Revo.Buttons.Nosit.Position = UDim2.new(0, 215, 0, 215)
        Revo.Buttons.Nosit.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        Revo.Buttons.Nosit.TextColor3 = Color3.fromRGB(255, 255, 255)
        Revo.Buttons.Nosit.Font = Enum.Font.Gotham
        Revo.Buttons.Nosit.TextSize = 16
        Revo.Buttons.Nosit.Text = "No Sit"
        Revo.Buttons.Nosit.Parent = Revo.GUI.MiscSection
        Revo.GUI:AddCorner(Revo.Buttons.Nosit)

    -- No Sit Function

        Revo.Buttons.Nosit.MouseButton1Click:Connect(function()
            Revo.NoSit.Enabled = not Revo.NoSit.Enabled
            
            Revo:Notify(
                "Revo",
                Revo.NoSit.Enabled and "No Sit Enabled" or "No Sit Disabled",
                5
            )
            
            if Revo.NoSit.Enabled then
                local humanoid = Revo.Player.Character and Revo.Player.Character:FindFirstChildWhichIsA("Humanoid")
                if humanoid then
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
                end
                
                Revo.Player.CharacterAdded:Connect(function(character)
                    repeat wait() until character:FindFirstChildWhichIsA("Humanoid")
                    local newHumanoid = character:FindFirstChildWhichIsA("Humanoid")
                    newHumanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
                end)
            else
                local humanoid = Revo.Player.Character and Revo.Player.Character:FindFirstChildWhichIsA("Humanoid")
                if humanoid then
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
                end
            end
        end)

    -- Teleport To Last Location On Respawn

        function Revo:OnCharacterAdded(character)
            local humanoid = character:WaitForChild("Humanoid")
            local root = character:WaitForChild("HumanoidRootPart")

            Revo.Buttons.JumpPower.Text = "Jump Power: 50"
            Revo.Buttons.WalkSpeed.Text = "Walk Speed: 16"

            if Revo.Respawn.LastCFrame then
                task.wait(0.1)
                root.CFrame = Revo.Respawn.LastCFrame
            end

            humanoid.Died:Connect(function()
                if root then
                    if root.Position.Y > Revo.VOID_Y then
                        Revo.Respawn.LastCFrame = root.CFrame
                    else
                        Revo.Respawn.LastCFrame = nil
                    end
                end
            end)
        end

        if Revo.Player.Character then
            Revo:OnCharacterAdded(Revo.Player.Character)
        end

        Revo.Player.CharacterAdded:Connect(function(character)
            Revo:OnCharacterAdded(character)
        end)

    -- Base Plate

        Revo.BasePlate = {}

        Revo.BasePlate.ValidPlaceIds = {
            6884319169,
            292628125,
        }

        Revo.BasePlate.TILE_SIZE = 256 
        Revo.BasePlate.CHUNK_SIZE = 8
        Revo.BasePlate.RENDER_DISTANCE = 2
        Revo.BasePlate.UNLOAD_DISTANCE = 3
        Revo.BasePlate.ChunkFolders = {}
        Revo.BasePlate.LoadedChunks = {}

        function Revo.BasePlate:GetFolder()
            local folder = workspace:FindFirstChild("MicUpBaseplateFolder")
            if not folder then
                folder = Instance.new("Folder")
                folder.Name = "MicUpBaseplateFolder"
                folder.Parent = workspace
            end
            return folder
        end

        function Revo.BasePlate:GenerateChunk(cx, cz)
            local key = cx .. "," .. cz
            if self.LoadedChunks[key] then return end

            self.LoadedChunks[key] = true

            local chunkFolder = Instance.new("Folder")
            chunkFolder.Name = "Chunk_" .. key
            chunkFolder.Parent = self:GetFolder()
            self.ChunkFolders[key] = chunkFolder

            for x = 0, self.CHUNK_SIZE - 1 do
                for z = 0, self.CHUNK_SIZE - 1 do
                    local part = Instance.new("Part")
                    part.Name = "MicUpTile"
                    part.Anchored = true
                    part.CanCollide = true
                    part.Locked = true
                    part.Size = Vector3.new(self.TILE_SIZE, 1, self.TILE_SIZE)

                    part.Position = Vector3.new(
                        (cx * self.CHUNK_SIZE + x) * self.TILE_SIZE,
                        -0.7,
                        (cz * self.CHUNK_SIZE + z) * self.TILE_SIZE
                    )

                    part.Material = Enum.Material.Grass
                    part.Color = Color3.fromRGB(85, 175, 100)
                    part.TopSurface = Enum.SurfaceType.Smooth
                    part.BottomSurface = Enum.SurfaceType.Smooth
                    part.Parent = chunkFolder
                end
            end
        end

        function Revo.BasePlate:UnloadFarChunks(cx, cz)
            for key in pairs(self.LoadedChunks) do
                local x, z = key:match("([^,]+),([^,]+)")
                x = tonumber(x)
                z = tonumber(z)

                if math.abs(x - cx) > self.UNLOAD_DISTANCE
                or math.abs(z - cz) > self.UNLOAD_DISTANCE then

                    if self.ChunkFolders[key] then
                        self.ChunkFolders[key]:Destroy()
                        self.ChunkFolders[key] = nil
                    end

                    self.LoadedChunks[key] = nil
                end
            end
        end

        function Revo.BasePlate:Update()
            if not game.Players.LocalPlayer.Character then return end
            if not game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then return end

            local pos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position

            local cx = math.floor(pos.X / (self.TILE_SIZE * self.CHUNK_SIZE))
            local cz = math.floor(pos.Z / (self.TILE_SIZE * self.CHUNK_SIZE))

            for x = -self.RENDER_DISTANCE, self.RENDER_DISTANCE do
                for z = -self.RENDER_DISTANCE, self.RENDER_DISTANCE do
                    self:GenerateChunk(cx + x, cz + z)
                end
            end

            self:UnloadFarChunks(cx, cz)
        end

        if table.find(Revo.BasePlate.ValidPlaceIds, game.PlaceId) then
            game:GetService("RunService").Heartbeat:Connect(function()
                Revo.BasePlate:Update()
            end)
        end


-- Loading Finish

    Revo.GUI.ScreenGui.Parent = Revo.PlayerGui
    Revo.GUI.MainFrame.Visible = true

    -- Explode

        -- Explode Button

            Revo.Buttons.Explode = Instance.new("TextButton")
            Revo.Buttons.Explode.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.Explode.Position = UDim2.new(0, 5, 0, 145)
            Revo.Buttons.Explode.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.Explode.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.Explode.Font = Enum.Font.Gotham
            Revo.Buttons.Explode.TextSize = 16
            Revo.Buttons.Explode.Text = "Explode"
            Revo.Buttons.Explode.Parent = Revo.GUI.MiscLeftColumn
            Revo.GUI:AddCorner(Revo.Buttons.Explode)

        -- Explode Function

            Revo.Buttons.Explode.MouseButton1Click:Connect(function()

                local char = Revo.Player.Character
                if not char then return end

                local hum = char:FindFirstChildOfClass("Humanoid")
                local root = Revo:GetRoot(Revo.Player)
                if not hum or not root then return end

                local bypassing = false
                if Revo.NoRagdoll and Revo.NoRagdoll.Enabled then
                    Revo.NoRagdoll.Bypass = true
                    bypassing = true
                end

                hum:ChangeState(Enum.HumanoidStateType.Physics)

                local oldSpin = root:FindFirstChild("Spin")
                if oldSpin then oldSpin:Destroy() end

                local spin = Instance.new("BodyAngularVelocity")
                spin.Name = "Spin"
                spin.MaxTorque = Vector3.new(0, math.huge, 0)
                spin.AngularVelocity = Vector3.new(0, 150, 0)
                spin.Parent = root

                task.delay(3, function()

                    if spin then
                        spin:Destroy()
                    end

                    if hum then
                        hum.Health = 0
                    end

                    if hum then
                        hum:ChangeState(Enum.HumanoidStateType.GettingUp)
                    end

                    if bypassing then
                        Revo.NoRagdoll.Bypass = false
                    end
                end)

            end)

    -- GUI Key

        -- GUI Key

            if Revo.Save.Data.GUIKey then
                Revo.GUI.Key = Enum.KeyCode[Revo.Save.Data.GUIKey]
            end

            Revo.Buttons.GUIKey = Instance.new("TextButton")
            Revo.Buttons.GUIKey.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.GUIKey.Position = UDim2.new(0, 215, 0, 145)
            Revo.Buttons.GUIKey.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.GUIKey.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.GUIKey.Font = Enum.Font.Gotham
            Revo.Buttons.GUIKey.TextSize = 16
            Revo.Buttons.GUIKey.Text = "GUI Key: " .. (Revo.GUI.Key and Revo.GUI.Key.Name or "None")
            Revo.Buttons.GUIKey.Parent = Revo.GUI.MiscRightColumn
            Revo.GUI:AddCorner(Revo.Buttons.GUIKey)

            Revo:UpdateHomeText()

            Revo.Inputs = Revo.Inputs or {}
            Revo.Inputs.GUIKey = Instance.new("TextBox")
            Revo.Inputs.GUIKey.Size = Revo.Buttons.GUIKey.Size
            Revo.Inputs.GUIKey.Position = Revo.Buttons.GUIKey.Position
            Revo.Inputs.GUIKey.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Inputs.GUIKey.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Inputs.GUIKey.Font = Enum.Font.Gotham
            Revo.Inputs.GUIKey.TextSize = 16
            Revo.Inputs.GUIKey.PlaceholderText = "Press Any Key"
            Revo.Inputs.GUIKey.Visible = false
            Revo.Inputs.GUIKey.ClearTextOnFocus = true
            Revo.Inputs.GUIKey.Text = ""
            Revo.Inputs.GUIKey.Parent = Revo.GUI.MiscRightColumn
            Revo.GUI:AddCorner(Revo.Inputs.GUIKey)

        -- GUI Key Function

            function Revo.GUI:EnableGlobalListener()
                if self.GlobalConnection then return end

                self.GlobalConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if self.CapturingKey then return end

                    if self.Key and input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == self.Key then
                        self.Enabled = not self.Enabled
                        Revo.GUI.ScreenGui.Enabled = self.Enabled

                        if not self.Enabled then
                            Revo.GUI:ResetWalkOnAirFlags()
                        end
                    end
                end)
            end

            Revo.GUI:EnableGlobalListener()

            function Revo.GUI:CancelKeyCapture()
                self.Key = nil
                Revo.Buttons.GUIKey.Text = "GUI Key: None"

                Revo.Inputs.GUIKey.Visible = false
                Revo.Buttons.GUIKey.Visible = true
                self.CapturingKey = false

                if self.CaptureConnection then
                    self.CaptureConnection:Disconnect()
                    self.CaptureConnection = nil
                end

                Revo.Save.Data.GUIKey = nil
                if Revo.Save.Enabled then
                    Revo.Save:Write()
                end

                self:EnableGlobalListener()
                Revo:UpdateHomeText()
            end

            Revo.Buttons.GUIKey.MouseButton1Click:Connect(function()
                Revo.Buttons.GUIKey.Visible = false
                Revo.Inputs.GUIKey.Visible = true
                Revo.Inputs.GUIKey.Text = "Press Any Key"

                Revo.GUI.CapturingKey = true

                if Revo.GUI.GlobalConnection then
                    Revo.GUI.GlobalConnection:Disconnect()
                    Revo.GUI.GlobalConnection = nil
                end

                if Revo.GUI.CaptureConnection then
                    Revo.GUI.CaptureConnection:Disconnect()
                end

                Revo.GUI.CaptureConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if not Revo.GUI.CapturingKey then return end

                    if input.UserInputType == Enum.UserInputType.Keyboard then
                        if input.KeyCode == Enum.KeyCode.Backspace then
                            Revo.GUI:CancelKeyCapture()
                        else
                            Revo.GUI.Key = input.KeyCode
                            Revo.Buttons.GUIKey.Text = "GUI Key: " .. Revo.GUI.Key.Name
                            Revo:UpdateHomeText()

                            if Revo.Save.Enabled then
                                Revo.Save.Data.GUIKey = Revo.GUI.Key.Name
                                Revo.Save:Write()
                            end

                            Revo.Inputs.GUIKey.Visible = false
                            Revo.Buttons.GUIKey.Visible = true
                            Revo.GUI.CapturingKey = false

                            Revo.GUI.CaptureConnection:Disconnect()
                            Revo.GUI.CaptureConnection = nil

                            Revo.GUI:EnableGlobalListener()
                        end
                    elseif input.UserInputType == Enum.UserInputType.MouseButton1
                        or input.UserInputType == Enum.UserInputType.MouseButton2
                        or input.UserInputType == Enum.UserInputType.MouseButton3 then
                        Revo.GUI:CancelKeyCapture()
                    end
                end)
            end)

    -- Auto Clicker

        -- Auto Clicker Key

            if Revo.Save.Data.AutoClickerKey then
                Revo.AutoClicker.Key = Enum.KeyCode[Revo.Save.Data.AutoClickerKey]
            end


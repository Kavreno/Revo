            local function enableGlobalTripListener()
                if globalTripConnection then return end

                globalTripConnection = UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if capturingTripKey then return end

                    if TripKey
                    and input.UserInputType == Enum.UserInputType.Keyboard
                    and input.KeyCode == TripKey then
                        tripPlayer()
                    end
                end)
            end

            enableGlobalTripListener()


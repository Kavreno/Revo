-- Core Buttons

    Revo.ButtonBar = Instance.new("Frame")
    Revo.ButtonBar.Size = UDim2.new(1, 0, 0, 28)
    Revo.ButtonBar.Position = UDim2.new(0, 0, 0, 45)
    Revo.ButtonBar.BackgroundTransparency = 1
    Revo.ButtonBar.Parent = Revo.GUI.MainFrame

    Revo.UIList = Instance.new("UIListLayout")
    Revo.UIList.FillDirection = Enum.FillDirection.Horizontal
    Revo.UIList.Padding = UDim.new(0, 4)
    Revo.UIList.HorizontalAlignment = Enum.HorizontalAlignment.Center
    Revo.UIList.VerticalAlignment = Enum.VerticalAlignment.Center
    Revo.UIList.Parent = Revo.ButtonBar

    function Revo:CreateButton(name)
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(0, 80, 1, 0)
        btn.Text = name
        btn.BackgroundColor3 = Color3.fromRGB(50,50,50)
        btn.TextColor3 = Color3.fromRGB(255,255,255)
        btn.Font = Enum.Font.Gotham
        btn.TextSize = 14
        btn.Parent = self.ButtonBar
        self.GUI:AddCorner(btn)

        btn.MouseButton1Click:Connect(function()
            for _, page in pairs(self.Pages) do
                page.Visible = false
            end

            local target = self.Pages[name]
            if target then
                target.Visible = true
            end

            for _, b in ipairs(self.ButtonBar:GetChildren()) do
                if b:IsA("TextButton") then
                    b.Visible = true
                end
            end

        end)

        return btn
    end

    Revo.Buttons = {}
    Revo.Inputs = {}

    Revo.Buttons.Home = Revo:CreateButton("Home")
    Revo.Buttons.Character = Revo:CreateButton("Character")
    Revo.Buttons.Target = Revo:CreateButton("Target")
    Revo.Buttons.Animations = Revo:CreateButton("Animations")
    Revo.Buttons.Misc = Revo:CreateButton("Misc")

-- Title Bar

    Revo.GUI.TitleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            Revo.GUI.Dragging = true
            Revo.GUI.DragStart = input.Position
            Revo.GUI.StartPos = Revo.GUI.MainFrame.Position

            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    Revo.GUI.Dragging = false
                end
            end)
        end
    end)

    Revo.GUI.TitleBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            if Revo.GUI.Dragging then
                local delta = input.Position - Revo.GUI.DragStart
                Revo.GUI.MainFrame.Position = UDim2.new(
                    Revo.GUI.StartPos.X.Scale,
                    Revo.GUI.StartPos.X.Offset + delta.X,
                    Revo.GUI.StartPos.Y.Scale,
                    Revo.GUI.StartPos.Y.Offset + delta.Y
                )
            end
        end
    end)


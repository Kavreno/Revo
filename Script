            Revo.Buttons.WalkOnAirUp = Instance.new("TextButton")
            Revo.Buttons.WalkOnAirUp.Size = UDim2.new(0, 65, 0, 30)
            Revo.Buttons.WalkOnAirUp.Position = UDim2.new(0, 140, 0, 110)
            Revo.Buttons.WalkOnAirUp.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.WalkOnAirUp.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.WalkOnAirUp.Font = Enum.Font.Gotham

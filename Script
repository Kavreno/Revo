    -- Jerk

        -- Jerk Button Button
        
            local JerkButton = Instance.new("TextButton")
            JerkButton.Size = UDim2.new(0, 200, 0, 30)
            JerkButton.Position = UDim2.new(0, 5, 0, 75)
            JerkButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)

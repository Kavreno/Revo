        local TargetName_Input = Instance.new("TextBox")
        TargetName_Input.Name = "TargetName_Input"
        TargetName_Input.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
        TargetName_Input.BackgroundTransparency = 0.5
        TargetName_Input.BorderColor3 = Color3.fromRGB(100, 100, 100)
        TargetName_Input.Position = UDim2.new(0, 110, 0, 10)
        TargetName_Input.Size = UDim2.new(0, 200, 0, 30)
        TargetName_Input.Font = Enum.Font.Gotham
        TargetName_Input.PlaceholderText = "Target"
        TargetName_Input.Text = ""
        TargetName_Input.TextColor3 = Color3.fromRGB(255, 255, 255)
        TargetName_Input.TextSize = 14
        TargetName_Input.ClearTextOnFocus = true
        TargetName_Input.Parent = Target_Section
        AddCorner(TargetName_Input)

    -- Update Target
    
        local function UpdateTarget(player)
            if (player ~= nil and player ~= "" and player ~= Players.LocalPlayer) then
                TargetedPlayer = player.Name
                TargetName_Input.Text = player.Name
                UserIDTargetLabel.Text = ("UserID: "..player.UserId.."\nDisplay: "..player.DisplayName.."\nJoined: "..os.date("%d-%m-%Y", os.time()-player.AccountAge * 24 * 3600).." [Day/Month/Year]")
                TargetImage.Image = Players:GetUserThumbnailAsync(player.UserId,Enum.ThumbnailType.HeadShot,Enum.ThumbnailSize.Size420x420)
            else
                TargetName_Input.Text = "Target"
                UserIDTargetLabel.Text = "UserID: \nDisplay: \nJoined: "
                TargetImage.Image = "rbxassetid://10818605405"
                TargetedPlayer = nil
            end
        end

    -- Find Player

        local function GetPlayer(UserDisplay)
            if UserDisplay ~= "" then
                for i,v in pairs(Players:GetPlayers()) do
                    if v.Name:lower():match(UserDisplay) or v.DisplayName:lower():match(UserDisplay) then
                        return v
                    end
                end
                return nil
            else
                return nil
            end
        end

        TargetName_Input.FocusLost:Connect(function()
            local text = TargetName_Input.Text:gsub("@", "")

            if text == "" then
                UpdateTarget(nil)
                return
            end

            local player = GetPlayer(text)

            if player and player ~= Players.LocalPlayer then
                UpdateTarget(player)
                return
            end

            UpdateTarget(nil)
        end)

    -- Target Tool

        -- Target Tool Button

            local ClickTargetTool_Button = Instance.new("ImageButton")
            ClickTargetTool_Button.Name = "ClickTargetTool_Button"
            ClickTargetTool_Button.BackgroundTransparency = 1
            ClickTargetTool_Button.Position = UDim2.new(1, 5, 0, 0)
            ClickTargetTool_Button.Size = UDim2.new(0, 30, 0, 30)
            ClickTargetTool_Button.Image = "rbxassetid://2716591855"
            ClickTargetTool_Button.Parent = TargetName_Input

        -- Target Tool Function

            ClickTargetTool_Button.MouseButton1Click:Connect(function()
                local GetTargetTool = Instance.new("Tool")
                GetTargetTool.Name = "ClickTarget"
                GetTargetTool.RequiresHandle = false
                GetTargetTool.TextureId = "rbxassetid://2716591855"
                GetTargetTool.ToolTip = "Select Target"

                local function ActivateTool()
                    local root = GetRoot(player)
                    local hit = mouse.Target
                    local person = nil
                    if hit and hit.Parent then
                        if hit.Parent:IsA("Model") then
                            person = game.Players:GetPlayerFromCharacter(hit.Parent)
                        elseif hit.Parent:IsA("Accessory") then
                            person = game.Players:GetPlayerFromCharacter(hit.Parent.Parent)
                        end
                        if person then
                            UpdateTarget(person)
                        end
                    end
                end

                GetTargetTool.Activated:Connect(function()
                    ActivateTool()
                end)
                GetTargetTool.Parent = player.Backpack
            end)

    -- View Target

        ViewTarget_Button.MouseButton1Click:Connect(function()
            if not TargetedPlayer then return end
            viewingTarget = not viewingTarget

            if viewingTarget then
            while viewingTarget do
                    pcall(function()
                        game.Workspace.CurrentCamera.CameraSubject = Players[TargetedPlayer].Character.Humanoid
                    end)
                    task.wait(0.2)
                end
            else
                game.Workspace.CurrentCamera.CameraSubject = player.Character.Humanoid
            end
        end)

    -- Focus Target

        FocusTarget_Button.MouseButton1Click:Connect(function()
            if not TargetedPlayer then return end

            focusingTarget = not focusingTarget

            if focusingTarget then
                task.spawn(function()
                    while focusingTarget do
                        local targetPlayer = Players:FindFirstChild(TargetedPlayer)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            TeleportMeToPlayer(targetPlayer, "safe")
                        end
                        task.wait(0.2)
                    end
                end)
            end
        end)

    -- Teleport Target

        TeleportTarget_Button.MouseButton1Click:Connect(function()
            if not TargetedPlayer then return end

            local target = Players:FindFirstChild(TargetedPlayer)
            if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
                TeleportMeToPlayer(target, "safe")
            end
        end)

    -- Headsit Target

        HeadsitTarget_Button.MouseButton1Click:Connect(function()
            if TargetedPlayer then
                headsittingTarget = not headsittingTarget
                if not headsittingTarget and GetRoot(player):FindFirstChild("BreakVelocity") then
                    GetRoot(player).BreakVelocity:Destroy()
                    return
                end

                task.spawn(function()
                    while headsittingTarget do
                        pcall(function()
                            local root = GetRoot(player)

                            if not root:FindFirstChild("BreakVelocity") then
                                local tempV = Velocity_Asset:Clone()
                                tempV.Parent = root
                            end

                            local targetHead = Players[TargetedPlayer].Character.Head
                            player.Character.Humanoid.Sit = true
                            root.CFrame = targetHead.CFrame * CFrame.new(0, 2, 0)
                            root.Velocity = Vector3.new(0, 0, 0)
                        end)
                        task.wait()
                    end
                end)
            end
        end)

    -- Backpack Target

        BackpackTarget_Button.MouseButton1Click:Connect(function()
            if TargetedPlayer then
                backpackingTarget = not backpackingTarget

                if not backpackingTarget and GetRoot(player):FindFirstChild("BreakVelocity") then
                    GetRoot(player).BreakVelocity:Destroy()
                    return
                end

                task.spawn(function()
                    while backpackingTarget do
                        pcall(function()
                            local playerRoot = GetRoot(player)
                            local targetRoot = GetRoot(Players[TargetedPlayer])

                            if not playerRoot:FindFirstChild("BreakVelocity") then
                                local tempV = Velocity_Asset:Clone()
                                tempV.Parent = playerRoot
                            end

                            player.Character.Humanoid.Sit = true
                            playerRoot.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 1.2) * CFrame.Angles(0, -3, 0)
                            playerRoot.Velocity = Vector3.new(0, 0, 0)
                        end)
                        task.wait()
                    end
                end)
            end
        end)

    -- Stand Target

        StandTarget_Button.MouseButton1Click:Connect(function()
            if TargetedPlayer then
                standingTarget = not standingTarget

                if not standingTarget and GetRoot(player):FindFirstChild("BreakVelocity") then
                    StopAnim()
                    GetRoot(player).BreakVelocity:Destroy()
                    return
                end

                task.spawn(function()
                    PlayAnim(13823324057, 4, 0)

                    while standingTarget do
                        pcall(function()
                            local playerRoot = GetRoot(player)
                            local targetRoot = GetRoot(Players[TargetedPlayer])

                            if not playerRoot:FindFirstChild("BreakVelocity") then
                                local tempV = Velocity_Asset:Clone()
                                tempV.Parent = playerRoot
                            end

                            playerRoot.CFrame = targetRoot.CFrame * CFrame.new(-3, 1, 0)
                            playerRoot.Velocity = Vector3.new(0, 0, 0)
                        end)
                        task.wait()
                    end

                    StopAnim()
                    if GetRoot(player):FindFirstChild("BreakVelocity") then
                        GetRoot(player).BreakVelocity:Destroy()
                    end
                end)
            end
        end)

    -- Doggy Target



    -- Drag Target



    -- Bang Target

        BangTarget_Button.MouseButton1Click:Connect(function()
            if TargetedPlayer then
                BangTarget = not BangTarget

                if not BangTarget and GetRoot(player):FindFirstChild("BreakVelocity") then
                    StopAnim()
                    GetRoot(player).BreakVelocity:Destroy()
                    return
                end

                task.spawn(function()
                    PlayAnim(5918726674, 0, 1)

                    while BangTarget do
                        pcall(function()
                            local playerRoot = GetRoot(player)
                            local targetRoot = GetRoot(Players[TargetedPlayer])

                            if not playerRoot:FindFirstChild("BreakVelocity") then
                                local tempV = Velocity_Asset:Clone()
                                tempV.Parent = playerRoot
                            end

                            playerRoot.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 1.1)
                            playerRoot.Velocity = Vector3.new(0, 0, 0)
                        end)
                        task.wait()
                    end

                    StopAnim()
                    if GetRoot(player):FindFirstChild("BreakVelocity") then
                        GetRoot(player).BreakVelocity:Destroy()
                    end
                end)
            end
        end)


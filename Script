    local function CreateAnimButton(column, name, animNumber)
        local Btn = Instance.new("TextButton")
        Btn.Size = UDim2.new(0, 200, 0, 30)
        Btn.Text = name
        Btn.BackgroundColor3 = Color3.fromRGB(50,50,50)
        Btn.TextColor3 = Color3.fromRGB(255,255,255)
        Btn.Font = Enum.Font.Gotham
        Btn.TextSize = 16
        Btn.Parent = column
        AddCorner(Btn)

        Btn.MouseButton1Click:Connect(function()
            local Animate = player.Character.Animate
            Animate.Disabled = true
            StopAnim()
            Animate.idle.Animation1.AnimationId = AnimationTable[animNumber][1]
            Animate.idle.Animation2.AnimationId = AnimationTable[animNumber][2]
            Animate.walk.WalkAnim.AnimationId = AnimationTable[animNumber][3]
            Animate.run.RunAnim.AnimationId = AnimationTable[animNumber][4]
            Animate.jump.JumpAnim.AnimationId = AnimationTable[animNumber][5]
            Animate.climb.ClimbAnim.AnimationId = AnimationTable[animNumber][6]
            Animate.fall.FallAnim.AnimationId = AnimationTable[animNumber][7]
            player.Character.Humanoid:ChangeState(3)
            Animate.Disabled = false
        end)

        return Btn
    end

    local UseLeft = true
    local function AddAnimButton(name, animNumber)
        local column = UseLeft and LeftColumn or RightColumn
        CreateAnimButton(column, name, animNumber)
        UseLeft = not UseLeft
    end


            RewindKey_Input.ClearTextOnFocus = true
            RewindKey_Input.Text = ""
            RewindKey_Input.Parent = CharacterPage
            AddCorner(RewindKey_Input)

        -- Rewind Function

           RunService.Heartbeat:Connect(function()
                if not recordingEnabled then return end
                local now = os.clock()
                if now - lastRecordTime < RECORD_INTERVAL then return end
                lastRecordTime = now

                local char = player.Character
                if not char then return end
                local hrp = char:FindFirstChild("HumanoidRootPart")
                if not hrp then return end

                rewindHistory[writeIndex] = hrp.CFrame
                writeIndex = writeIndex + 1
                if writeIndex > MAX_FRAMES then writeIndex = 1 end
                storedFrames = math.min(storedFrames + 1, MAX_FRAMES)
            end)

            local function toggleRewind()
                rewinding = not rewinding
                recordingEnabled = not rewinding

                if not rewinding then return end

                task.spawn(function()
                    local char = player.Character
                    if not char then return end
                    local hrp = char:FindFirstChild("HumanoidRootPart")
                    if not hrp then return end

                    local lastRewindTime = 0

                    while rewinding and storedFrames > 0 do
                        local now = os.clock()
                        if now - lastRewindTime >= REWIND_SPEED then
                            lastRewindTime = now

                            local prevIndex = writeIndex - 1
                            if prevIndex < 1 then prevIndex = MAX_FRAMES end

                            local cf1 = rewindHistory[prevIndex]
                            local cf2 = rewindHistory[writeIndex]
                            if cf1 and cf2 then
                                hrp.CFrame = lerpCFrame(cf2, cf1, 0.5)
                            end

                            writeIndex = writeIndex - 1
                            if writeIndex < 1 then writeIndex = MAX_FRAMES end
                            if storedFrames > 0 then storedFrames = storedFrames - 1 end
                        end
                        RunService.Heartbeat:Wait()
                    end

                    rewinding = false
                    recordingEnabled = true
                end)
            end

        -- Rewind Key Function

            local function enableGlobalRewindListener()
                if globalRewindConnection then return end

                globalRewindConnection = UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if capturingRewindKey then return end

                    if RewindToggleKey
                    and input.UserInputType == Enum.UserInputType.Keyboard
                    and input.KeyCode == RewindToggleKey then
                        toggleRewind()
                    end
                end)
            end

            enableGlobalRewindListener()       
            

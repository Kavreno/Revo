        -- Jump Power Function

            Revo.Buttons.JumpPower.MouseButton1Click:Connect(function()
                Revo.Buttons.JumpPower.Visible = false
                Revo.Inputs.JumpPower.Visible = true
                Revo.Inputs.JumpPower:CaptureFocus()
            end)

            Revo.Inputs.JumpPower.FocusLost:Connect(function()
                pcall(function()
                    local Jump = Revo.Inputs.JumpPower.Text:gsub("%D", "")
                    if Jump == "" then Jump = 50 end

                    Jump = tonumber(Jump)

                    Revo.JumpPower = Jump

                    local hum = Revo.Player.Character and Revo.Player.Character:FindFirstChildOfClass("Humanoid")
                    if hum then
                        hum.UseJumpPower = true
                        hum.JumpPower = Jump
                    end

                    Revo.Buttons.JumpPower.Text = "Jump Power: " .. Jump

                    Revo.Save.Data.JumpPower = Jump
                    Revo.Save:Write()
                end)

                Revo.Inputs.JumpPower.Text = ""
                Revo.Inputs.JumpPower.Visible = false
                Revo.Buttons.JumpPower.Visible = true
            end)

    -- Jerk

        -- Jerk Button

            Revo.Buttons.Jerk = Instance.new("TextButton")
            Revo.Buttons.Jerk.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.Jerk.Position = UDim2.new(0, 5, 0, 75)
            Revo.Buttons.Jerk.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.Jerk.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.Jerk.Font = Enum.Font.Gotham
            Revo.Buttons.Jerk.TextSize = 16
            Revo.Buttons.Jerk.Text = "Jerk"
            Revo.Buttons.Jerk.Parent = Revo.GUI.CharacterLeftColumn
            Revo.GUI:AddCorner(Revo.Buttons.Jerk)

        -- Jerk Function

            function Revo:runJerk()
                local character = self.Player.Character or self.Player.CharacterAdded:Wait()
                local humanoid = character:WaitForChild("Humanoid")
                local backpack = self.Player:WaitForChild("Backpack")

                if backpack:FindFirstChild("Jerk Off") then return end

                local tool = Instance.new("Tool")
                tool.Name = "Jerk Off"
                tool.ToolTip = ";)"
                tool.RequiresHandle = false
                tool.Parent = backpack

                local jorkin = false
                local track

                local function stopTomfoolery()
                    jorkin = false
                    if track then
                        track:Stop()
                        track = nil
                    end
                end

                tool.Equipped:Connect(function()
                    jorkin = true
                end)

                tool.Unequipped:Connect(stopTomfoolery)
                humanoid.Died:Connect(stopTomfoolery)

                task.spawn(function()
                    while task.wait() do
                        if jorkin then
                            local isR15 = humanoid.RigType == Enum.HumanoidRigType.R15

                            if not track then
                                local anim = Instance.new("Animation")
                                anim.AnimationId = isR15
                                    and "rbxassetid://698251653"
                                    or "rbxassetid://72042024"

                                track = humanoid:LoadAnimation(anim)
                            end

                            track:Play()
                            track:AdjustSpeed(isR15 and 0.7 or 0.65)
                            track.TimePosition = 0.6

                            task.wait(0.1)
                            while track and track.TimePosition < (isR15 and 0.7 or 0.65) do
                                task.wait(0.1)
                            end

                            if track then
                                track:Stop()
                                track = nil
                            end
                        end
                    end
                end)
            end

            Revo.Buttons.Jerk.MouseButton1Click:Connect(function()
                Revo:runJerk()
            end)

    -- Walk On Air

        -- Walk On Air Button

            Revo.GUI.WalkOnAirFrame = Instance.new("Frame")
            Revo.GUI.WalkOnAirFrame.Size = UDim2.new(1, 0, 0, 30)
            Revo.GUI.WalkOnAirFrame.BackgroundTransparency = 1

-- Functions

    -- Create Menu Pages
    
        local function CreatePage(name)
            local Page = Instance.new("Frame")
            Page.Size = UDim2.new(1, 0, 1, -42)
            Page.Position = UDim2.new(0, 0, 0, 42)
            Page.BackgroundTransparency = 1
            Page.Parent = MainFrame
            Page.Visible = false
            Pages[name] = Page
            return Page
        end

    -- Sends Notification To User

        local function Notify(title, text, duration)
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = title,
                Text = text,
                Duration = duration or 5
            })
        end

    -- Get Root

        local function GetRoot(player)
            if player.Character then
                return player.Character:FindFirstChild("HumanoidRootPart")
            end
            return nil
        end

    -- Add Commands

        Commands = {}
        Aliases = {}

        function addcmd(name, data)
            Commands[name] = data

            if data.Aliases then
                for _, a in ipairs(data.Aliases) do
                    Aliases[a] = name
                end
            end
        end

    -- Run Commands

        function runCommand(text)
            local split = string.split(text, " ")
            local cmdName = split[1]:lower()
            table.remove(split, 1)

            cmdName = Aliases[cmdName] or cmdName
            local cmd = Commands[cmdName]

            if not cmd then
                Notify("Revo", "Unknown Command: "..cmdName, 3)
                return
            end

            cmd.Run(split)
        end

    -- Bind Button

        function BindButton(button, cmdName)
            button.MouseButton1Click:Connect(function()
                runCommand(cmdName)
            end)
        end

    -- Get Ping

        local function GetPing()
            return (game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue())/1000
        end

    -- TeleportTO

        local function TeleportToPosition(player, position, method)
            local root = GetRoot(player)
            if not root then return end

            local function apply()
                root.Velocity = Vector3.zero
                root.AssemblyLinearVelocity = Vector3.zero
                root.CFrame = CFrame.new(position)
            end

            if method == "safe" then
                task.spawn(function()
                    for _ = 1, 25 do
                        task.wait()
                        apply()
                    end
                end)
            else
                apply()
            end
        end

        local function TeleportMeToPlayer(targetPlayer, method)
            if not targetPlayer then return end
            local targetRoot = GetRoot(targetPlayer)
            if not targetRoot then return end

            TeleportToPosition(player, targetRoot.Position + Vector3.new(0, 2, 0), method)
        end

    -- Toggle Ragdoll

        local function ToggleRagdoll(bool)
            pcall(function()
                player.Character["Falling down"].Disabled = bool
                player.Character["Swimming"].Disabled = bool
                player.Character["StartRagdoll"].Disabled = bool
                player.Character["Pushed"].Disabled = bool
                player.Character["RagdollMe"].Disabled = bool
            end)
        end

    -- Velocity Asset

        local Velocity_Asset = Instance.new("BodyAngularVelocity")
        Velocity_Asset.AngularVelocity = Vector3.new(0,0,0)
        Velocity_Asset.MaxTorque = Vector3.new(50000,50000,50000)
        Velocity_Asset.P = 1250
        Velocity_Asset.Name = "BreakVelocity"

    -- Stop Animations

        local function StopAnim()
            player.Character.Animate.Disabled = false
            local animtrack = player.Character.Humanoid:GetPlayingAnimationTracks()
            for i, track in pairs (animtrack) do
                track:Stop()
            end
        end

    -- Play Animations

        local function PlayAnim(id,time,speed)
            pcall(function()
                player.Character.Animate.Disabled = false
                local hum = player.Character.Humanoid
                local animtrack = hum:GetPlayingAnimationTracks()
                for i,track in pairs(animtrack) do
                    track:Stop()
                end
                player.Character.Animate.Disabled = true
                local Anim = Instance.new("Animation")
                Anim.AnimationId = "rbxassetid://"..id
                local loadanim = hum:LoadAnimation(Anim)
                loadanim:Play()
                loadanim.TimePosition = time
                loadanim:AdjustSpeed(speed)
                loadanim.Stopped:Connect(function()
                    player.Character.Animate.Disabled = false
                    for i, track in pairs (animtrack) do
                        track:Stop()
                    end
                end)
            end)
        end

    -- Prediction Teleport

        local function PredictionTP(player,method)
            local root = GetRoot(player)
            local pos = root.Position
            local vel = root.Velocity
            GetRoot(player).CFrame = CFrame.new((pos.X)+(vel.X)*(GetPing()*3.5),(pos.Y)+(vel.Y)*(GetPing()*2),(pos.Z)+(vel.Z)*(GetPing()*3.5))
            if method == "safe" then
                task.wait()
                GetRoot(player).CFrame = CFrame.new(pos)
                task.wait()
                GetRoot(player).CFrame = CFrame.new((pos.X)+(vel.X)*(GetPing()*3.5),(pos.Y)+(vel.Y)*(GetPing()*2),(pos.Z)+(vel.Z)*(GetPing()*3.5))
            end
        end

    -- Get Push

        local function GetPush()
            local TempPush = nil
            pcall(function()
                if player.Backpack:FindFirstChild("Push") then
                    PushTool = player.Backpack.Push
                    PushTool.Parent = player.Character
                    TempPush = PushTool
                end
                for i,v in pairs(Players:GetPlayers()) do
                    if v.Character:FindFirstChild("Push") then
                        TempPush = v.Character.Push
                    end
                end
            end)
            return TempPush
        end

    -- Push

        local function Push(Target)
            local Push = GetPush()
            local FixTool = nil
            if Push ~= nil then
                local args = {[1] = Target.Character}
                GetPush().PushTool:FireServer(unpack(args))
            end
            if player.Character:FindFirstChild("Push") then
                player.Character.Push.Parent = player.Backpack
            end
            if player.Character:FindFirstChild("ModdedPush") then
                FixTool = player.Character:FindFirstChild("ModdedPush")
                FixTool.Parent = player.Backpack
                FixTool.Parent = player.Character
            end
            if player.Character:FindFirstChild("ClickTarget") then
                FixTool = player.Character:FindFirstChild("ClickTarget")
                FixTool.Parent = player.Backpack
                FixTool.Parent = player.Character
            end
            if player.Character:FindFirstChild("potion") then
                FixTool = player.Character:FindFirstChild("potion")
                FixTool.Parent = player.Backpack
                FixTool.Parent = player.Character
            end
        end

    -- Lerp

        local function lerpCFrame(a, b, t)
            return a:Lerp(b, t)
        end


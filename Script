        -- Teleport Function

            function Revo.Teleport:TeleportToMouse()
                local char = Revo.Player.Character
                if not char then return end

                local rootPart = Revo:GetRoot(Revo.Player)
                if not rootPart then return end

                local humanoid = char:FindFirstChildOfClass("Humanoid")
                if not humanoid then return end

                local hitPos = Revo.Player:GetMouse().Hit.Position
                local targetPos = Vector3.new(hitPos.X, hitPos.Y + humanoid.HipHeight, hitPos.Z)
                local lookDirection = (Vector3.new(hitPos.X, rootPart.Position.Y, hitPos.Z) - rootPart.Position).Unit

                local uprightCFrame = CFrame.new(targetPos, targetPos + lookDirection)
                uprightCFrame = CFrame.new(
                    uprightCFrame.Position,
                    Vector3.new(uprightCFrame.Position.X + lookDirection.X, uprightCFrame.Position.Y, uprightCFrame.Position.Z + lookDirection.Z)
                )

                humanoid.Sit = false
                rootPart.CFrame = uprightCFrame
                humanoid.AutoRotate = true

                if Revo.WalkOnAir.Enabled and Revo.WalkOnAir.Platform then
                    Revo.WalkOnAir.BaseY = rootPart.Position.Y + Revo.WalkOnAir.HeightOffset
                end
            end

        -- Teleport Key Function

            function Revo.Teleport:EnableGlobalListener()
                if self.GlobalConnection then return end

                self.GlobalConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if self.CapturingKey then return end

                    if self.Key and input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == self.Key then
                        self.Held = true
                    end

                    if input.UserInputType == Enum.UserInputType.MouseButton1 and self.Held then
                        self:TeleportToMouse()
                    end
                end)
            end

            Revo.Teleport:EnableGlobalListener()

            Revo.Services.UserInputService.InputEnded:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Revo.Teleport.Key then
                    Revo.Teleport.Held = false
                end
            end)

            function Revo.Teleport:CancelKeyCapture()
                self.Key = nil
                Revo.Buttons.TeleportKey.Text = "Teleport Key: None"

                Revo.Inputs.TeleportKey.Visible = false
                Revo.Buttons.TeleportKey.Visible = true

                self.CapturingKey = false

                if self.CaptureConnection then
                    self.CaptureConnection:Disconnect()
                    self.CaptureConnection = nil
                end

                self:EnableGlobalListener()
            end

            Revo.Buttons.TeleportKey.MouseButton1Click:Connect(function()
                Revo.Buttons.TeleportKey.Visible = false
                Revo.Inputs.TeleportKey.Visible = true
                Revo.Inputs.TeleportKey.Text = "Press Any Key"

                Revo.Teleport.CapturingKey = true

                if Revo.Teleport.GlobalConnection then
                    Revo.Teleport.GlobalConnection:Disconnect()
                    Revo.Teleport.GlobalConnection = nil
                end

                if Revo.Teleport.CaptureConnection then
                    Revo.Teleport.CaptureConnection:Disconnect()
                end

                Revo.Teleport.CaptureConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if not Revo.Teleport.CapturingKey then return end

                    if input.UserInputType == Enum.UserInputType.Keyboard then
                        if input.KeyCode == Enum.KeyCode.Backspace then
                            Revo.Teleport:CancelKeyCapture()
                        else
                            Revo.Teleport.Key = input.KeyCode

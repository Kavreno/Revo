            Revo.GUI.WalkOnAirLayout.Padding = UDim.new(0, 5)
            Revo.GUI.WalkOnAirLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
            Revo.GUI.WalkOnAirLayout.VerticalAlignment = Enum.VerticalAlignment.Center
            Revo.GUI.WalkOnAirLayout.Parent = Revo.GUI.WalkOnAirFrame
            Revo.GUI.WalkOnAirLayout.Padding = UDim.new(0, 2) 

            Revo.Buttons.WalkOnAir = Instance.new("TextButton")

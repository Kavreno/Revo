            Revo.Buttons.WalkOnAirDown.Visible = false

        -- Walk On Air Button Function
                    
            Revo.Buttons.WalkOnAir.MouseButton1Click:Connect(function()
                Revo.WalkOnAir.Enabled = true

                local root = Revo.Player.Character and Revo.Player.Character:FindFirstChild("HumanoidRootPart")
                if root then
                    Revo.WalkOnAir.BaseY = root.Position.Y + Revo.WalkOnAir.HeightOffset
                end

                Revo.Buttons.WalkOnAir.Visible = false
                Revo.Buttons.WalkOnAirOff.Visible = true
                Revo.Buttons.WalkOnAirUp.Visible = true
                Revo.Buttons.WalkOnAirDown.Visible = true

                if not Revo.WalkOnAir.Platform then
                    Revo.WalkOnAir.Platform = Revo:CreateWalkOnAirPlatform()
                end
            end)

            Revo.Buttons.WalkOnAirOff.MouseButton1Click:Connect(function()
                Revo.WalkOnAir.Enabled = false

                if Revo.WalkOnAir.Platform then
                    Revo.WalkOnAir.Platform:Destroy()
                    Revo.WalkOnAir.Platform = nil
                end

                Revo.Buttons.WalkOnAirOff.Visible = false
                Revo.Buttons.WalkOnAirUp.Visible = false
                Revo.Buttons.WalkOnAirDown.Visible = false
                Revo.Buttons.WalkOnAir.Visible = true
            end)

            Revo.Buttons.WalkOnAirUp.MouseButton1Down:Connect(function()
                Revo.WalkOnAir.Up = true
            end)

            Revo.Buttons.WalkOnAirUp.MouseLeave:Connect(function()
                Revo.WalkOnAir.Up = false
            end)

            Revo.Buttons.WalkOnAirUp.MouseButton1Up:Connect(function()
                Revo.WalkOnAir.Up = false
            end)

            Revo.Buttons.WalkOnAirDown.MouseButton1Down:Connect(function()
                Revo.WalkOnAir.Down = true
            end)

            Revo.Buttons.WalkOnAirDown.MouseLeave:Connect(function()
                Revo.WalkOnAir.Down = false
            end)

            Revo.Buttons.WalkOnAirDown.MouseButton1Up:Connect(function()
                Revo.WalkOnAir.Down = false
            end)

            function Revo.GUI:ResetWalkOnAirFlags()
                Revo.WalkOnAir.Up = false
                Revo.WalkOnAir.Down = false
            end

    -- Walk On Air Function

        function Revo:CreateWalkOnAirPlatform()
            local part = Instance.new("Part")
            part.Name = "WalkOnAirPlatform"
            part.Size = Vector3.new(6, 1, 6)
            part.Anchored = true
            part.CanCollide = true
            part.Material = Enum.Material.SmoothPlastic
            part.Color = Color3.fromRGB(170, 170, 255)
            part.Transparency = 1
            part.Parent = workspace
            return part
        end

        Revo.Services.RunService.RenderStepped:Connect(function()
            if not Revo.WalkOnAir.Enabled then return end

            local character = Revo.Player.Character
            local root = character and character:FindFirstChild("HumanoidRootPart")
            local platform = Revo.WalkOnAir.Platform

            if not root or not platform or not Revo.WalkOnAir.BaseY then return end

            if Revo.WalkOnAir.Up then
                Revo.WalkOnAir.BaseY = Revo.WalkOnAir.BaseY + 0.2
            end
            if Revo.WalkOnAir.Down then
                Revo.WalkOnAir.BaseY = Revo.WalkOnAir.BaseY - 0.2
            end

            platform.CFrame = CFrame.new(
                root.Position.X,
                Revo.WalkOnAir.BaseY,
                root.Position.Z
            )
        end)

    -- Invisibility

        -- Invisibility Button

            Revo.Buttons.InvisibilityKey = Instance.new("TextButton")
            Revo.Buttons.InvisibilityKey.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.InvisibilityKey.Position = UDim2.new(0, 5, 0, 145)
            Revo.Buttons.InvisibilityKey.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.InvisibilityKey.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.InvisibilityKey.Font = Enum.Font.Gotham
            Revo.Buttons.InvisibilityKey.TextSize = 16
            Revo.Buttons.InvisibilityKey.Text = "Invisibility Key: "..(Revo.Invisibility.Key and Revo.Invisibility.Key.Name or "None")
            Revo.Buttons.InvisibilityKey.Parent = Revo.GUI.CharacterSection
            Revo.GUI:AddCorner(Revo.Buttons.InvisibilityKey)

            Revo.Inputs.InvisibilityKey = Instance.new("TextBox")
            Revo.Inputs.InvisibilityKey.Size = Revo.Buttons.InvisibilityKey.Size
            Revo.Inputs.InvisibilityKey.Position = Revo.Buttons.InvisibilityKey.Position
            Revo.Inputs.InvisibilityKey.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Inputs.InvisibilityKey.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Inputs.InvisibilityKey.Font = Enum.Font.Gotham
            Revo.Inputs.InvisibilityKey.TextSize = 16
            Revo.Inputs.InvisibilityKey.PlaceholderText = "Press Any Key"
            Revo.Inputs.InvisibilityKey.Visible = false
            Revo.Inputs.InvisibilityKey.ClearTextOnFocus = true
            Revo.Inputs.InvisibilityKey.Text = ""
            Revo.Inputs.InvisibilityKey.Parent = Revo.GUI.CharacterSection
            Revo.GUI:AddCorner(Revo.Inputs.InvisibilityKey)

        -- Invisibility Key Function

            function Revo.Invisibility:EnableGlobalListener()
                if self.GlobalConnection then return end
                self.GlobalConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if self.CapturingKey then return end
                    if self.Key and input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == self.Key then
                        Revo:ToggleInvisibility()
                    end
                end)
            end
            Revo.Invisibility:EnableGlobalListener()

            function Revo.Invisibility:CancelKeyCapture()
                self.Key = nil
                Revo.Buttons.InvisibilityKey.Text = "Invisibility Key: None"
                self.CapturingKey = false
                if self.CaptureConnection then
                    self.CaptureConnection:Disconnect()
                    self.CaptureConnection = nil
                end
                self:EnableGlobalListener()
            end

            Revo.Buttons.InvisibilityKey.MouseButton1Click:Connect(function()
                Revo.Buttons.InvisibilityKey.Text = "Press Any Key"
                Revo.Invisibility.CapturingKey = true

                if Revo.Invisibility.GlobalConnection then
                    Revo.Invisibility.GlobalConnection:Disconnect()
                    Revo.Invisibility.GlobalConnection = nil
                end
                if Revo.Invisibility.CaptureConnection then
                    Revo.Invisibility.CaptureConnection:Disconnect()
                end

                Revo.Invisibility.CaptureConnection = Revo.Services.UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if not Revo.Invisibility.CapturingKey then return end

                    if input.UserInputType == Enum.UserInputType.Keyboard then
                        if input.KeyCode == Enum.KeyCode.Backspace then
                            Revo.Invisibility:CancelKeyCapture()
                        else
                            Revo.Invisibility.Key = input.KeyCode
                            Revo.Buttons.InvisibilityKey.Text = "Invisibility Key: "..Revo.Invisibility.Key.Name
                            Revo.Invisibility.CapturingKey = false
                            Revo.Invisibility.CaptureConnection:Disconnect()
                            Revo.Invisibility.CaptureConnection = nil
                            Revo.Invisibility:EnableGlobalListener()
                        end
                    else
                        Revo.Invisibility:CancelKeyCapture()
                    end
                end)
            end)

        -- Invisibility Fucntion

            Revo.Invisibility.RealCharacter = Revo.Player.Character or Revo.Player.CharacterAdded:Wait()
            Revo.Invisibility.RealCharacter.Archivable = true

            Revo.Invisibility.FakeCharacter = Revo.Invisibility.RealCharacter:Clone()

            Revo.Invisibility.Part = Instance.new("Part", workspace)
            Revo.Invisibility.Part.Anchored = true
            Revo.Invisibility.Part.Size = Vector3.new(200, 1, 200)
            Revo.Invisibility.Part.Position = Vector3.new(0, -500, 0)
            Revo.Invisibility.Part.CanCollide = true

            Revo.Invisibility.FakeCharacter.Parent = workspace
            Revo.Invisibility.FakeCharacter.HumanoidRootPart.CFrame = Revo.Invisibility.Part.CFrame * CFrame.new(0, 5, 0)

            for _, v in ipairs(Revo.Invisibility.RealCharacter:GetChildren()) do
                if v:IsA("LocalScript") then
                    local clone = v:Clone()
                    clone.Disabled = true
                    clone.Parent = Revo.Invisibility.FakeCharacter
                end
            end

            if Revo.Invisibility.Transparency then
                for _, v in ipairs(Revo.Invisibility.FakeCharacter:GetDescendants()) do
                    if v:IsA("BasePart") then
                        v.Transparency = 0.7
                    end
                end
            end

            function Revo:ResetInvisible()
                Revo.Invisibility.CanInvis = false

                Revo.Invisibility.RealCharacter:Destroy()
                Revo.Invisibility.RealCharacter = Revo.Player.Character
                Revo.Invisibility.CanInvis = true
                Revo.Invisibility.IsInvisible = false

                Revo.Invisibility.FakeCharacter:Destroy()

                workspace.CurrentCamera.CameraSubject = Revo.Invisibility.RealCharacter.Humanoid

                Revo.Invisibility.RealCharacter.Archivable = true
                Revo.Invisibility.FakeCharacter = Revo.Invisibility.RealCharacter:Clone()

                Revo.Invisibility.Part:Destroy()
                Revo.Invisibility.Part = Instance.new("Part", workspace)
                Revo.Invisibility.Part.Anchored = true
                Revo.Invisibility.Part.Size = Vector3.new(200, 1, 200)
                Revo.Invisibility.Part.Position = Vector3.new(9999, 9999, 9999)
                Revo.Invisibility.Part.CanCollide = true

                Revo.Invisibility.FakeCharacter.Parent = workspace
                Revo.Invisibility.FakeCharacter.HumanoidRootPart.CFrame = Revo.Invisibility.Part.CFrame * CFrame.new(0, 5, 0)

                for _, v in ipairs(Revo.Invisibility.RealCharacter:GetChildren()) do
                    if v:IsA("LocalScript") then
                        local clone = v:Clone()
                        clone.Disabled = true
                        clone.Parent = Revo.Invisibility.FakeCharacter
                    end
                end

                if Revo.Invisibility.Transparency then
                    for _, v in ipairs(Revo.Invisibility.FakeCharacter:GetDescendants()) do
                        if v:IsA("BasePart") then
                            v.Transparency = 0.7
                        end
                    end
                end
            end

            Revo.Invisibility.RealCharacter.Humanoid.Died:Connect(function()
                Revo:ResetInvisible()
            end)
            Revo.Player.CharacterAppearanceLoaded:Connect(function()
                Revo:ResetInvisible()
            end)

            Revo.Invisibility.PseudoAnchor = Revo.Invisibility.FakeCharacter.HumanoidRootPart

            game:GetService("RunService").RenderStepped:Connect(function()
                if Revo.Invisibility.PseudoAnchor ~= nil then
                    Revo.Invisibility.PseudoAnchor.CFrame = Revo.Invisibility.Part.CFrame * CFrame.new(0, 5, 0)
                end
                if Revo.Invisibility.NoClip then
                    Revo.Invisibility.FakeCharacter.Humanoid:ChangeState(11)
                end
            end)

            function Revo:ToggleInvisibility()
                if Revo.Invisibility.IsInvisible == false then
                    local StoredCF = Revo.Invisibility.RealCharacter.HumanoidRootPart.CFrame
                    Revo.Invisibility.RealCharacter.HumanoidRootPart.CFrame = Revo.Invisibility.FakeCharacter.HumanoidRootPart.CFrame
                    Revo.Invisibility.FakeCharacter.HumanoidRootPart.CFrame = StoredCF

                    Revo.Invisibility.RealCharacter.Humanoid:UnequipTools()
                    Revo.Player.Character = Revo.Invisibility.FakeCharacter
                    workspace.CurrentCamera.CameraSubject = Revo.Invisibility.FakeCharacter.Humanoid

                    Revo.Invisibility.PseudoAnchor = Revo.Invisibility.RealCharacter.HumanoidRootPart

                    for _, v in ipairs(Revo.Invisibility.FakeCharacter:GetChildren()) do
                        if v:IsA("LocalScript") then
                            v.Disabled = false
                        end
                    end

                    Revo.Invisibility.IsInvisible = true
                    Revo.Invisibility.Enabled = true
                else
                    local StoredCF = Revo.Invisibility.FakeCharacter.HumanoidRootPart.CFrame
                    Revo.Invisibility.FakeCharacter.HumanoidRootPart.CFrame = Revo.Invisibility.RealCharacter.HumanoidRootPart.CFrame
                    Revo.Invisibility.RealCharacter.HumanoidRootPart.CFrame = StoredCF

                    Revo.Invisibility.FakeCharacter.Humanoid:UnequipTools()
                    Revo.Player.Character = Revo.Invisibility.RealCharacter
                    workspace.CurrentCamera.CameraSubject = Revo.Invisibility.RealCharacter.Humanoid

                    Revo.Invisibility.PseudoAnchor = Revo.Invisibility.FakeCharacter.HumanoidRootPart

                    for _, v in ipairs(Revo.Invisibility.FakeCharacter:GetChildren()) do
                        if v:IsA("LocalScript") then
                            v.Disabled = true
                        end
                    end

                    Revo.Invisibility.IsInvisible = false
                    Revo.Invisibility.Enabled = false
                end
            end

-- Target

    Revo.GUI.LeftLayout = Instance.new("UIListLayout")
    Revo.GUI.LeftLayout.FillDirection = Enum.FillDirection.Vertical
    Revo.GUI.LeftLayout.Padding = UDim.new(0, 5)
    Revo.GUI.LeftLayout.SortOrder = Enum.SortOrder.LayoutOrder
    Revo.GUI.LeftLayout.Parent = LeftColumn

    Revo.GUI.RightLayout = Instance.new("UIListLayout")
    Revo.GUI.RightLayout.FillDirection = Enum.FillDirection.Vertical
    Revo.GUI.RightLayout.Padding = UDim.new(0, 5)
    Revo.GUI.RightLayout.SortOrder = Enum.SortOrder.LayoutOrder
    Revo.GUI.RightLayout.Parent = RightColumn

    function Revo:UpdateCanvas()
        pcall(function()
            local scroll = Revo.GUI.AnimScrollFrame
            if not scroll then return end
            if not Revo.GUI.LeftLayout or not Revo.GUI.RightLayout then return end

            local leftHeight  = Revo.GUI.LeftLayout.AbsoluteContentSize.Y
            local rightHeight = Revo.GUI.RightLayout.AbsoluteContentSize.Y

            scroll.CanvasSize =
                UDim2.new(0, 0, 0, math.max(leftHeight, rightHeight) + 10)
        end)
    end

    Revo.GUI.AnimScrollFrame = Instance.new("ScrollingFrame")
    Revo.GUI.AnimScrollFrame.Size = UDim2.new(1, 20, 1, 0)
    Revo.GUI.AnimScrollFrame.Position = UDim2.new(0, 0, 0, 0)
    Revo.GUI.AnimScrollFrame.BackgroundTransparency = 1
    Revo.GUI.AnimScrollFrame.ScrollBarThickness = 0
    Revo.GUI.AnimScrollFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
    Revo.GUI.AnimScrollFrame.ScrollingDirection = Enum.ScrollingDirection.Y
    Revo.GUI.AnimScrollFrame.Parent = Revo.Pages.Target

    Revo.GUI.LeftLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        Revo:UpdateCanvas()
    end)

    Revo.GUI.RightLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        Revo:UpdateCanvas()
    end)

    task.defer(function()
        Revo:UpdateCanvas()
    end)

    Revo.GUI.TargetSection = Instance.new("ScrollingFrame")
    Revo.GUI.TargetSection.Name = "Target_Section"
    Revo.GUI.TargetSection.Size = UDim2.new(1, 20, 1, -40)

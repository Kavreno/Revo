        Revo.GUI.TargetLeftColumn.Position = UDim2.new(0, 0, 0, 120)
        Revo.GUI.TargetLeftColumn.BackgroundTransparency = 1
        Revo.GUI.TargetLeftColumn.Parent = Revo.GUI.TargetSection

        Revo.GUI.TargetRightColumn = Instance.new("Frame")
        Revo.GUI.TargetRightColumn.Size = UDim2.new(0.5, 0, 1, -120)
        Revo.GUI.TargetRightColumn.Position = UDim2.new(0.5, 0, 0, 120)
        Revo.GUI.TargetRightColumn.BackgroundTransparency = 1
        Revo.GUI.TargetRightColumn.Parent = Revo.GUI.TargetSection

        Revo.GUI.TargetLeftLayout = Instance.new("UIListLayout")
        Revo.GUI.TargetLeftLayout.FillDirection = Enum.FillDirection.Vertical
        Revo.GUI.TargetLeftLayout.Padding = UDim.new(0, 5)
        Revo.GUI.TargetLeftLayout.SortOrder = Enum.SortOrder.LayoutOrder
        Revo.GUI.TargetLeftLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
        Revo.GUI.TargetLeftLayout.Parent = Revo.GUI.TargetLeftColumn

        Revo.GUI.TargetRightLayout = Instance.new("UIListLayout")
        Revo.GUI.TargetRightLayout.FillDirection = Enum.FillDirection.Vertical
        Revo.GUI.TargetRightLayout.Padding = UDim.new(0, 5)
        Revo.GUI.TargetRightLayout.SortOrder = Enum.SortOrder.LayoutOrder
        Revo.GUI.TargetRightLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
        Revo.GUI.TargetRightLayout.Parent = Revo.GUI.TargetRightColumn

        Revo.GUI.TargetPadding = Instance.new("UIPadding")
        Revo.GUI.TargetPadding.PaddingTop = UDim.new(0, 3)
        Revo.GUI.TargetPadding.PaddingBottom = UDim.new(0, 5)
        Revo.GUI.TargetPadding.PaddingLeft = UDim.new(0, 0)
        Revo.GUI.TargetPadding.PaddingRight = UDim.new(0, 0)
        Revo.GUI.TargetPadding.Parent = Revo.GUI.TargetSection

        function Revo:UpdateTargetCanvas()
            pcall(function()
                local scroll = Revo.GUI.TargetSection
                local leftLayout = Revo.GUI.TargetLeftLayout
                local rightLayout = Revo.GUI.TargetRightLayout
                if not scroll or not leftLayout or not rightLayout then return end

                scroll.CanvasSize = UDim2.new(0, 0, 0, math.max(leftLayout.AbsoluteContentSize.Y, rightLayout.AbsoluteContentSize.Y) + 10)
            end)
        end

        Revo.GUI.TargetLeftLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            Revo:UpdateTargetCanvas()
        end)
        Revo.GUI.TargetRightLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            Revo:UpdateTargetCanvas()
        end)

        task.defer(function()
            Revo:UpdateTargetCanvas()
        end)

    -- Buttons And Image

        Revo.GUI.TargetImage = Instance.new("ImageLabel")
        Revo.GUI.TargetImage.Name = "TargetImage"
        Revo.GUI.TargetImage.BackgroundColor3 = Color3.fromRGB(28, 28, 28)
        Revo.GUI.TargetImage.BorderColor3 = Color3.fromRGB(100, 100, 100)
        Revo.GUI.TargetImage.Position = UDim2.new(0, 5, 0, 10)
        Revo.GUI.TargetImage.Size = UDim2.new(0, 100, 0, 100)
        Revo.GUI.TargetImage.Image = "rbxassetid://10818605405"
        Revo.GUI.TargetImage.Parent = Revo.GUI.TargetSection
        Revo.GUI:AddCorner(Revo.GUI.TargetImage)

        Revo.GUI.UserIDTargetLabel = Instance.new("TextLabel")
        Revo.GUI.UserIDTargetLabel.Name = "UserIDTargetLabel"
        Revo.GUI.UserIDTargetLabel.BackgroundTransparency = 1
        Revo.GUI.UserIDTargetLabel.Position = UDim2.new(0, 110, 0, 61)
        Revo.GUI.UserIDTargetLabel.Size = UDim2.new(0, 300, 0, 55)
        Revo.GUI.UserIDTargetLabel.Font = Enum.Font.Gotham

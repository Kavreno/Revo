    Revo.UIList.FillDirection = Enum.FillDirection.Horizontal
    Revo.UIList.Padding = UDim.new(0, 4)
    Revo.UIList.HorizontalAlignment = Enum.HorizontalAlignment.Center
    Revo.UIList.VerticalAlignment = Enum.VerticalAlignment.Center
    Revo.UIList.Parent = Revo.ButtonBar

    function Revo:CreateButton(name)
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(0, 80, 1, 0)
        btn.Text = name
        btn.BackgroundColor3 = Color3.fromRGB(50,50,50)
        btn.TextColor3 = Color3.fromRGB(255,255,255)
        btn.Font = Enum.Font.Gotham
        btn.TextSize = 14
        btn.Parent = self.ButtonBar
        self.GUI:AddCorner(btn)

        btn.MouseButton1Click:Connect(function()
            for _, page in pairs(self.Pages) do
                page.Visible = false
            end

            local target = self.Pages[name]
            if target then
                target.Visible = true
            end

            for _, b in ipairs(self.ButtonBar:GetChildren()) do
                if b:IsA("TextButton") then
                    b.Visible = true
                end
            end

        end)

        return btn
    end

    Revo.Buttons = {}
    Revo.Inputs = {}

    Revo.Buttons.Home = Revo:CreateButton("Home")
    Revo.Buttons.Character = Revo:CreateButton("Character")

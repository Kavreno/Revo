    -- Get Target
        
        local function UpdateTarget(player)
            if (player ~= nil) then
                TargetedPlayer = player.Name
                TargetName_Input.Text = player.Name
                UserIDTargetLabel.Text = ("UserID: "..player.UserId.."\nDisplay: "..player.DisplayName.."\nJoined: "..os.date("%d-%m-%Y", os.time()-player.AccountAge * 24 * 3600).." [Day/Month/Year]")
                TargetImage.Image = Players:GetUserThumbnailAsync(player.UserId,Enum.ThumbnailType.HeadShot,Enum.ThumbnailSize.Size420x420)
            else
                TargetName_Input.Text = "Target"
                UserIDTargetLabel.Text = "UserID: \nDisplay: \nJoined: "
                TargetImage.Image = "rbxassetid://10818605405"
                TargetedPlayer = nil
            end
        end

    -- Target Input

        local function FindPlayerByNameOrDisplay(name)
            name = string.lower(name)

            for _, p in ipairs(Players:GetPlayers()) do
                local username = string.lower(p.Name)
                local displayname = string.lower(p.DisplayName)

                if username:sub(1,#name) == name or displayname:sub(1,#name) == name then
                    return p
                end
            end

            return nil
        end

        TargetName_Input.FocusLost:Connect(function()
            local text = TargetName_Input.Text:gsub("@","")
            if text == "" then
                headsitting = false
                backpacking = false
                standing = false
                viewing = false
                local localHumanoid = plr.Character and plr.Character:FindFirstChildOfClass("Humanoid")
                if localHumanoid then
                    game.Workspace.CurrentCamera.CameraSubject = localHumanoid
                end
                UpdateTarget(nil)
                return
            end

            local player = FindPlayerByNameOrDisplay(text)
            if player then
                UpdateTarget(player)
            else
                headsitting = false
                backpacking = false
                focusing = false
                viewing = false
                local localHumanoid = plr.Character and plr.Character:FindFirstChildOfClass("Humanoid")
                if localHumanoid then
                    game.Workspace.CurrentCamera.CameraSubject = localHumanoid
                end
                UpdateTarget(nil)
            end
        end)

    -- Target Tool

        ClickTargetTool_Button.MouseButton1Click:Connect(function()
            local GetTargetTool = Instance.new("Tool")
            GetTargetTool.Name = "ClickTarget"
            GetTargetTool.RequiresHandle = false
            GetTargetTool.TextureId = "rbxassetid://2716591855"
            GetTargetTool.ToolTip = "Select Target"

            local function ActivateTool()
                local root = GetRoot(plr)
                local hit = mouse.Target
                local person = nil
                if hit and hit.Parent then
                    if hit.Parent:IsA("Model") then
                        person = game.Players:GetPlayerFromCharacter(hit.Parent)
                    elseif hit.Parent:IsA("Accessory") then
                        person = game.Players:GetPlayerFromCharacter(hit.Parent.Parent)
                    end
                    if person then
                        UpdateTarget(person)
                    end
                end
            end

            GetTargetTool.Activated:Connect(function()
                ActivateTool()
            end)
            GetTargetTool.Parent = plr.Backpack
        end)

    -- Velocity Asset

        local Velocity_Asset = Instance.new("BodyAngularVelocity")

        Velocity_Asset.AngularVelocity = Vector3.new(0,0,0)
        Velocity_Asset.MaxTorque = Vector3.new(50000,50000,50000)
        Velocity_Asset.P = 1250
        Velocity_Asset.Name = "BreakVelocity"

    -- Stop Animations

        local function StopAnim()
            plr.Character.Animate.Disabled = false
            local animtrack = plr.Character.Humanoid:GetPlayingAnimationTracks()
            for i, track in pairs (animtrack) do
                track:Stop()
            end
        end

    -- Play Animations

        local function PlayAnim(id,time,speed)
            pcall(function()
                plr.Character.Animate.Disabled = false
                local hum = plr.Character.Humanoid
                local animtrack = hum:GetPlayingAnimationTracks()
                for i,track in pairs(animtrack) do
                    track:Stop()
                end
                plr.Character.Animate.Disabled = true
                local Anim = Instance.new("Animation")
                Anim.AnimationId = "rbxassetid://"..id
                local loadanim = hum:LoadAnimation(Anim)
                loadanim:Play()
                loadanim.TimePosition = time
                loadanim:AdjustSpeed(speed)
                loadanim.Stopped:Connect(function()
                    plr.Character.Animate.Disabled = false
                    for i, track in pairs (animtrack) do
                        track:Stop()
                    end
                end)
            end)
        end

    -- View Target

        local viewingTarget = false

        ViewTarget_Button.MouseButton1Click:Connect(function()
            if not TargetedPlayer then return end

            viewingTarget = not viewingTarget

            if viewingTarget then
                while viewingTarget do
                    pcall(function()
                        game.Workspace.CurrentCamera.CameraSubject =
                            Players[TargetedPlayer].Character.Humanoid
                    end)
                    task.wait(0.5)
                end
            else
                game.Workspace.CurrentCamera.CameraSubject =
                    plr.Character.Humanoid
            end
        end)

    -- Focus Target

        local focusingTarget = false

        FocusTarget_Button.MouseButton1Click:Connect(function()
            if not TargetedPlayer then return end

            focusingTarget = not focusingTarget

            if focusingTarget then
                while focusingTarget do
                    pcall(function()
                        local target = Players[TargetedPlayer]
                        TeleportTO(0, 0, 0, target)
                        Push(target)
                    end)
                    task.wait(0.2)
                end
            end
        end)

    -- Teleport Target

    TeleportTarget_Button.MouseButton1Click:Connect(function()
        if TargetedPlayer then
            pcall(function()
                TeleportTO(0, 0, 0, Players[TargetedPlayer], "safe")
            end)
        end
    end)

    -- Headsit Target

        local headsittingTarget = false

        HeadsitTarget_Button.MouseButton1Click:Connect(function()
            if not TargetedPlayer then return end

            headsittingTarget = not headsittingTarget

            if headsittingTarget then
                while headsittingTarget do
                    pcall(function()
                        -- Ensure BreakVelocity exists
                        if not GetRoot(plr):FindFirstChild("BreakVelocity") then
                            local TempV = Velocity_Asset:Clone()
                            TempV.Parent = GetRoot(plr)
                        end

                        local targetHead = Players[TargetedPlayer].Character.Head
                        plr.Character.Humanoid.Sit = true
                        GetRoot(plr).CFrame = targetHead.CFrame * CFrame.new(0, 2, 0)
                        GetRoot(plr).Velocity = Vector3.new(0, 0, 0)
                    end)
                    task.wait()
                end

                -- Cleanup
                if GetRoot(plr):FindFirstChild("BreakVelocity") then
                    GetRoot(plr).BreakVelocity:Destroy()
                end
            end
        end)

    -- Backpack Target

        local backpackingTarget = false

        BackpackTarget_Button.MouseButton1Click:Connect(function()
            if not TargetedPlayer then return end

            backpackingTarget = not backpackingTarget

            if backpackingTarget then
                while backpackingTarget do
                    pcall(function()
                        if not GetRoot(plr):FindFirstChild("BreakVelocity") then
                            local TempV = Velocity_Asset:Clone()
                            TempV.Parent = GetRoot(plr)
                        end

                        local targetRoot = GetRoot(Players[TargetedPlayer])
                        if targetRoot then
                            plr.Character.Humanoid.Sit = true
                            GetRoot(plr).CFrame = targetRoot.CFrame * CFrame.new(0, 0, 1.2) * CFrame.Angles(0, -3, 0)
                            GetRoot(plr).Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end

                if GetRoot(plr):FindFirstChild("BreakVelocity") then
                    GetRoot(plr).BreakVelocity:Destroy()
                end
            end
        end)

    -- Stand Target

        local standingTarget = false

        StandTarget_Button.MouseButton1Click:Connect(function()
            if not TargetedPlayer then return end

            standingTarget = not standingTarget

            if standingTarget then
                PlayAnim(13823324057, 4, 0)

                while standingTarget do
                    pcall(function()
                        if not GetRoot(plr):FindFirstChild("BreakVelocity") then
                            local TempV = Velocity_Asset:Clone()
                            TempV.Parent = GetRoot(plr)
                        end

                        local targetRoot = GetRoot(Players[TargetedPlayer])
                        if targetRoot then
                            GetRoot(plr).CFrame = targetRoot.CFrame * CFrame.new(-3, 1, 0)
                            GetRoot(plr).Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end

                StopAnim()

                if GetRoot(plr):FindFirstChild("BreakVelocity") then
                    GetRoot(plr).BreakVelocity:Destroy()
                end
            end
        end)

    -- Doggy Target

        local doggingTarget = false

        DoggyTarget_Button.MouseButton1Click:Connect(function()
            if not TargetedPlayer then return end

            doggingTarget = not doggingTarget

            if doggingTarget then
                PlayAnim(13694096724, 3.4, 0)

                while doggingTarget do
                    pcall(function()
                        if not GetRoot(plr):FindFirstChild("BreakVelocity") then
                            local TempV = Velocity_Asset:Clone()
                            TempV.Parent = GetRoot(plr)
                        end

                        local targetRoot = Players[TargetedPlayer].Character:FindFirstChild("LowerTorso")
                        if targetRoot then
                            GetRoot(plr).CFrame = targetRoot.CFrame * CFrame.new(0, 0.23, 0)
                            GetRoot(plr).Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end

                StopAnim()

                if GetRoot(plr):FindFirstChild("BreakVelocity") then
                    GetRoot(plr).BreakVelocity:Destroy()
                end
            end
        end)

    -- Drag Target

        local draggingTarget = false

        DragTarget_Button.MouseButton1Click:Connect(function()
            if not TargetedPlayer then return end

            draggingTarget = not draggingTarget

            if draggingTarget then
                PlayAnim(10714360343, 0.5, 0)

                while draggingTarget do
                    pcall(function()
                        if not GetRoot(plr):FindFirstChild("BreakVelocity") then
                            local TempV = Velocity_Asset:Clone()
                            TempV.Parent = GetRoot(plr)
                        end

                        local targetRoot = Players[TargetedPlayer].Character:FindFirstChild("RightHand")
                        if targetRoot then
                            GetRoot(plr).CFrame = targetRoot.CFrame * CFrame.new(0, -2.5, 1) * CFrame.Angles(-2, -3, 0)
                            GetRoot(plr).Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end

                StopAnim()

                if GetRoot(plr):FindFirstChild("BreakVelocity") then
                    GetRoot(plr).BreakVelocity:Destroy()
                end
            end
        end)

    -- Bang Target

        local BangTarget = false

        BangTarget_Button.MouseButton1Click:Connect(function()
            if not TargetedPlayer then return end

            BangTarget = not BangTarget
            if BangTarget then
                PlayAnim(5918726674, 0, 1)

                while BangTarget do
                    pcall(function()
                        if not GetRoot(plr):FindFirstChild("BreakVelocity") then
                            local TempV = Velocity_Asset:Clone()
                            TempV.Parent = GetRoot(plr)
                        end

                        local targetRoot = GetRoot(Players[TargetedPlayer])
                        if targetRoot then
                            GetRoot(plr).CFrame = targetRoot.CFrame * CFrame.new(0, 0, 1.1)
                            GetRoot(plr).Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end

                StopAnim()

                if GetRoot(plr):FindFirstChild("BreakVelocity") then
                    GetRoot(plr).BreakVelocity:Destroy()
                end
            end
        end)

    -- Fling Target

    -- Push Target

    -- Player Removing

    Players.PlayerRemoving:Connect(function(player)
        pcall(function()
            if player.Name == TargetedPlayer then
                UpdateTarget(nil)

                viewingTarget = false
                focusingTarget = false
                headsittingTarget = false
                backpackingTarget = false
                standingTarget = false
                doggingTarget = false
                draggingTarget = false
                BangTarget = false
                flingingTarget = false

                Notify("Revo", "Targeted player left/rejoined.", 5)
            end
        end)
    end)

-- Animations

    local function StopAnim()
        plr.Character.Animate.Disabled = false
        local animtrack = plr.Character.Humanoid:GetPlayingAnimationTracks()
        for i, track in pairs (animtrack) do
            track:Stop()
        end
    end

    local BorderOutline = Instance.new("UIStroke")
    BorderOutline.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    BorderOutline.Color = Color3.fromRGB(50, 50, 50)
    BorderOutline.Thickness = 2
    BorderOutline.Parent = TargetBorder

    local AnimScrollFrame = Instance.new("ScrollingFrame")
    AnimScrollFrame.Size = UDim2.new(1, 0, 1, 0)
    AnimScrollFrame.Position = UDim2.new(0, 0, 0, 0)
    AnimScrollFrame.BackgroundTransparency = 1
    AnimScrollFrame.ScrollBarThickness = 0
    AnimScrollFrame.Parent = AnimationsPage

    local horizontalOffset = 5

    local LeftColumn = Instance.new("Frame")
    LeftColumn.Size = UDim2.new(0.5, -horizontalOffset, 1, 0)
    LeftColumn.Position = UDim2.new(0, horizontalOffset, 0, 0)
    LeftColumn.BackgroundTransparency = 1
    LeftColumn.Parent = AnimScrollFrame

    local LeftPadding = Instance.new("UIPadding")
    LeftPadding.PaddingTop = UDim.new(0, 5)
    LeftPadding.Parent = LeftColumn
    local RightColumn = Instance.new("Frame")
    RightColumn.Size = UDim2.new(0.5, -horizontalOffset, 1, 0)
    RightColumn.Position = UDim2.new(0.5, horizontalOffset, 0, 0)
    RightColumn.BackgroundTransparency = 1
    RightColumn.Parent = AnimScrollFrame

    local RightPadding = Instance.new("UIPadding")
    RightPadding.PaddingTop = UDim.new(0, 5)
    RightPadding.Parent = RightColumn

    local LeftLayout = Instance.new("UIListLayout")
    LeftLayout.Padding = UDim.new(0, 8)
    LeftLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    LeftLayout.Parent = LeftColumn

    local RightLayout = Instance.new("UIListLayout")
    RightLayout.Padding = UDim.new(0, 8)
    RightLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    RightLayout.Parent = RightColumn

    local function UpdateCanvasSize()
        AnimScrollFrame.CanvasSize = UDim2.new(
            0, 0,
            0, math.max(
                LeftLayout.AbsoluteContentSize.Y,
                RightLayout.AbsoluteContentSize.Y
            ) + 10
        )
    end

    LeftLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(UpdateCanvasSize)
    RightLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(UpdateCanvasSize)


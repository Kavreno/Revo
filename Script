    -- Drag Target

        Revo.GUI.TargetButtons.Drag.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Dragging = not Revo.Target.Dragging

            if not Revo.Target.Dragging then
                Revo:StopAnim()
                local root = Revo:GetRoot(Revo.Player)
                if root and root:FindFirstChild("BreakVelocity") then
                    root.BreakVelocity:Destroy()
                end
                return
            end

            Revo:PlayAnim(10714360343, 0.5, 0)

            task.spawn(function()
                while Revo.Target.Dragging do
                    pcall(function()
                        local root = Revo:GetRoot(Revo.Player)
                        if not root then return end

                        if not root:FindFirstChild("BreakVelocity") then
                            local tempV = Revo.VelocityAsset:Clone()
                            tempV.Parent = root
                        end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("RightHand") then
                            local targetRoot = targetPlayer.Character.RightHand
                            root.CFrame = targetRoot.CFrame * CFrame.new(0, -2.5, 1) * CFrame.Angles(-2, -3, 0)
                            root.Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end
            end)
        end)

    -- Bang Target

        Revo.GUI.TargetButtons.Bang.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Banging = not Revo.Target.Banging

            if not Revo.Target.Banging then
                Revo:StopAnim()
                local root = Revo:GetRoot(Revo.Player)
                if root and root:FindFirstChild("BreakVelocity") then
                    root.BreakVelocity:Destroy()
                end
                return
            end

            Revo:PlayAnim(5918726674, 0, 1)

            task.spawn(function()
                while Revo.Target.Banging do
                    pcall(function()
                        local root = Revo:GetRoot(Revo.Player)
                        if not root then return end

                        if not root:FindFirstChild("BreakVelocity") then
                            local tempV = Revo.VelocityAsset:Clone()
                            tempV.Parent = root
                        end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character then
                            local otherRoot = Revo:GetRoot(targetPlayer)
                            root.CFrame = otherRoot.CFrame * CFrame.new(0, 0, 1.1)
                            root.Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end
            end)
        end)

    -- Fling Target

        Revo.GUI.TargetButtons.Fling.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Flinging = not Revo.Target.Flinging

            local myRoot = Revo:GetRoot(Revo.Player)
            if not myRoot then return end

            if Revo.Target.Flinging then
                Revo.Target._FlingOldCFrame = myRoot.CFrame
            end

            if not Revo.Target.Flinging then
                if myRoot:FindFirstChild("BreakVelocity") then
                    myRoot.BreakVelocity:Destroy()
                end

                if Revo.Target._FlingOldCFrame then
                    myRoot.AssemblyLinearVelocity = Vector3.zero
                    myRoot.Velocity = Vector3.zero
                    myRoot.CFrame = Revo.Target._FlingOldCFrame
                end

                return
            end

            task.spawn(function()
                while Revo.Target.Flinging do
                    pcall(function()
                        local myRoot = Revo:GetRoot(Revo.Player)
                        if not myRoot then return end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if not targetPlayer then return end

                        local targetRoot = Revo:GetRoot(targetPlayer)
                        if not targetRoot then return end

                        myRoot.CFrame = targetRoot.CFrame

                    end)

                    task.wait()
                end
            end)
        end)

    -- Push Target

        Revo.GUI.TargetButtons.Push.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            local myRoot = Revo:GetRoot(Revo.Player)
            if not myRoot then return end

            local oldCFrame = myRoot.CFrame

            pcall(function()
                local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                if not targetPlayer then return end

                local targetRoot = Revo:GetRoot(targetPlayer)
                if not targetRoot then return end

                myRoot.CFrame = targetRoot.CFrame
            end)

            task.wait(0.417)

            if myRoot:FindFirstChild("BreakVelocity") then
                myRoot.BreakVelocity:Destroy()
            end

            myRoot.AssemblyLinearVelocity = Vector3.zero
            myRoot.Velocity = Vector3.zero
            myRoot.CFrame = oldCFrame
        end)

    -- Face Bang Target

        -- Speed And Distance Buttons

            Revo.Buttons.FaceBangSpeed = Instance.new("TextButton")
            Revo.Buttons.FaceBangSpeed.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.FaceBangSpeed.Position = UDim2.new(0, 215, 0, 330)
            Revo.Buttons.FaceBangSpeed.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.FaceBangSpeed.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.FaceBangSpeed.Font = Enum.Font.Gotham
            Revo.Buttons.FaceBangSpeed.TextSize = 16
            Revo.Buttons.FaceBangSpeed.Text = "Face Bang Speed: " .. (Revo.FaceBangSettings.Speed or 3)
            Revo.Buttons.FaceBangSpeed.Parent = Revo.GUI.TargetSection
            Revo.GUI:AddCorner(Revo.Buttons.FaceBangSpeed)

            Revo.Inputs.FaceBangSpeed = Instance.new("TextBox")
            Revo.Inputs.FaceBangSpeed.Size = Revo.Buttons.FaceBangSpeed.Size
            Revo.Inputs.FaceBangSpeed.Position = Revo.Buttons.FaceBangSpeed.Position
            Revo.Inputs.FaceBangSpeed.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Inputs.FaceBangSpeed.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Inputs.FaceBangSpeed.Font = Enum.Font.Gotham
            Revo.Inputs.FaceBangSpeed.TextSize = 16
            Revo.Inputs.FaceBangSpeed.PlaceholderText = "Enter Face Bang Speed"
            Revo.Inputs.FaceBangSpeed.Visible = false
            Revo.Inputs.FaceBangSpeed.ClearTextOnFocus = true
            Revo.Inputs.FaceBangSpeed.Text = ""
            Revo.Inputs.FaceBangSpeed.Parent = Revo.GUI.TargetSection
            Revo.GUI:AddCorner(Revo.Inputs.FaceBangSpeed)

            Revo.Buttons.FaceBangDistance = Instance.new("TextButton")
            Revo.Buttons.FaceBangDistance.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.FaceBangDistance.Position = UDim2.new(0, 5, 0, 330)
            Revo.Buttons.FaceBangDistance.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.FaceBangDistance.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.FaceBangDistance.Font = Enum.Font.Gotham
            Revo.Buttons.FaceBangDistance.TextSize = 16
            Revo.Buttons.FaceBangDistance.Text = "Face Bang Distance: " .. (Revo.FaceBangSettings.Distance or 0.5)
            Revo.Buttons.FaceBangDistance.Parent = Revo.GUI.TargetSection
            Revo.GUI:AddCorner(Revo.Buttons.FaceBangDistance)

            Revo.Inputs.FaceBangDistance = Instance.new("TextBox")
            Revo.Inputs.FaceBangDistance.Size = Revo.Buttons.FaceBangDistance.Size
            Revo.Inputs.FaceBangDistance.Position = Revo.Buttons.FaceBangDistance.Position
            Revo.Inputs.FaceBangDistance.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Inputs.FaceBangDistance.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Inputs.FaceBangDistance.Font = Enum.Font.Gotham
            Revo.Inputs.FaceBangDistance.TextSize = 16
            Revo.Inputs.FaceBangDistance.PlaceholderText = "Enter Face Bang Distance"
            Revo.Inputs.FaceBangDistance.Visible = false
            Revo.Inputs.FaceBangDistance.ClearTextOnFocus = true
            Revo.Inputs.FaceBangDistance.Text = ""
            Revo.Inputs.FaceBangDistance.Parent = Revo.GUI.TargetSection
            Revo.GUI:AddCorner(Revo.Inputs.FaceBangDistance)

            -- Speed And Distance Functions

                Revo.Buttons.FaceBangSpeed.MouseButton1Click:Connect(function()
                    Revo.Buttons.FaceBangSpeed.Visible = false
                    Revo.Inputs.FaceBangSpeed.Visible = true
                    Revo.Inputs.FaceBangSpeed:CaptureFocus()
                end)

                Revo.Inputs.FaceBangSpeed.FocusLost:Connect(function()
                    pcall(function()
                        local Speed = Revo.Inputs.FaceBangSpeed.Text:gsub("%D", "")
                        if Speed == "" then Speed = 3 end

                        Revo.FaceBangSettings.Speed = tonumber(Speed)
                        Revo.Buttons.FaceBangSpeed.Text = "Face Bang Speed: " .. Revo.FaceBangSettings.Speed
                    end)

                    Revo.Inputs.FaceBangSpeed.Text = ""
                    Revo.Inputs.FaceBangSpeed.Visible = false
                    Revo.Buttons.FaceBangSpeed.Visible = true
                end)

                Revo.Buttons.FaceBangDistance.MouseButton1Click:Connect(function()
                    Revo.Buttons.FaceBangDistance.Visible = false
                    Revo.Inputs.FaceBangDistance.Visible = true
                    Revo.Inputs.FaceBangDistance:CaptureFocus()
                end)

                Revo.Inputs.FaceBangDistance.FocusLost:Connect(function()
                    pcall(function()
                        local Distance = Revo.Inputs.FaceBangDistance.Text:gsub("[^%d%.]", "")
                        if Distance == "" then Distance = 0.5 end

                        Revo.FaceBangSettings.Distance = tonumber(Distance)
                        Revo.Buttons.FaceBangDistance.Text = "Face Bang Distance: " .. Revo.FaceBangSettings.Distance
                    end)

                    Revo.Inputs.FaceBangDistance.Text = ""
                    Revo.Inputs.FaceBangDistance.Visible = false
                    Revo.Buttons.FaceBangDistance.Visible = true
                end)

        -- Face Bang Function

            Revo.GUI.TargetButtons.FaceBang.MouseButton1Click:Connect(function()
                if not Revo.Target.Player then return end

                Revo.Target.FaceBanging = not Revo.Target.FaceBanging

                if not Revo.Target.FaceBanging then
                    local char = Revo.Player.Character
                    if not char then return end

                    local root = Revo:GetRoot(Revo.Player)
                    if not root then return end

                    if root:FindFirstChild("BreakVelocity") then
                        root.BreakVelocity:Destroy()
                    end

                    local humanoid = char:FindFirstChildOfClass("Humanoid")
                    if humanoid then
                        humanoid.AutoRotate = true
                        humanoid:ChangeState(Enum.HumanoidStateType.Running)
                    end

                    char.Animate.Disabled = false

                    return
                end

                task.spawn(function()
                    local t = 0
                    while Revo.Target.FaceBanging do
                        pcall(function()
                            local root = Revo:GetRoot(Revo.Player)
                            if not root then return end

                            Revo:StopAnim()

                            if not root:FindFirstChild("BreakVelocity") then
                                local tempV = Revo.VelocityAsset:Clone()
                                tempV.Parent = root
                            end

                            local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                            if targetPlayer 
                            and targetPlayer.Character 
                            and targetPlayer.Character:FindFirstChild("Head") then

                                local targetHead = targetPlayer.Character.Head

                                local startOffset = 0.7                    
                                local offsetZ = math.sin(t * Revo.FaceBangSettings.Speed) * Revo.FaceBangSettings.Distance
                                root.CFrame = targetHead.CFrame 
                                    * CFrame.new(0, 1, -Revo.FaceBangSettings.Distance - startOffset + offsetZ)
                                    * CFrame.Angles(0, math.rad(180), 0)

                                root.Velocity = Vector3.new(0, 0, 0)
                            end

                            t = t + task.wait()
                        end)
                    end
                end)
            end)

    -- Inspect Target

        Revo.GUI.TargetButtons.Inspect.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            local target = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
            if not target or not target.Character then return end

            local humanoid = target.Character:FindFirstChildOfClass("Humanoid")
            if not humanoid then return end

            local description = humanoid:GetAppliedDescription()

            Revo.Services.GuiService:InspectPlayerFromHumanoidDescription(
                description,
                target.Name
            )
        end)

    -- Player Removing

        function Revo:StopTargetActions()
            Revo.Target.Viewing      = false
            Revo.Target.Focusing     = false
            Revo.Target.Headsitting  = false
            Revo.Target.Backpacking  = false
            Revo.Target.Standing     = false
            Revo.Target.Dogging      = false
            Revo.Target.Dragging     = false
            Revo.Target.Banging      = false
            Revo.Target.Flinging     = false
            Revo.Target.FaceBanging  = false

            Revo:StopAnim()

            local root = Revo:GetRoot(Revo.Player)
            if root and root:FindFirstChild("BreakVelocity") then
                root.BreakVelocity:Destroy()
            end
            
            if humanoid then
                humanoid.Sit = false
                humanoid.AutoRotate = true
                humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)
                task.wait()
                humanoid:ChangeState(Enum.HumanoidStateType.Running)

                -- Restore NoSit settings if needed
                if self._NoSitBeforeActions then
                    self.NoSit.Enabled = true
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
                end
            end

            local camera = workspace.CurrentCamera
            if camera and Revo.Player.Character and Revo.Player.Character:FindFirstChild("Humanoid") then
                camera.CameraSubject = Revo.Player.Character.Humanoid
            end
        end

        Revo.Services.Players.PlayerRemoving:Connect(function(player)
            pcall(function()
                if Revo.Target.Player and player.Name == Revo.Target.Player then
                    Revo:UpdateTarget(nil)
                    Revo:StopTargetActions()
                    Revo:Notify("Revo", "Targeted Player Left/Rejoined.", 5)
                end
            end)
        end)

-- Animations

    Revo.AnimScrollFrame = Instance.new("ScrollingFrame")
    Revo.AnimScrollFrame.Size = UDim2.new(1, 0, 1, -40)
    Revo.AnimScrollFrame.Position = UDim2.new(0, 0, 0, 35)
    Revo.AnimScrollFrame.BackgroundTransparency = 1
    Revo.AnimScrollFrame.ScrollBarThickness = 0
    Revo.AnimScrollFrame.Parent = Revo.Pages.Animations

    Revo.AnimLeftColumn = Instance.new("Frame")
    Revo.AnimLeftColumn.Size = UDim2.new(0.5, 0, 1, 0)
    Revo.AnimLeftColumn.Position = UDim2.new(0, 0, 0, 0)
    Revo.AnimLeftColumn.BackgroundTransparency = 1
    Revo.AnimLeftColumn.Parent = Revo.AnimScrollFrame

    Instance.new("UIPadding", Revo.AnimLeftColumn).PaddingTop = UDim.new(0, 3)

    Revo.AnimLeftLayout = Instance.new("UIListLayout")
    Revo.AnimLeftLayout.Padding = UDim.new(0, 5)
    Revo.AnimLeftLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    Revo.AnimLeftLayout.Parent = Revo.AnimLeftColumn

    Revo.AnimRightColumn = Instance.new("Frame")
    Revo.AnimRightColumn.Size = UDim2.new(0.5, 0, 1, 0)
    Revo.AnimRightColumn.Position = UDim2.new(0.5, 0, 0, 0)
    Revo.AnimRightColumn.BackgroundTransparency = 1
    Revo.AnimRightColumn.Parent = Revo.AnimScrollFrame

    Instance.new("UIPadding", Revo.AnimRightColumn).PaddingTop = UDim.new(0, 3)

    Revo.AnimRightLayout = Instance.new("UIListLayout")
    Revo.AnimRightLayout.Padding = UDim.new(0, 5)
    Revo.AnimRightLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    Revo.AnimRightLayout.Parent = Revo.AnimRightColumn

    function Revo:UpdateAnimCanvas()
        Revo.AnimScrollFrame.CanvasSize = UDim2.new(
            0, 0,
            0, math.max(
                Revo.AnimLeftLayout.AbsoluteContentSize.Y,
                Revo.AnimRightLayout.AbsoluteContentSize.Y
            ) + 10
        )
    end

    Revo.AnimLeftLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        Revo:UpdateAnimCanvas()
    end)
    Revo.AnimRightLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        Revo:UpdateAnimCanvas()
    end)


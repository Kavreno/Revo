    Revo.GUI.TargetButtons = {}

    function Revo.GUI:CreateTargetButton(name, text, pos)
        local btn = Instance.new("TextButton")
        btn.Name = name
        btn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        btn.BackgroundTransparency = 0
        btn.BorderSizePixel = 0
        btn.Position = pos
        btn.Size = UDim2.new(0, 200, 0, 30)
        btn.Font = Enum.Font.Gotham
        btn.Text = text
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        btn.TextSize = 16
        btn.Parent = self.TargetSection
        self:AddCorner(btn)

        self.TargetButtons[name] = btn

        return btn
    end

    Revo.GUI.TargetButtons.Fling        = Revo.GUI:CreateTargetButton("Fling",        "Fling",                 UDim2.new(0, 5, 0, 120))
    Revo.GUI.TargetButtons.View         = Revo.GUI:CreateTargetButton("View",         "View",                  UDim2.new(0, 215, 0, 120))
    Revo.GUI.TargetButtons.Focus        = Revo.GUI:CreateTargetButton("Focus",        "Focus",                 UDim2.new(0, 5, 0, 155))
    Revo.GUI.TargetButtons.Bang         = Revo.GUI:CreateTargetButton("Bang",         "Bang",                  UDim2.new(0, 215, 0, 155))
    Revo.GUI.TargetButtons.Stand        = Revo.GUI:CreateTargetButton("Stand",        "Stand",                 UDim2.new(0, 5, 0, 190))
    Revo.GUI.TargetButtons.Headsit      = Revo.GUI:CreateTargetButton("Headsit",      "Headsit",               UDim2.new(0, 215, 0, 190))
    Revo.GUI.TargetButtons.Doggy        = Revo.GUI:CreateTargetButton("Doggy",        "Doggy",                 UDim2.new(0, 5, 0, 225))
    Revo.GUI.TargetButtons.Backpack     = Revo.GUI:CreateTargetButton("Backpack",     "Backpack",              UDim2.new(0, 215, 0, 225))
    Revo.GUI.TargetButtons.Drag         = Revo.GUI:CreateTargetButton("Drag",         "Drag",                  UDim2.new(0, 5, 0, 260))
    Revo.GUI.TargetButtons.Teleport     = Revo.GUI:CreateTargetButton("Teleport",     "Teleport",              UDim2.new(0, 215, 0, 260))
    Revo.GUI.TargetButtons.Push         = Revo.GUI:CreateTargetButton("Push",         "Push",                  UDim2.new(0, 5, 0, 295))
    Revo.GUI.TargetButtons.FaceBang     = Revo.GUI:CreateTargetButton("FaceBang",     "Face Bang",             UDim2.new(0, 215, 0, 295))
    Revo.GUI.TargetButtons.Inspect      = Revo.GUI:CreateTargetButton("Inspect",     "Inspect",                UDim2.new(0, 5, 0, 365))

    Revo.GUI.TargetNameInput = Instance.new("TextBox")
    Revo.GUI.TargetNameInput.Name = "TargetName_Input"
    Revo.GUI.TargetNameInput.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
    Revo.GUI.TargetNameInput.BackgroundTransparency = 0.5
    Revo.GUI.TargetNameInput.BorderColor3 = Color3.fromRGB(100, 100, 100)
    Revo.GUI.TargetNameInput.Position = UDim2.new(0, 110, 0, 10)
    Revo.GUI.TargetNameInput.Size = UDim2.new(0, 200, 0, 30)
    Revo.GUI.TargetNameInput.Font = Enum.Font.Gotham
    Revo.GUI.TargetNameInput.PlaceholderText = "Target"
    Revo.GUI.TargetNameInput.Text = ""
    Revo.GUI.TargetNameInput.TextColor3 = Color3.fromRGB(255, 255, 255)
    Revo.GUI.TargetNameInput.TextSize = 14
    Revo.GUI.TargetNameInput.ClearTextOnFocus = true
    Revo.GUI.TargetNameInput.Parent = Revo.GUI.TargetSection
    Revo.GUI:AddCorner(Revo.GUI.TargetNameInput)

    -- Update Target

        function Revo:UpdateTarget(player)
            if player and player ~= "" and player ~= self.Player then
                self.Target.Player = player.Name

                Revo.GUI.TargetNameInput.Text = player.Name
                Revo.GUI.UserIDTargetLabel.Text =
                    "UserID: "..player.UserId..
                    "\nDisplay: "..player.DisplayName..
                    "\nJoined: "..os.date("%d-%m-%Y", os.time() - player.AccountAge * 24 * 3600).." [Day/Month/Year]"

                Revo.GUI.TargetImage.Image = self.Services.Players:GetUserThumbnailAsync(
                    player.UserId,
                    Enum.ThumbnailType.HeadShot,
                    Enum.ThumbnailSize.Size420x420
                )

            else
                Revo.GUI.TargetNameInput.Text = "Target"
                Revo.GUI.UserIDTargetLabel.Text = "UserID: \nDisplay: \nJoined: "
                Revo.GUI.TargetImage.Image = "rbxassetid://10818605405"
                self.Target.Player = nil

            end
        end

    -- Get Player

        function Revo:GetPlayer(UserDisplay)
            if UserDisplay ~= "" then
                UserDisplay = UserDisplay:lower()
                for _, v in pairs(self.Services.Players:GetPlayers()) do
                    if v.Name:lower():match(UserDisplay) or v.DisplayName:lower():match(UserDisplay) then
                        return v
                    end
                end
            end
            return nil
        end

        Revo.GUI.TargetNameInput.FocusLost:Connect(function()
            local text = Revo.GUI.TargetNameInput.Text:gsub("@", "")

            if text == "" then
                Revo:UpdateTarget(nil)
                return
            end

            local player = Revo:GetPlayer(text)

            if player and player ~= Revo.Player then
                Revo:UpdateTarget(player)
                return
            end

            Revo:UpdateTarget(nil)
        end)

    -- Target Tool

        -- Target Tool Button

            Revo.GUI.ClickTargetToolButton = Instance.new("ImageButton")
            Revo.GUI.ClickTargetToolButton.Name = "ClickTargetToolButton"
            Revo.GUI.ClickTargetToolButton.BackgroundTransparency = 1
            Revo.GUI.ClickTargetToolButton.Position = UDim2.new(1, 5, 0, 0)
            Revo.GUI.ClickTargetToolButton.Size = UDim2.new(0, 30, 0, 30)
            Revo.GUI.ClickTargetToolButton.Image = "rbxassetid://2716591855"
            Revo.GUI.ClickTargetToolButton.Parent = Revo.GUI.TargetNameInput

        -- Target Tool Function

            Revo.GUI.ClickTargetToolButton.MouseButton1Click:Connect(function()
                local GetTargetTool = Instance.new("Tool")
                GetTargetTool.Name = "ClickTarget"
                GetTargetTool.RequiresHandle = false
                GetTargetTool.TextureId = "rbxassetid://2716591855"
                GetTargetTool.ToolTip = "Select Target"

                local function ActivateTool()
                    local root = Revo.Player.Character and Revo.Player.Character:FindFirstChild("HumanoidRootPart")
                    if not root then return end

                    local hit = Revo.Mouse.Target
                    local person = nil

                    if hit and hit.Parent then
                        if hit.Parent:IsA("Model") then
                            person = Revo.Services.Players:GetPlayerFromCharacter(hit.Parent)
                        elseif hit.Parent:IsA("Accessory") then
                            person = Revo.Services.Players:GetPlayerFromCharacter(hit.Parent.Parent)
                        end

                        if person then
                            Revo:UpdateTarget(person)
                        end
                    end
                end

                GetTargetTool.Activated:Connect(ActivateTool)
                GetTargetTool.Parent = Revo.Player.Backpack
            end)

    -- View Target

        Revo.GUI.TargetButtons.View.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end
            Revo.Target.Viewing = not Revo.Target.Viewing

            if Revo.Target.Viewing then
                task.spawn(function()
                    while Revo.Target.Viewing do
                        pcall(function()
                            local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                            if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
                                game.Workspace.CurrentCamera.CameraSubject = targetPlayer.Character.Humanoid
                            end
                        end)
                        task.wait(0.2)
                    end
                end)
            else
                if Revo.Player.Character and Revo.Player.Character:FindFirstChild("Humanoid") then
                    game.Workspace.CurrentCamera.CameraSubject = Revo.Player.Character.Humanoid
                end
            end
        end)

    -- Focus Target

        Revo.GUI.TargetButtons.Focus.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Focusing = not Revo.Target.Focusing

            if Revo.Target.Focusing then
                task.spawn(function()
                    while Revo.Target.Focusing do
                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            Revo:TeleportMeToPlayer(targetPlayer, "safe")
                        end
                        task.wait(0.2)
                    end
                end)
            end
        end)

    -- Teleport To Target

        Revo.GUI.TargetButtons.Teleport.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
            if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                Revo:TeleportMeToPlayer(targetPlayer, "safe")
            end
        end)

    -- Headsit Target

        Revo.GUI.TargetButtons.Headsit.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Headsitting = not Revo.Target.Headsitting

            local char = Revo.Player.Character
            if not char then return end

            local humanoid = char:FindFirstChildOfClass("Humanoid")
            local root = Revo:GetRoot(Revo.Player)
            if not humanoid or not root then return end

            if not Revo.Target.Headsitting then

                if root:FindFirstChild("BreakVelocity") then
                    root.BreakVelocity:Destroy()
                end

                humanoid.Sit = false
                humanoid.AutoRotate = true
                humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)

                task.wait()
                humanoid:ChangeState(Enum.HumanoidStateType.Running)

                if Revo._NoSitBeforeActions then
                    Revo.NoSit.Enabled = true
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
                end

                return
            end

            Revo._NoSitBeforeActions = Revo.NoSit.Enabled

            if Revo.NoSit.Enabled then
                Revo.NoSit.Enabled = false
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
            end

            task.spawn(function()
                while Revo.Target.Headsitting do
                    pcall(function()
                        if not root:FindFirstChild("BreakVelocity") then
                            local tempV = Revo.VelocityAsset:Clone()
                            tempV.Parent = root
                        end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
                            local targetHead = targetPlayer.Character.Head
                            humanoid.Sit = true
                            root.CFrame = targetHead.CFrame * CFrame.new(0, 2, 0)
                            root.Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end
            end)
        end)

    -- Backpack Target

        Revo.GUI.TargetButtons.Backpack.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Backpacking = not Revo.Target.Backpacking

            local char = Revo.Player.Character
            if not char then return end

            local humanoid = char:FindFirstChildOfClass("Humanoid")
            local root = Revo:GetRoot(Revo.Player)
            if not humanoid or not root then return end

            if not Revo.Target.Backpacking then

                if root:FindFirstChild("BreakVelocity") then
                    root.BreakVelocity:Destroy()
                end

                humanoid.Sit = false
                humanoid.AutoRotate = true
                humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)

                task.wait()
                humanoid:ChangeState(Enum.HumanoidStateType.Running)

                if Revo._NoSitBeforeSitActions then
                    Revo.NoSit.Enabled = true
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
                end

                return
            end

            Revo._NoSitBeforeSitActions = Revo.NoSit.Enabled

            if Revo.NoSit.Enabled then
                Revo.NoSit.Enabled = false
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
            end

            humanoid.Sit = true
            humanoid.AutoRotate = false

            task.spawn(function()
                while Revo.Target.Backpacking do
                    pcall(function()
                        local root = Revo:GetRoot(Revo.Player)
                        if not root then return end

                        if not root:FindFirstChild("BreakVelocity") then
                            local tempV = Revo.VelocityAsset:Clone()
                            tempV.Parent = root
                        end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            local targetRoot = targetPlayer.Character.HumanoidRootPart

                            root.CFrame =
                                targetRoot.CFrame
                                * CFrame.new(0, 0, 1.2)
                                * CFrame.Angles(0, -3, 0)

                            root.Velocity = Vector3.zero
                        end
                    end)

                    task.wait()
                end
            end)
        end)

    -- Stand Target

        Revo.GUI.TargetButtons.Stand.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Standing = not Revo.Target.Standing

            if not Revo.Target.Standing then
                Revo:StopAnim()
                local root = Revo:GetRoot(Revo.Player)
                if root and root:FindFirstChild("BreakVelocity") then
                    root.BreakVelocity:Destroy()
                end
                return
            end

            Revo:PlayAnim(13823324057, 4, 0)

            task.spawn(function()
                while Revo.Target.Standing do
                    pcall(function()
                        local root = Revo:GetRoot(Revo.Player)
                        if not root then return end

                        if not root:FindFirstChild("BreakVelocity") then
                            local tempV = Revo.VelocityAsset:Clone()
                            tempV.Parent = root
                        end

                        local targetPlayer = Revo.Services.Players:FindFirstChild(Revo.Target.Player)
                        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            local targetRoot = targetPlayer.Character.HumanoidRootPart
                            root.CFrame = targetRoot.CFrame * CFrame.new(-3, 1, 0)
                            root.Velocity = Vector3.new(0, 0, 0)
                        end
                    end)
                    task.wait()
                end
            end)
        end)
    
    -- Doggy Target

        Revo.GUI.TargetButtons.Doggy.MouseButton1Click:Connect(function()
            if not Revo.Target.Player then return end

            Revo.Target.Dogging = not Revo.Target.Dogging

            if not Revo.Target.Dogging then
                Revo:StopAnim()
                local root = Revo:GetRoot(Revo.Player)

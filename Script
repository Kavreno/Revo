                Revo.Inputs.FlySpeed = Instance.new("TextBox")
                Revo.Inputs.FlySpeed.Size = Revo.Buttons.FlySpeed.Size
                Revo.Inputs.FlySpeed.Position = Revo.Buttons.FlySpeed.Position
                Revo.Inputs.FlySpeed.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                Revo.Inputs.FlySpeed.TextColor3 = Color3.fromRGB(255, 255, 255)
                Revo.Inputs.FlySpeed.Font = Enum.Font.Gotham
                Revo.Inputs.FlySpeed.TextSize = 16
                Revo.Inputs.FlySpeed.PlaceholderText = "Enter Fly Speed"
                Revo.Inputs.FlySpeed.Visible = false
                Revo.Inputs.FlySpeed.ClearTextOnFocus = true
                Revo.Inputs.FlySpeed.Parent = Revo.GUI.CharacterRightColumn
                Revo.GUI:AddCorner(Revo.Inputs.FlySpeed)


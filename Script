                TripKey = nil
                TripKeyButton.Text = "Trip Key: None"

                TripKey_Input.Visible = false
                TripKeyButton.Visible = true

                capturingTripKey = false

                if tripCaptureConnection then
                    tripCaptureConnection:Disconnect()
                    tripCaptureConnection = nil
                end

                enableGlobalTripListener()
            end


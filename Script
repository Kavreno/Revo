                Revo.Inputs.JumpPower.Text = ""
                Revo.Inputs.JumpPower.Visible = false
                Revo.Buttons.JumpPower.Visible = true
            end)

    -- Fly

        -- Fly Function

            function Revo.Fly:Start()
                if self.ThreadRunning then return end
                self.ThreadRunning = true
                self.Enabled = true

                local player = Revo.Player
                local char = player.Character
                if not char then self.ThreadRunning = false return end

                local torso = char:FindFirstChild("UpperTorso") or char:FindFirstChild("Torso")
                local humanoid = char:FindFirstChildOfClass("Humanoid")
                if not torso then self.ThreadRunning = false return end

                local bg = Instance.new("BodyGyro")
                bg.P = 1e5
                bg.maxTorque = Vector3.new(400000, 400000, 400000)
                bg.CFrame = CFrame.new(torso.Position, torso.Position + workspace.CurrentCamera.CFrame.LookVector)
                bg.Parent = torso

                local bv = Instance.new("BodyVelocity")
                bv.velocity = Vector3.new(0, 0.1, 0)
                bv.maxForce = Vector3.new(400000, 400000, 400000)
                bv.Parent = torso

                if humanoid then
                    humanoid.PlatformStand = true
                end

                local speed = 0

                while self.Enabled and torso.Parent do
                    task.wait()

                    if humanoid then
                        for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
                            track:Stop()
                        end
                        humanoid.Jump = false
                    end

                    local ctrl = self.Ctrl
                    local lastctrl = self.LastCtrl

                    if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then
                        speed = math.clamp(speed + self.Speed * 0.1, 0, self.Speed)
                    else
                        speed = math.clamp(speed - self.Speed * 0.1, 0, self.Speed)
                    end

                    if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then
                        bv.velocity = ((workspace.CurrentCamera.CFrame.LookVector * (ctrl.f + ctrl.b)) +
                            ((workspace.CurrentCamera.CFrame * CFrame.new(ctrl.l + ctrl.r, (ctrl.f + ctrl.b) * 0.2, 0).p) - workspace.CurrentCamera.CFrame.p)) * speed
                        self.LastCtrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r}
                    elseif speed ~= 0 then
                        bv.velocity = ((workspace.CurrentCamera.CFrame.LookVector * (lastctrl.f + lastctrl.b)) +
                            ((workspace.CurrentCamera.CFrame * CFrame.new(lastctrl.l + lastctrl.r, (lastctrl.f + lastctrl.b) * 0.2, 0).p) - workspace.CurrentCamera.CFrame.p)) * speed
                    else
                        bv.velocity = Vector3.new(0, 0.1, 0)
                    end

                    bg.CFrame = CFrame.new(torso.Position, torso.Position + workspace.CurrentCamera.CFrame.LookVector)
                end

                bg:Destroy()
                bv:Destroy()
                if humanoid then
                    humanoid.PlatformStand = false
                end
                self.ThreadRunning = false
            end

            function Revo.Fly:Stop()
                self.Enabled = false
            end

            -- Connect WASD keys
            function Revo.Fly:ConnectKeys(mouse)
                self.KeyDownConnection = mouse.KeyDown:Connect(function(key)
                    key = key:lower()
                    if key == "w" then self.Ctrl.f = 1
                    elseif key == "s" then self.Ctrl.b = -1
                    elseif key == "a" then self.Ctrl.l = -1
                    elseif key == "d" then self.Ctrl.r = 1
                    end
                end)

                self.KeyUpConnection = mouse.KeyUp:Connect(function(key)
                    key = key:lower()
                    if key == "w" then self.Ctrl.f = 0
                    elseif key == "s" then self.Ctrl.b = 0
                    elseif key == "a" then self.Ctrl.l = 0
                    elseif key == "d" then self.Ctrl.r = 0
                    end
                end)
            end

            function Revo.Fly:DisconnectKeys()
                if self.KeyDownConnection then self.KeyDownConnection:Disconnect() end
                if self.KeyUpConnection then self.KeyUpConnection:Disconnect() end
            end


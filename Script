        Revo.GUI.MiscRightLayout.Parent = Revo.GUI.MiscRightColumn

        Revo.GUI.MiscPadding = Instance.new("UIPadding")
        Revo.GUI.MiscPadding.PaddingTop = UDim.new(0, 3)
        Revo.GUI.MiscPadding.PaddingBottom = UDim.new(0, 5)
        Revo.GUI.MiscPadding.PaddingLeft = UDim.new(0, 0)
        Revo.GUI.MiscPadding.PaddingRight = UDim.new(0, 0)
        Revo.GUI.MiscPadding.Parent = Revo.GUI.MiscSection

        function Revo:UpdateMiscCanvas()
            pcall(function()
                local scroll = Revo.GUI.MiscSection
                local leftLayout = Revo.GUI.MiscLeftLayout
                local rightLayout = Revo.GUI.MiscRightLayout
                if not scroll or not leftLayout or not rightLayout then return end

                scroll.CanvasSize = UDim2.new(0, 0, 0, math.max(leftLayout.AbsoluteContentSize.Y, rightLayout.AbsoluteContentSize.Y) + 10)
            end)
        end

        Revo.GUI.MiscLeftLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            Revo:UpdateMiscCanvas()
        end)
        Revo.GUI.MiscRightLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            Revo:UpdateMiscCanvas()
        end)

        task.defer(function()
            Revo:UpdateMiscCanvas()
        end)

    -- Rejoin

        -- Rejoin Button

            Revo.Buttons.Rejoin = Instance.new("TextButton")
            Revo.Buttons.Rejoin.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.Rejoin.Position = UDim2.new(0, 5, 0, 5)
            Revo.Buttons.Rejoin.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.Rejoin.TextColor3 = Color3.fromRGB(255, 255, 255)

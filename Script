-- Title Bar

    Revo.GUI.TitleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            Revo.GUI.Dragging = true
            Revo.GUI.DragStart = input.Position
            Revo.GUI.StartPos = Revo.GUI.MainFrame.Position

            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    Revo.GUI.Dragging = false
                end
            end)
        end
    end)

    Revo.GUI.TitleBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            if Revo.GUI.Dragging then
                local delta = input.Position - Revo.GUI.DragStart
                Revo.GUI.MainFrame.Position = UDim2.new(
                    Revo.GUI.StartPos.X.Scale,
                    Revo.GUI.StartPos.X.Offset + delta.X,
                    Revo.GUI.StartPos.Y.Scale,
                    Revo.GUI.StartPos.Y.Offset + delta.Y
                )
            end
        end
    end)


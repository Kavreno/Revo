        -- Walk On Air Button Function
                    
            Revo.Buttons.WalkOnAir.MouseButton1Click:Connect(function()
                Revo.WalkOnAir.Enabled = true

                local root = Revo.Player.Character and Revo.Player.Character:FindFirstChild("HumanoidRootPart")
                if root then
                    Revo.WalkOnAir.BaseY = root.Position.Y + Revo.WalkOnAir.HeightOffset
                end

                Revo.Buttons.WalkOnAir.Visible = false
                Revo.Buttons.WalkOnAirOff.Visible = true
                Revo.Buttons.WalkOnAirUp.Visible = true
                Revo.Buttons.WalkOnAirDown.Visible = true

                if not Revo.WalkOnAir.Platform then
                    Revo.WalkOnAir.Platform = Revo:CreateWalkOnAirPlatform()
                end
            end)

            Revo.Buttons.WalkOnAirOff.MouseButton1Click:Connect(function()
                Revo.WalkOnAir.Enabled = false

                if Revo.WalkOnAir.Platform then
                    Revo.WalkOnAir.Platform:Destroy()
                    Revo.WalkOnAir.Platform = nil
                end

                Revo.Buttons.WalkOnAirOff.Visible = false
                Revo.Buttons.WalkOnAirUp.Visible = false
                Revo.Buttons.WalkOnAirDown.Visible = false
                Revo.Buttons.WalkOnAir.Visible = true
            end)

            Revo.Buttons.WalkOnAirUp.MouseButton1Down:Connect(function()
                Revo.WalkOnAir.Up = true
            end)

            Revo.Buttons.WalkOnAirUp.MouseLeave:Connect(function()
                Revo.WalkOnAir.Up = false
            end)

            Revo.Buttons.WalkOnAirUp.MouseButton1Up:Connect(function()
                Revo.WalkOnAir.Up = false
            end)

            Revo.Buttons.WalkOnAirDown.MouseButton1Down:Connect(function()
                Revo.WalkOnAir.Down = true
            end)

            Revo.Buttons.WalkOnAirDown.MouseLeave:Connect(function()
                Revo.WalkOnAir.Down = false
            end)

            Revo.Buttons.WalkOnAirDown.MouseButton1Up:Connect(function()
                Revo.WalkOnAir.Down = false
            end)

            function Revo.GUI:ResetWalkOnAirFlags()
                Revo.WalkOnAir.Up = false
                Revo.WalkOnAir.Down = false
            end

        -- Walk On Air Function

            function Revo:CreateWalkOnAirPlatform()
                local part = Instance.new("Part")
                part.Name = "WalkOnAirPlatform"
                part.Size = Vector3.new(6, 1, 6)
                part.Anchored = true
                part.CanCollide = true
                part.Material = Enum.Material.SmoothPlastic
                part.Color = Color3.fromRGB(170, 170, 255)
                part.Transparency = 1
                part.Parent = workspace
                return part
            end

            Revo.Services.RunService.RenderStepped:Connect(function()
                if not Revo.WalkOnAir.Enabled then return end

                local character = Revo.Player.Character
                local root = character and character:FindFirstChild("HumanoidRootPart")
                local platform = Revo.WalkOnAir.Platform

                if not root or not platform or not Revo.WalkOnAir.BaseY then return end

                if Revo.WalkOnAir.Up then
                    Revo.WalkOnAir.BaseY = Revo.WalkOnAir.BaseY + 0.2
                end
                if Revo.WalkOnAir.Down then
                    Revo.WalkOnAir.BaseY = Revo.WalkOnAir.BaseY - 0.2
                end

                platform.CFrame = CFrame.new(
                    root.Position.X,
                    Revo.WalkOnAir.BaseY,
                    root.Position.Z
                )
            end)

    -- Invisibility

        -- Invisibility Button

            if Revo.Save.Data.InvisibilityKey then

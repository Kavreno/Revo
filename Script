        function Revo:UpdateCharacterCanvas()
            pcall(function()
                local scroll = Revo.GUI.CharacterSection
                local leftLayout = Revo.GUI.CharacterLeftLayout
                local rightLayout = Revo.GUI.CharacterRightLayout
                if not scroll or not leftLayout or not rightLayout then return end

                scroll.CanvasSize = UDim2.new(0, 0, 0, math.max(leftLayout.AbsoluteContentSize.Y, rightLayout.AbsoluteContentSize.Y) + 10)
            end)
        end

        Revo.GUI.CharacterLeftLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            Revo:UpdateCharacterCanvas()
        end)
        Revo.GUI.CharacterRightLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            Revo:UpdateCharacterCanvas()
        end)

        task.defer(function()
            Revo:UpdateCharacterCanvas()
        end)

    -- Walk Speed

        -- Walk Speed Button
     
            Revo.WalkSpeed = Revo.Save.Data.WalkSpeed or 16

            Revo.Buttons.WalkSpeed = Instance.new("TextButton")
            Revo.Buttons.WalkSpeed.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.WalkSpeed.Position = UDim2.new(0, 5, 0, 5)
            Revo.Buttons.WalkSpeed.BackgroundColor3 = Color3.fromRGB(50, 50, 50)

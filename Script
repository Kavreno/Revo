            if not AnimScrollFrame then return end
            if not LeftLayout or not RightLayout then return end

            local leftHeight = LeftLayout.AbsoluteContentSize.Y
            local rightHeight = RightLayout.AbsoluteContentSize.Y

            AnimScrollFrame.CanvasSize =
                UDim2.new(0, 0, 0, math.max(leftHeight, rightHeight) + 10)
        end


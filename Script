            RejoinButton.Size = UDim2.new(0, 200, 0, 30)
            RejoinButton.Position = UDim2.new(0, 5, 0, 5)
            RejoinButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            RejoinButton.TextColor3 = Color3.fromRGB(255, 255, 255)

            Revo.Buttons.WalkOnAir.Visible = true

            Revo.Buttons.WalkOnAirOff = Instance.new("TextButton")
            Revo.Buttons.WalkOnAirOff.Size = UDim2.new(0, 65, 0, 30)
            Revo.Buttons.WalkOnAirOff.Position = UDim2.new(0, 72.5, 0, 110)
            Revo.Buttons.WalkOnAirOff.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.WalkOnAirOff.TextColor3 = Color3.fromRGB(255, 255, 255)

                task.wait()
                player.Character.Humanoid:ChangeState(0)
                local bav = Instance.new("BodyAngularVelocity")
                bav.Parent = GetRoot(player)
                bav.Name = "Spin"
                bav.MaxTorque = Vector3.new(0,math.huge,0)
                bav.AngularVelocity = Vector3.new(0,150,0)
                task.wait(3)
                player.Character.Humanoid:ChangeState(15)
            end)

    -- GUI Key

        -- GUI Key Button

            local GUIKeyButton = Instance.new("TextButton")
            GUIKeyButton.Size = UDim2.new(0, 200, 0, 30)
            GUIKeyButton.Position = UDim2.new(0, 215, 0, 145)
            GUIKeyButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            GUIKeyButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            GUIKeyButton.Font = Enum.Font.Gotham
            GUIKeyButton.TextSize = 16
            GUIKeyButton.Text = "GUI Key: "..(GUIKey and GUIKey.Name or "None")
            GUIKeyButton.Parent = MiscPage
            AddCorner(GUIKeyButton)

            local GUIKey_Input = Instance.new("TextBox")
            GUIKey_Input.Size = GUIKeyButton.Size
            GUIKey_Input.Position = GUIKeyButton.Position
            GUIKey_Input.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            GUIKey_Input.TextColor3 = Color3.fromRGB(255, 255, 255)
            GUIKey_Input.Font = Enum.Font.Gotham
            GUIKey_Input.TextSize = 16
            GUIKey_Input.PlaceholderText = "Press Any Key"
            GUIKey_Input.Visible = false
            GUIKey_Input.ClearTextOnFocus = true
            GUIKey_Input.Text = ""
            GUIKey_Input.Parent = MiscPage
            AddCorner(GUIKey_Input)

        -- GUI Key Function

            local function enableGlobalGUIListener()
                if globalGUIConnection then return end

                globalGUIConnection = UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if capturingGUIKey then return end

                    if GUIKey
                    and input.UserInputType == Enum.UserInputType.Keyboard
                    and input.KeyCode == GUIKey then
                        ScreenGui.Enabled = not ScreenGui.Enabled
                    end
                end)
            end

            enableGlobalGUIListener()

            local function cancelGUICapture()
                GUIKey = nil
                GUIKeyButton.Text = "GUI Key: None"

                GUIKey_Input.Visible = false
                GUIKeyButton.Visible = true

                capturingGUIKey = false

                if guiCaptureConnection then
                    guiCaptureConnection:Disconnect()
                    guiCaptureConnection = nil
                end

                enableGlobalGUIListener()
            end

            GUIKeyButton.MouseButton1Click:Connect(function()
                GUIKeyButton.Visible = false
                GUIKey_Input.Visible = true
                GUIKey_Input.Text = "Press Any Key"

                capturingGUIKey = true

                if globalGUIConnection then
                    globalGUIConnection:Disconnect()
                    globalGUIConnection = nil
                end

                if guiCaptureConnection then
                    guiCaptureConnection:Disconnect()
                end

                guiCaptureConnection = UserInputService.InputBegan:Connect(function(input, gpe)
                    if gpe then return end
                    if not capturingGUIKey then return end

                    if input.UserInputType == Enum.UserInputType.Keyboard then
                        if input.KeyCode == Enum.KeyCode.Backspace then
                            cancelGUICapture()
                        else
                            GUIKey = input.KeyCode
                            GUIKeyButton.Text = "GUI Key: " .. GUIKey.Name
                            updateHomeText()

                            GUIKey_Input.Visible = false
                            GUIKeyButton.Visible = true
                            capturingGUIKey = false

                            guiCaptureConnection:Disconnect()
                            guiCaptureConnection = nil

                            enableGlobalGUIListener()
                        end

                    elseif input.UserInputType == Enum.UserInputType.MouseButton1
                        or input.UserInputType == Enum.UserInputType.MouseButton2
                        or input.UserInputType == Enum.UserInputType.MouseButton3 then
                        cancelGUICapture()
                        updateHomeText()
                    end
                end)
            end)

    -- Base Plate

        -- Valid Place IDs

            local validPlaceIds = {
            6884319169,
            292628125,
            }

        -- Base Plate Genertaion

            if table.find(validPlaceIds, game.PlaceId) then
                if workspace:FindFirstChild("MicUpBaseplateFolder") then return end

                local folder = Instance.new("Folder")
                folder.Name = "MicUpBaseplateFolder"
                folder.Parent = workspace

                local TILE_SIZE = 512
                local GRID = 16

                for x = -GRID, GRID do
                    for z = -GRID, GRID do
                        local part = Instance.new("Part")
                        part.Name = "MicUpTile"
                        part.Anchored = true
                        part.CanCollide = true
                        part.Locked = true
                        part.Size = Vector3.new(TILE_SIZE, 1, TILE_SIZE)
                        part.Position = Vector3.new(x * TILE_SIZE, -0.7, z * TILE_SIZE)
                        part.Material = Enum.Material.Grass
                        part.Color = Color3.fromRGB(85, 175, 100)
                        part.TopSurface = Enum.SurfaceType.Smooth
                        part.BottomSurface = Enum.SurfaceType.Smooth
                        part.Parent = folder
                    end
                end
            end


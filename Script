        Revo.Humanoid = Revo.Player.Character and Revo.Player.Character:FindFirstChildOfClass("Humanoid")

    -- Save

        Revo.Save = {
            FileName = "RevoSettings.json",
            Data = {},
            Enabled = false
        }

    -- Save And Load Function (Don't Ask Why Its Here)

        function Revo.Save:Load()
            if not isfile(self.FileName) then
                self.Data = {}
                return
            end

            local success, decoded = pcall(function()
                return Revo.Services.HttpService:JSONDecode(readfile(self.FileName))
            end)

            if success and typeof(decoded) == "table" then
                self.Data = decoded
            else
                self.Data = {}
            end
        end

        function Revo.Save:Write()
            if not self.Enabled then
                return
            end

            pcall(function()
                writefile(
                    self.FileName,
                    Revo.Services.HttpService:JSONEncode(self.Data)
                )
            end)
        end

        Revo.Save:Load()

        Revo.Save.Enabled = false
        Revo.Save.Data.SaveEnabled = false


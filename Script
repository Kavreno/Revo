                    Revo.Save.Enabled = not Revo.Save.Enabled
                    Revo.Save.Data.SaveEnabled = Revo.Save.Enabled

                    pcall(function()
                        writefile(
                            Revo.Save.FileName,
                            Revo.Services.HttpService:JSONEncode(Revo.Save.Data)
                        )
                    end)

                    Revo:Notify(
                        "Revo",
                        Revo.Save.Enabled and "Saving Enabled" or "Saving Disabled",
                        5
                    )

                end)

-- Functions

    -- Create Pages

        function Revo.GUI:CreatePage(name)
            local page = Instance.new("Frame")
            page.Size = UDim2.new(1, 0, 1, -42)
            page.Position = UDim2.new(0, 0, 0, 42)
            page.BackgroundTransparency = 1
            page.Parent = self.MainFrame
            page.Visible = false

            self.Pages = self.Pages or {}
            self.Pages[name] = page

            return page
        end

    -- Send Notifications

        function Revo:Notify(title, text, duration)
            Revo.Services.StarterGui:SetCore("SendNotification", {
                Title = title,
                Text = text,
                Duration = duration or 5
            })
        end

    -- Get Root

        function Revo:GetRoot(player)
            local character = player.Character
            if character then
                return character:FindFirstChild("HumanoidRootPart")
            end
            return nil
        end

    -- Get Ping

        function Revo:GetPing()
            return (Revo.Services.Stats.Network.ServerStatsItem["Data Ping"]:GetValue()) / 1000
        end

    -- Teleport To

        -- Teleport To Position

            function Revo:TeleportToPosition(player, position, method)
                local root = self:GetRoot(player)
                if not root then return end

                local function apply()
                    root.Velocity = Vector3.zero
                    root.AssemblyLinearVelocity = Vector3.zero
                    root.CFrame = CFrame.new(position)
                end

                if method == "safe" then
                    task.spawn(function()
                        for _ = 1, 25 do
                            task.wait()
                            apply()
                        end
                    end)
                else
                    apply()
                end
            end

        -- Teleport To Player

            function Revo:TeleportMeToPlayer(targetPlayer, method)
                if not targetPlayer then return end
                local targetRoot = self:GetRoot(targetPlayer)
                if not targetRoot then return end

                self:TeleportToPosition(self.Player, targetRoot.Position + Vector3.new(0, 2, 0), method)
            end

    -- Toggle Ragdoll

        function Revo:ToggleRagdoll(bool)
            local char = self.Player.Character
            if not char then return end

            pcall(function()
                char["Falling down"].Disabled = bool
                char["Swimming"].Disabled = bool
                char["StartRagdoll"].Disabled = bool
                char["Pushed"].Disabled = bool
                char["RagdollMe"].Disabled = bool
            end)
        end

    -- Velocity Asset

        Revo.VelocityAsset = Instance.new("BodyAngularVelocity")
        Revo.VelocityAsset.AngularVelocity = Vector3.new(0, 0, 0)
        Revo.VelocityAsset.MaxTorque = Vector3.new(50000, 50000, 50000)
        Revo.VelocityAsset.P = 1250
        Revo.VelocityAsset.Name = "BreakVelocity"

    -- Stop Animations

        function Revo:StopAnim()
            local char = self.Player.Character
            if not char then return end

            char.Animate.Disabled = false

            local animTracks = char.Humanoid:GetPlayingAnimationTracks()
            for _, track in pairs(animTracks) do
                track:Stop()
            end
        end

    -- Play Animations

        function Revo:PlayAnim(id, time, speed)
            local char = self.Player.Character
            if not char then return end
            pcall(function()
                char.Animate.Disabled = false
                local hum = char:FindFirstChildOfClass("Humanoid")
                if not hum then return end

                for _, track in pairs(hum:GetPlayingAnimationTracks()) do
                    track:Stop()
                end

                char.Animate.Disabled = true

                local anim = Instance.new("Animation")
                anim.AnimationId = "rbxassetid://"..id
                local loadAnim = hum:LoadAnimation(anim)
                loadAnim:Play()
                loadAnim.TimePosition = time
                loadAnim:AdjustSpeed(speed)
                loadAnim.Stopped:Connect(function()
                    char.Animate.Disabled = false
                    for _, track in pairs(hum:GetPlayingAnimationTracks()) do
                        track:Stop()
                    end
                end)
            end)
        end

    -- Get Y

        function Revo:GetVoidY()
            local lowestY = math.huge

            for _, part in ipairs(workspace:GetDescendants()) do
                if part:IsA("BasePart") and part.CanCollide then
                    if part.Position.Y < lowestY then
                        lowestY = part.Position.Y
                    end
                end
            end

            return lowestY - 475
        end

        Revo.VOID_Y = Revo:GetVoidY()


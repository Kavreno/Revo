        Revo.GUI.TargetButtons = {}

        function Revo.GUI:CreateTargetButton(name, text, pos)
            local btn = Instance.new("TextButton")
            btn.Name = name
            btn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            btn.BackgroundTransparency = 0
            btn.BorderSizePixel = 0
            btn.Position = pos
            btn.Size = UDim2.new(0, 200, 0, 30)
            btn.Font = Enum.Font.Gotham
            btn.Text = text
            btn.TextColor3 = Color3.fromRGB(255, 255, 255)
            btn.TextSize = 16
            btn.Parent = self.TargetSection
            self:AddCorner(btn)

            self.TargetButtons[name] = btn

            return btn
        end

        Revo.GUI.TargetButtons.Fling        = Revo.GUI:CreateTargetButton("Fling",        "Fling",                 UDim2.new(0, 5, 0, 120))
        Revo.GUI.TargetButtons.View         = Revo.GUI:CreateTargetButton("View",         "View",                  UDim2.new(0, 215, 0, 120))
        Revo.GUI.TargetButtons.Focus        = Revo.GUI:CreateTargetButton("Focus",        "Focus",                 UDim2.new(0, 5, 0, 155))
        Revo.GUI.TargetButtons.Bang         = Revo.GUI:CreateTargetButton("Bang",         "Bang",                  UDim2.new(0, 215, 0, 155))
        Revo.GUI.TargetButtons.Stand        = Revo.GUI:CreateTargetButton("Stand",        "Stand",                 UDim2.new(0, 5, 0, 190))
        Revo.GUI.TargetButtons.Headsit      = Revo.GUI:CreateTargetButton("Headsit",      "Headsit",               UDim2.new(0, 215, 0, 190))
        Revo.GUI.TargetButtons.Doggy        = Revo.GUI:CreateTargetButton("Doggy",        "Doggy",                 UDim2.new(0, 5, 0, 225))

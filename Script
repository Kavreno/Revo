    -- Rewind

        -- Rewind Key

            Revo.Buttons.RewindKey = Instance.new("TextButton")
            Revo.Buttons.RewindKey.Size = UDim2.new(0, 200, 0, 30)
            Revo.Buttons.RewindKey.Position = UDim2.new(0, 215, 0, 75)
            Revo.Buttons.RewindKey.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            Revo.Buttons.RewindKey.TextColor3 = Color3.fromRGB(255, 255, 255)
            Revo.Buttons.RewindKey.Font = Enum.Font.Gotham

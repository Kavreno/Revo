    Revo.UseLeftColumn = true

    function Revo:CreateAnimButton(column, name, animNumber)
        local Btn = Instance.new("TextButton")
        Btn.Size = UDim2.new(0, 200, 0, 30)
        Btn.Text = name
        Btn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        Btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        Btn.Font = Enum.Font.Gotham
        Btn.TextSize = 16
        Btn.Parent = column
        self.GUI:AddCorner(Btn)

        local anims = self.AnimationTable[animNumber]

        Btn.MouseButton1Click:Connect(function()
            local Animate = self.Player.Character.Animate
            Animate.Disabled = true
            Revo:StopAnim()
            Animate.idle.Animation1.AnimationId = anims[1]
            Animate.idle.Animation2.AnimationId = anims[2]
            Animate.walk.WalkAnim.AnimationId = anims[3]
            Animate.run.RunAnim.AnimationId = anims[4]
            Animate.jump.JumpAnim.AnimationId = anims[5]
            Animate.climb.ClimbAnim.AnimationId = anims[6]
            Animate.fall.FallAnim.AnimationId = anims[7]
            self.Player.Character.Humanoid:ChangeState(3)
            Animate.Disabled = false
        end)

        return Btn
    end

    function Revo:AddAnimButton(name, animNumber)
        local column = self.UseLeftColumn and self.AnimLeftColumn or self.AnimRightColumn
        self:CreateAnimButton(column, name, animNumber)
        self.UseLeftColumn = not self.UseLeftColumn
    end

    local anims = {
        "Bold", "Rthro", "Vampire", "Hero", "Zombie Classic", "Mage", "Ghost", "Elder",
        "Levitation", "Astronaut", "Ninja", "Werewolf", "Cartoon", "Pirate", "Sneaky",
        "Toy", "Knight", "Confident", "Popstar", "Princess", "Cowboy", "Patrol", "FE Zombie",
        "Catwalk", "Unboxed", "Popular", "Community", "Aura", "Old", "Sports", "Stylish",
        "Wicked", "Bubbly", "Robot", "NFL", "Boundaries", "Realistic"
    }

    for i, name in ipairs(anims) do
        Revo:AddAnimButton(name, i)
    end

